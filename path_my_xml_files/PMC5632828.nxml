ontrib contrib-type="author"><name><surname>Leroy</surname><given-names>Didier</given-names></name><address><email>leroyd@mmv.org</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Laurijssens</surname><given-names>Bart E.</given-names></name><address><email>bart.laurijssens@orange.fr</email></address><xref ref-type="aff" rid="Aff15">15</xref></contrib><contrib contrib-type="author"><name><surname>Biguenet</surname><given-names>Sophie</given-names></name><address><email>biguenets@mmv.org</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Kibuuka</surname><given-names>Afizi</given-names></name><address><email>afizikibuuka@gmail.com</email></address><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Tshefu</surname><given-names>Antoinette Kitoto</given-names></name><address><email>antotshe@yahoo.com</email></address><xref ref-type="aff" rid="Aff16">16</xref></contrib><contrib contrib-type="author"><name><surname>Smith</surname><given-names>Melnick</given-names></name><address><email>melnick.smith@quintilesims.com</email></address><xref ref-type="aff" rid="Aff17">17</xref></contrib><contrib contrib-type="author"><name><surname>Foster</surname><given-names>Chanelle</given-names></name><address><email>chanelle.foster@quintilesims.com</email></address><xref ref-type="aff" rid="Aff17">17</xref></contrib><contrib contrib-type="author"><name><surname>Leipoldt</surname><given-names>Illse</given-names></name><address><email>illse.leipoldt@quintilesims.com</email></address><xref ref-type="aff" rid="Aff17">17</xref></contrib><contrib contrib-type="author"><name><surname>Kremsner</surname><given-names>Peter G.</given-names></name><address><email>peter.kremsner@uni-tuebingen.de</email></address><xref ref-type="aff" rid="Aff5">5</xref><xref ref-type="aff" rid="Aff6">6</xref></contrib><contrib contrib-type="author"><