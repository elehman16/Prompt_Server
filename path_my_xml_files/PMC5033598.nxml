e-group><article-title>Decreased Brain Neurokinin-1 Receptor Availability in Chronic Tennis Elbow</article-title><alt-title alt-title-type="running-head">Substance P Receptors in Chronic Pain</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Linnman</surname><given-names>Clas</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Catana</surname><given-names>Ciprian</given-names></name><xref ref-type="aff" rid="aff002"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>Sv&#x000e4;rdsudd</surname><given-names>Kurt</given-names></name><xref ref-type="aff" rid="aff003"><sup>3</sup></xref></contrib><contrib contrib-type="author"><name><surname>Appel</surname><given-names>Lieuwe</given-names></name><xref ref-type="aff" rid="aff004"><sup>4</sup></xref></contrib><contrib contrib-type="author"><name><surname>Engler</surname><given-names>Henry</given-names></name><xref ref-type="aff" rid="aff004"><sup>4</sup></xref><xref ref-type="aff" rid="aff005"><sup>5</sup></xref></contrib><contrib contrib-type="author"><name><surname>L&#x000e5;ngstr&#x000f6;m</surname><given-names>Bengt</given-names></name><xref ref-type="aff" rid="aff004"><sup>4</sup></xref><xref ref-type="aff" rid="aff006"><sup>6</sup></xref><xref ref-type="aff" rid="aff007"><sup>7</sup></xref></contrib><contrib contrib-type="author"><name><surname>S&#x000f6;rensen</surname><given-names>Jens</given-names></name><xref ref-type="aff" rid="aff004"><sup>4</sup></xref></contrib><contrib contrib-type="author"><name><surname>Furmark</surname><given-names>Tomas</given-names></name><xref ref-type="aff" rid="aff008"><sup>8</sup></xref></contrib><contrib contrib-type="author"><name><surname>Fredrikson</surname><given-names>Mats</given-names></name><xref ref-type="aff" rid="aff008"><sup>8</sup></xref><xref ref-type="aff" rid="aff009"><sup>9</sup></xref></contrib><contrib contrib-type="author"><name