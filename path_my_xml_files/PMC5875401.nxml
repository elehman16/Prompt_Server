b-type="author"><name><surname>Min</surname><given-names>Kyeong Tae</given-names></name><xref ref-type="aff" rid="af1-tcrm-14-601">1</xref></contrib><contrib contrib-type="author"><name><surname>Woo</surname><given-names>Jae Hee</given-names></name><xref ref-type="aff" rid="af3-tcrm-14-601">3</xref></contrib><contrib contrib-type="author"><name><surname>Baik</surname><given-names>Hee Jung</given-names></name><xref ref-type="aff" rid="af3-tcrm-14-601">3</xref><xref ref-type="corresp" rid="c1-tcrm-14-601"/></contrib></contrib-group><aff id="af1-tcrm-14-601">
