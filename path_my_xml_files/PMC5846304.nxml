<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Pain Res</journal-id><journal-id journal-id-type="iso-abbrev">J Pain Res</journal-id><journal-id journal-id-type="publisher-id">Journal of Pain Research</journal-id><journal-title-group><journal-title>Journal of Pain Research</journal-title></journal-title-group><issn pub-type="epub">1178-7090</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29563830</article-id><article-id pub-id-type="pmc">5846304</article-id><article-id pub-id-type="doi">10.2147/JPR.S152550</article-id><article-id pub-id-type="publisher-id">jpr-11-505</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Different mechanisms of contralateral- or ipsilateral-acupuncture to modulate the brain activity in patients with unilateral chronic shoulder pain: a pilot fMRI study</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Zhang</surname><given-names>Shuai</given-names></name><xref ref-type="aff" rid="af1-jpr-11-505">1</xref><xref ref-type="author-notes" rid="fn1-jpr-11-505">*</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Wang</surname><given-names>Xu</given-names></name><xref ref-type="aff" rid="af2-jpr-11-505">2</xref><xref ref-type="author-notes" rid="fn1-jpr-11-505">*</xref></contrib><contrib contrib-type="author"><name><surname>Yan</surname><given-names>Chao-Qun</given-names></name><xref ref-type="aff" rid="af1-jpr-11-505">1</xref></contrib><contrib contrib-type="author"><name><surname>Hu</surname><given-names>Shang-Qing</given-names></name><xref ref-type="aff" rid="af3-jpr-11-505">3</xref></contrib><contrib contrib-type="author"><name><surname>Huo</surname><given-names>Jian-Wei</given-names></name><xref ref-type="aff" rid="af4-jpr-11-505">4</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Zhong-Yan</given-names></name><xref ref-type="aff" rid="af4-jpr-11-505">4</xref></contrib><contrib contrib-type="author"><name><surname>Zhou</surname><given-names>Ping</given-names></name><xref ref-type="aff" rid="af1-jpr-11-505">1</xref></contrib><contrib contrib-type="author"><name><surname>Liu</surname><given-names>Chun-Hong</given-names></name><xref ref-type="aff" rid="af1-jpr-11-505">1</xref></contrib><contrib contrib-type="author"><name><surname>Liu</surname><given-names>Cun-Zhi</given-names></name><xref ref-type="aff" rid="af3-jpr-11-505">3</xref><xref ref-type="corresp" rid="c1-jpr-11-505"/></contrib></contrib-group><aff id="af1-jpr-11-505">
