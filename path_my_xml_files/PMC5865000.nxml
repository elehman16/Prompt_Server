<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Res Protoc</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Res Protoc</journal-id><journal-id journal-id-type="publisher-id">ResProt</journal-id><journal-title-group><journal-title>JMIR Research Protocols</journal-title></journal-title-group><issn pub-type="epub">1929-0748</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29519778</article-id><article-id pub-id-type="pmc">5865000</article-id><article-id pub-id-type="publisher-id">v7i3e44</article-id><article-id pub-id-type="doi">10.2196/resprot.5912</article-id><article-categories><subj-group subj-group-type="heading"><subject>Protocol</subject></subj-group><subj-group subj-group-type="article-type"><subject>Protocol</subject></subj-group></article-categories><title-group><article-title>Outcome of Critically ill Patients Undergoing Mandatory Insulin Therapy Compared to Usual Care Insulin Therapy: Protocol for a Pilot Randomized Controlled Trial&#x000a0;</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Bilotta</surname><given-names>Federico</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>van der Voort</surname><given-names>Peter HJ</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes" equal-contrib="yes"><name><surname>Watkinson</surname><given-names>Peter J</given-names></name><degrees>MD, FRCP (UK), MRCP (UK), MBChB</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>Critical Care Trials Group</institution><institution>University of Oxford</institution><addr-line>Kadoorie Centre for Critical Care Research and Education</addr-line><addr-line>John Radcliffe Hospital</addr-line><addr-line>Oxford, OX3 9DU</addr-line><country>United Kingdom</country><phone>44 01865 741166 ext 4265</phone><fax>44 01865 741166</fax><email>peter.watkinson@ndcn.ox.ac.uk</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-1023-3927</contrib-id></contrib><contrib id="contrib2" contrib-type="author" equal-contrib="yes"><name><surname>Barber</surname><given-names>Vicki S</given-names></name><degrees>PhD, BSc (Hons)</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9631-3666</contrib-id></contrib><contrib id="contrib3" contrib-type="author" equal-contrib="yes"><name><surname>Young</surname><given-names>J Duncan</given-names></name><degrees>MD, FRCA</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6838-4835</contrib-id></contrib></contrib-group><aff id="aff1">
