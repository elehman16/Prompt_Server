<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="protocol"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27932335</article-id><article-id pub-id-type="pmc">5168661</article-id><article-id pub-id-type="publisher-id">bmjopen-2016-011028</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2016-011028</article-id><article-categories><subj-group subj-group-type="heading"><subject>Rheumatology</subject><subj-group><subject>Protocol</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1732</subject><subject>1683</subject></subj-group></article-categories><title-group><article-title>Multicentre randomised placebo-controlled trial of oral anticoagulation with apixaban in systemic sclerosis-related pulmonary arterial hypertension: the SPHInX study protocol</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Calderone</surname><given-names>Alicia</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Stevens</surname><given-names>Wendy</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Prior</surname><given-names>David</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af3">3</xref></contrib><cont