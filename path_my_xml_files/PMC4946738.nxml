><xref ref-type="aff" rid="jve10-aff-0001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Thammajaruk</surname><given-names>Narukjaporn</given-names></name><xref ref-type="aff" rid="jve10-aff-0001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Sophonphan</surname><given-names>Jiratchaya</given-names></name><xref ref-type="aff" rid="jve10-aff-0001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Sriheara</surname><given-names>Chulalak</given-names></name><xref ref-type="aff" rid="jve10-aff-0001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Burger</surname><given-names>David M</given-names></name><xref ref-type="aff" rid="jve10-aff-0005"><sup>5</sup></xref></contrib><contrib contrib-type="author"><name><surname>Puthanakit</surname><given-names>Thanyawee</given-names></name><xref ref-type="corresp" rid="an1">*</xref><xref ref-type="aff" rid="jve10-aff-0001"><sup>1</sup></xref><xref ref-type="aff" rid="jve10-aff-0002"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>Ananworanich</surname><given-names>Jintanat</given-names></name><xref ref-type="aff" rid="jve10-aff-0001"><sup>1</sup></xref><xref ref-type="aff" rid="jve10-aff-0006"><sup>6</sup></xref><xref ref-type="aff" rid="jve10-aff-0007"><sup>7</sup></xref><xref ref-type="aff" rid="jve10-aff-0008"/><author-comment><p id="jve10-para-0001">on behalf of the HIV-NAT 167 study team</p></author-comment></contrib><aff id="jve10-aff-0001"><label><sup>1</sup></label><institution>HIV Netherlands Australia Thailand Research Collaboration (HIV-NAT)</institution>, 
