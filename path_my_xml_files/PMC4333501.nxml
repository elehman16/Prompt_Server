<article article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Br J Cancer</journal-id><journal-id journal-id-type="iso-abbrev">Br. J. Cancer</journal-id><journal-title-group><journal-title>British Journal of Cancer</journal-title></journal-title-group><issn pub-type="ppub">0007-0920</issn><issn pub-type="epub">1532-1827</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">25633037</article-id><article-id pub-id-type="pmc">4333501</article-id><article-id pub-id-type="pii">bjc201511</article-id><article-id pub-id-type="doi">10.1038/bjc.2015.11</article-id><article-categories><subj-group subj-group-type="heading"><subject>Clinical Study</subject></subj-group></article-categories><title-group><article-title>Self-sampling to increase participation in cervical cancer screening: an RCT comparing home mailing, distribution in pharmacies, and recall letter</article-title><alt-title alt-title-type="running">Self-sampling RCT in Italy</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Giorgi Rossi</surname><given-names>P</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><contrib contrib-type="author"><name><surname>Fortunato</surname><given-names>C</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Barbarino</surname><given-names>P</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Boveri</surname><given-names>S</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Caroli</surname><given-names>S</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Del Mistro</surname><given-names>A</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Ferro</surname><given-names>A</given-names></name><xref ref-type="aff" rid="aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Giammaria</surname><given-names>C</given-names></name><xref ref-type="aff" rid="aff8">8</xref></contrib><contrib contrib-type="author"><name><surname>Manfredi</surname><given-names>M</given-names></name><xref ref-type="aff" rid="aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Moretto</surname><given-names>T</given-names></name><xref ref-type="aff" rid="aff10">10</xref></contrib><contrib contrib-type="author"><name><surname>Pasquini</surname><given-names>A</given-names></name><xref ref-type="aff" rid="aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Sideri</surname><given-names>M</given-names></name><xref ref-type="author-notes" rid="note1"><sup>&#x02720;</sup></xref><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Tufi</surname><given-names>M C</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Cogo</surname><given-names>C</given-names></name><xref ref-type="aff" rid="aff11">11</xref></contrib><contrib contrib-type="author"><name><surname>Altobelli</surname><given-names>E</given-names></name><xref ref-type="aff" rid="aff3">3</xref><xref ref-type="corresp" rid="caf2">*</xref></contrib><contrib contrib-type="author"><collab>the HPV Self-sampling Italian Working Group<contrib-group><contrib contrib-type="author"><name><surname>Moretto</surname><given-names>Tina</given-names></name></contrib><contrib contrib-type="author"><name><surname>Barro</surname><given-names>Carmen</given-names></name></contrib><contrib contrib-type="author"><name><surname>Cinquetti</surname><given-names>Sandro</given-names></name></contrib><contrib contrib-type="author"><name><surname>Bittesini</surname><given-names>Lucia</given-names></name></contrib><contrib contrib-type="author"><name><surname>Fantin</surname><given-names>Gianpiero</given-names></name></contrib><contrib contrib-type="author"><name><surname>Ferro</surname><given-names>Antonio</given-names></name></contrib><contrib contrib-type="author"><name><surname>Penon</surname><given-names>Gabriella</given-names></name></contrib><contrib contrib-type="author"><name><surname>Bertazzo</surname><given-names>Sandra</given-names></name></contrib><contrib contrib-type="author"><name><surname>Toniolo</surname><given-names>Laura</given-names></name></contrib><contrib contrib-type="author"><name><surname>Farrugio</surname><given-names>Angelo</given-names></name></contrib><contrib contrib-type="author"><name><surname>Cocco</surname><given-names>Patrizia</given-names></name></contrib><contrib contrib-type="author"><name><surname>Gerace</surname><given-names>Pierfrancesco</given-names></name></contrib><contrib contrib-type="author"><name><surname>Capuzzo</surname><given-names>Sonia</given-names></name></contrib><contrib contrib-type="author"><name><surname>Vitaliani</surname><given-names>Mariangela</given-names></name></contrib><contrib contrib-type="author"><name><surname>Fioravanti</surname><given-names>Milena</given-names></name></contrib><contrib contrib-type="author"><name><surname>Benvegn&#x000f9;</surname><given-names>Cristina</given-names></name></contrib><contrib contrib-type="author"><name><surname>Del Mistro</surname><given-names>Annarosa</given-names></name></contrib><contrib contrib-type="author"><name><surname>Frayle</surname><given-names>Helena</given-names></name></contrib><contrib contrib-type="author"><name><surname>Rizzi</surname><given-names>Martina</given-names></name></contrib><contrib contrib-type="author"><name><surname>Trevisan</surname><given-names>Rossana</given-names></name></contrib><contrib contrib-type="author"><name><surname>Cogo</surname><given-names>Carla</given-names></name></contrib><contrib contrib-type="author"><name><surname>Zorzi</surname><given-names>Manuel</given-names></name></contrib><contrib contrib-type="author"><name><surname>Fedato</surname><given-names>Chiara</given-names></name></contrib><contrib contrib-type="author"><name><surname>Fortunato</surname><given-names>Carmine</given-names></name></contrib><contrib contrib-type="author"><name><surname>Scarselli</surname><given-names>Giorgio</given-names></name></contrib><contrib contrib-type="author"><name><surname>Altobelli</surname><given-names>Emma</given-names></name></contrib><contrib contrib-type="author"><name><surname>Lattanzi</surname><given-names>Amedeo</given-names></name></contrib><contrib contrib-type="author"><name><surname>Caroli</surname><given-names>Stefania</given-names></name></contrib><contrib contrib-type="author"><name><surname>Rossi</surname><given-names>Paolo Giorgi</given-names></name></contrib><contrib contrib-type="author"><name><surname>La Terra</surname><given-names>Katiuscia</given-names></name></contrib><contrib contrib-type="author"><name><surname>Barbarino</surname><given-names>Paolo</given-names></name></contrib><contrib contrib-type="author"><name><surname>Tufi</surname><given-names>Maria Concetta</given-names></name></contrib><contrib contrib-type="author"><name><surname>Gimmaria</surname><given-names>Carla</given-names></name></contrib><contrib contrib-type="author"><name><surname>Manfredi</surname><given-names>Marilena</given-names></name></contrib><contrib contrib-type="author"><name><surname>Pasquini</surname><given-names>Adriana</given-names></name></contrib><contrib contrib-type="author"><name><surname>Sideri</surname><given-names>Mario</given-names></name><xref ref-type="author-notes" rid="note1"><sup>&#x02720;</sup></xref></contrib><contrib contrib-type="author"><name><surname>Boveri</surname><given-names>Sara</given-names></name></contrib></c