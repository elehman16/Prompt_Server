tion><institution>University of Newcastle</institution><addr-line>Academic Office Block</addr-line><addr-line>University Drive</addr-line><addr-line>Callaghan, 2308</addr-line><country>Australia</country><phone>61 0429152405</phone><email>richard.fletcher@newcastle.edu.au</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-0970-4541</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>May</surname><given-names>Chris</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9198-277X</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Attia</surname><given-names>John</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9800-1308</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Garfield</surname><given-names>Craig Franklin</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6512-6005</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Skinner</surname><given-names>Geoff</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-7690-3746</contrib-id></contrib></contrib-group><aff id="aff1">
