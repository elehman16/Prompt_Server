stitution><institution>VU University Medical Center</institution><addr-line>De Boelelaan 1118</addr-line><addr-line>Amsterdam, 1081 HZ</addr-line><country>Netherlands</country><phone>31 0204440183</phone><fax>31 204448529</fax><email>j.leijenaar@vumc.nl</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-1524-3801</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Groeneveld</surname><given-names>Geert Jan</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4655-6667</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>van der Flier</surname><given-names>Wiesje Maria</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-8766-6224</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Scheltens</surname><given-names>Philip</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-1046-6408</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Klaassen</surname><given-names>Erica Surya</given-names></name><degrees>MSc</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-0089-1846</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Weinstein</surname><given-names>Henry Chanoch</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3534-4618</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Biessels</surname><given-names>Geert Jan</given-names></name><degrees>MD, PhD