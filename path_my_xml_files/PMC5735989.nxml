<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Neuropsychiatr Dis Treat</journal-id><journal-id journal-id-type="iso-abbrev">Neuropsychiatr Dis Treat</journal-id><journal-id journal-id-type="publisher-id">Neuropsychiatric Disease and Treatment</journal-id><journal-title-group><journal-title>Neuropsychiatric Disease and Treatment</journal-title></journal-title-group><issn pub-type="ppub">1176-6328</issn><issn pub-type="epub">1178-2021</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29290685</article-id><article-id pub-id-type="pmc">5735989</article-id><article-id pub-id-type="doi">10.2147/NDT.S139577</article-id><article-id pub-id-type="publisher-id">ndt-13-3001</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Accurately identifying patients who are excellent candidates or unsuitable for a medication: a novel approach</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>South</surname><given-names>Charles</given-names></name><xref ref-type="aff" rid="af1-ndt-13-3001">1</xref><xref ref-type="aff" rid="af2-ndt-13-3001">2</xref><xref ref-type="aff" rid="af3-ndt-13-3001">3</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Rush</surname><given-names>A John</given-names></name><xref ref-type="aff" rid="af4-ndt-13-3001">4</xref><xref ref-type="author-notes" rid="fn1-ndt-13-3001">*</xref></contrib><contrib contrib-type="author"><name><surname>Carmody</surname><given-names>Thomas J</given-names></name><xref ref-type="aff" rid="af1-ndt-13-3001">1</xref><xref ref-type="aff" rid="af2-ndt-13-3001">2</xref><xref ref-type="aff" rid="af3-ndt-13-3001">3</xref></contrib><contrib contrib-type="author"><name><surname>Jha</surname><given-names>Manish K</given-names></name><xref ref-type="aff" rid="af1-ndt-13-3001">1</xref><xref ref-type="aff" rid="af2-ndt-13-3001">2</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Trivedi</surname><given-names>Madhukar H</given-names></name><xref ref-type="aff" rid="af1-ndt-13-3001">1</xref><xref ref-type="aff" rid="af2-ndt-13-3001">2</xref><xref ref-type="author-notes" rid="fn1-ndt-13-3001">*</xref><xref ref-type="corresp" rid="c1-ndt-13-3001"/></contrib></contrib-group><aff id="af1-ndt-13-3001">
