<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Anesthesiol</journal-id><journal-id journal-id-type="iso-abbrev">BMC Anesthesiol</journal-id><journal-title-group><journal-title>BMC Anesthesiology</journal-title></journal-title-group><issn pub-type="epub">1471-2253</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">26801905</article-id><article-id pub-id-type="pmc">4724087</article-id><article-id pub-id-type="publisher-id">174</article-id><article-id pub-id-type="doi">10.1186/s12871-016-0174-5</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title>Dexketoprofen/tramadol 25&#x000a0;mg/75&#x000a0;mg: randomised double-blind trial in moderate-to-severe acute pain after abdominal hysterectomy</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Moore</surname><given-names>R. A.</given-names></name><address><phone>+44 1865 225674</phone><email>andrew.moore@ndcn.ox.ac.uk</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>McQuay</surname><given-names>H. J.</given-names></name><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Tomaszewski</surname><given-names>J.</given-names></name><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Raba</surname><given-names>G.</given-names></name><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Tutunaru</surname><given-names>D.</given-names></name><xref ref-type="aff" rid="Aff5">5</xref></contrib>