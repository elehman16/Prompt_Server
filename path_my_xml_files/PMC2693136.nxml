<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Public Health</journal-id><journal-title>BMC Public Health</journal-title><issn pub-type="epub">1471-2458</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19470176</article-id><article-id pub-id-type="pmc">2693136</article-id><article-id pub-id-type="publisher-id">1471-2458-9-152</article-id><article-id pub-id-type="doi">10.1186/1471-2458-9-152</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Health enhancing strength training in nonagenarians (STRONG): rationale, design and methods</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Rexach</surname><given-names>Jos&#x000e9; A Serra</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jserra.hgugm@salud.madrid.org</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Ruiz</surname><given-names>Jonatan R</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>ruizj@ugr.es</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Bustamante-Ara</surname><given-names>Natalia</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>natalia_eba@yahoo.es</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Villar&#x000e1;n</surname><given-names>Margarita Hierro</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>nogales.medicospacifico@residencias-asistidas.es</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Gil</surname><given-names>Pedro Gonz&#x000e1;lez</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>nogales.medicospacifico@residencias-asistidas.es</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Sanz Ib&#x000e1;&#x000f1;ez</surname><given-names>Maria J</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>nogales.medicospacifico@residencias-asistidas.es</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Sanz</surname><given-names>Nekane Blanco</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>nogales.medicospacifico@residencias-asistidas.es</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Santamar&#x000ed;a</surname><given-names>Victor Ortega</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>nogales.medicospacifico@residencias-asistidas.es</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Sanz</surname><given-names>Natalia Guti&#x000e9;rrez</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>nogales.medicospacifico@residencias-asistidas.es</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Prada</surname><given-names>Ana B Mar&#x000ed;n</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>nogales.medicospacifico@residencias-asistidas.es</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Gallardo</surname><given-names>Cristian</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>criscat96@gmail.com</email></contrib><contrib id="A12" contrib-type="author"><name><surname>Romo</surname><given-names>Gabriel Rodr&#x000ed;guez</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>gabriel.rodriguez@upm.es</email></contrib><contrib id="A13" corresp="yes" contrib-type="author"><name><surname>Lucia</surname><given-names>Alejandro</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>alejandro.lucia@uem.es</email></contrib></contrib-group><aff id="I1"><label>1</label>Geriatric Department, Hospital General Universitario Gregorio Mara&#x000f1;&#x000f3;n, Madrid, Spain</aff><aff id="I2"><label>2</label>Department of Biosciences and Nutrition at NOVUM, Unit for Preventive Nutrition, Karolinska Institutet, Huddinge, Sweden</aff><aff id="I3"><label>3</label>Department of Physiology, Universidad Europea de Madrid, Madrid, Spain</aff><aff id="I4"><label>4</label>Medical department, Residencia Los Nogales-Pac&#x000ed;fico, Madrid, Spain</aff><aff id="I5"><label>5</label>Sports department, Instituto Nacional de Educaci&#x000f3;n F&#x000ed;sica, Universidad Polit&#x000e9;cnica, Madrid, Spain</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>26</day><month>5</month><year>2009</year></pub-date><volume>9</volume><fpage>152</fpage><lpage>152</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1471-2458/9/152"/><history><date date-type="received"><day>26</day><month>3</month><year>2009</year></date><date date-type="accepted"><day>26</day><month>5</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Rexach et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Rexach et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
