<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Crit Care</journal-id><journal-title>Critical Care</journal-title><issn pub-type="ppub">1364-8535</issn><issn pub-type="epub">1466-609X</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">16696864</article-id><article-id pub-id-type="pmc">1550955</article-id><article-id pub-id-type="publisher-id">cc4913</article-id><article-id pub-id-type="doi">10.1186/cc4913</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Prospective, randomized trial comparing fluids and dobutamine optimization of oxygen delivery in high-risk surgical patients [ISRCTN42445141]</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Lobo</surname><given-names>Suzana M</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>suzanalobo@yahoo.com</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Lobo</surname><given-names>Francisco R</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Polachini</surname><given-names>Carlos A</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Patini</surname><given-names>Daniela S</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Yamamoto</surname><given-names>Adriana E</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A6" contrib-type="author"><name><surname>de Oliveira</surname><given-names>Neymar E</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Serrano</surname><given-names>Patricia</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Sanches</surname><given-names>Helder S</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Spegiorin</surname><given-names>Marco A</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Queiroz</surname><given-names>Marcio M</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Christiano</surname><given-names>Antonio C</given-names><suffix>Jr</suffix></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A12" contrib-type="author"><name><surname>Savieiro</surname><given-names>Elisangela F</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A13" contrib-type="author"><name><surname>Alvarez</surname><given-names>Paula A</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A14" contrib-type="author"><name><surname>Teixeira</surname><given-names>Silvia P</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib><contrib id="A15" contrib-type="author"><name><surname>Cunrath</surname><given-names>Geni S</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cepemi.hbase@famerp.br</email></contrib></contrib-group><aff id="I1"><label>1</label>Division of Critical Care Medicine, Departments of Internal Medicine, Anesthesiology and Surgery, Medical School &#x02013; FUNFARME and Hospital de Base, S&#x000e3;o Jos&#x000e9; do Rio Preto, S&#x000e3;o Paulo, Brazil</aff><pub-date pub-type="ppub"><year>2006</year></pub-date><pub-date pub-type="epub"><day>12</day><month>5</month><year>2006</year></pub-date><volume>10</volume><issue>3</issue><fpage>R72</fpage><lpage>R72</lpage><ext-link ext-link-type="uri" xlink:href="http://ccforum.com/content/10/3/R72"/><history><date date-type="received"><day>16</day><month>3</month><year>2006</year></date><date date-type="accepted"><day>6</day><month>4</month><year>2006</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2006 Lobo et al.; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2006</copyright-year><copyright-holder>Lobo et al.; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an open access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
