ity</institution><addr-line>Bruce Highway</addr-line><addr-line>Rockhampton, 4700</addr-line><country>Australia</country><phone>61 749232183</phone><email>c.vandelanotte@cqu.edu.au</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4445-8094</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Kolt</surname><given-names>Gregory S</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9223-3324</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Caperchione</surname><given-names>Cristina M</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2290-5946</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Savage</surname><given-names>Trevor N</given-names></name><degrees>MA</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6608-8664</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Rosenkranz</surname><given-names>Richard R</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff5">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8242-3546</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Maeder</surname><given-names>Anthony J</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff6">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-7398-2581</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Van Itallie</surname><given-names>Anetta</given-names></name><degrees>MA</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6506-1996</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>Tague</surname><given-names>Rhys</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-1733-3827</contrib-id></contrib><contrib id="contrib9" contrib-type="author"><name><surname>Oldmeadow</surname><given-names>Christopher</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff7">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6104-1322</contrib-id></contrib><contrib id="contrib10" contrib-type="author"><name><surname>Mummery</surname><given-names>W Kerry</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff8">8</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6583-0804</contrib-id></contrib><contrib id="contrib11" contrib-type="author"><name><surname>Duncan</surname><given-names>Mitch J</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff7">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9166-6195</contrib-id></contrib></contrib-group><aff id="aff1">
