<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Eur J Transl Myol</journal-id><journal-id journal-id-type="iso-abbrev">Eur J Transl Myol</journal-id><journal-id journal-id-type="publisher-id">EJTM</journal-id><journal-title-group><journal-title>European Journal of Translational Myology</journal-title></journal-title-group><issn pub-type="ppub">2037-7452</issn><issn pub-type="epub">2037-7460</issn><publisher><publisher-name>PAGEPress Publications, Pavia, Italy</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28713534</article-id><article-id pub-id-type="pmc">5505086</article-id><article-id pub-id-type="doi">10.4081/ejtm.2017.6292</article-id><article-categories><subj-group subj-group-type="heading"><subject>Strenght, Conditioning &#x00026; Physical Exercise - Original Articles</subject></subj-group></article-categories><title-group><article-title>Low-Load High-Velocity Resistance Exercises Improve Strength and Functional Capacity in Diabetic Patients</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Celes</surname><given-names>Rodrigo</given-names></name><xref ref-type="aff" rid="aff001">1</xref><xref ref-type="aff" rid="aff002">2</xref><xref ref-type="corresp" rid="cor1"/></contrib><contrib contrib-type="author"><name><surname>Bottaro</surname><given-names>Martim</given-names></name><xref ref-type="aff" rid="aff002">2</xref><email>martim@unb.br</email></contrib><contrib contrib-type="author"><name><surname>Cadore</surname><given-names>Eduardo</given-names></name><xref ref-type="aff" rid="aff003">3</xref><email>edcadore@yahoo.com.br</email></contrib><contrib contrib-type="author"><name><surname>Dullius</surname><given-names>Jane</given-names></name><xref ref-type="aff" rid="aff002">2</xref><email>janedullius@gmail.com</email></contrib><contrib contrib-type="author"><name><surname>Schwartz</surname><given-names>Fabiano</given-names></name><xref ref-type="aff" rid="aff002">2</xref><email>fabiano.schwartz@camara.gov.br</email></contrib><contrib contrib-type="author"><name><surname>Luzine</surname><given-names>Filipe</given-names></name><xref ref-type="aff" rid="aff002">2</xref><email>filipe.luzine@hotmail.com</email></contrib></contrib-group><aff id="aff001"><label>(1)</label>
