<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Musculoskelet Disord</journal-id><journal-title>BMC Musculoskeletal Disorders</journal-title><issn pub-type="epub">1471-2474</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19630975</article-id><article-id pub-id-type="pmc">2724401</article-id><article-id pub-id-type="publisher-id">1471-2474-10-92</article-id><article-id pub-id-type="doi">10.1186/1471-2474-10-92</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Contributions of myofascial pain in diagnosis and treatment of shoulder pain. A randomized control trial</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Perez-Palomares</surname><given-names>Sara</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>sperezp@salud.aragon.es</email></contrib><contrib id="A2" corresp="yes" contrib-type="author"><name><surname>Oliv&#x000e1;n-Bl&#x000e1;zquez</surname><given-names>B&#x000e1;rbara</given-names></name><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><email>bolivan.iacs@aragon.es</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Arnal-Burr&#x000f3;</surname><given-names>Ana M&#x000aa;</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>ana_arnal@hotmail.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Mayoral-Del Moral</surname><given-names>Orlando</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>orlando.mayoral@uclm.es</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Gaspar-Calvo</surname><given-names>Elena</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>helengasp@hotmail.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>de-la-Torre-Beldarra&#x000ed;n</surname><given-names>M&#x000aa; Luisa</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>delatorre.marisa@gmail.com</email></contrib><contrib id="A7" contrib-type="author"><name><surname>L&#x000f3;pez-Lape&#x000f1;a</surname><given-names>Elena</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>elenalolap@yahoo.es</email></contrib><contrib id="A8" contrib-type="author"><name><surname>P&#x000e9;rez-Benito</surname><given-names>Marina</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>mperezben@salud.aragon.es</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Ara-Loriente</surname><given-names>Victoria</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>vara@salud.aragon.es</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Romo-Calvo</surname><given-names>Laura</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>lauraromo74@yahoo.es</email></contrib></contrib-group><aff id="I1"><label>1</label>Arag&#x000f3;n Health Service, Primary Care, Zaragoza, Spain</aff><aff id="I2"><label>2</label>Research Network on Preventive Activities and Health Promotion, (REDIAPP-G06-018) Nodo de Arag&#x000f3;n, Spain</aff><aff id="I3"><label>3</label>Arag&#x000f3;n Health Sciences Institute, Zaragoza, Spain</aff><aff id="I4"><label>4</label>Arag&#x000f3;n Health Service, Zaragoza, Spain</aff><aff id="I5"><label>5</label>Hospital Provincial de Toledo, Toledo, Spain</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>24</day><month>7</month><year>2009</year></pub-date><volume>10</volume><fpage>92</fpage><lpage>92</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1471-2474/10/92"/><history><date date-type="received"><day>7</day><month>4</month><year>2009</year></date><date date-type="accepted"><day>24</day><month>7</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Perez-Palomares et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Perez-Palomares et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
