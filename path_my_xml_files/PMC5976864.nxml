<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Res Protoc</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Res Protoc</journal-id><journal-id journal-id-type="publisher-id">ResProt</journal-id><journal-title-group><journal-title>JMIR Research Protocols</journal-title></journal-title-group><issn pub-type="epub">1929-0748</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29769170</article-id><article-id pub-id-type="pmc">5976864</article-id><article-id pub-id-type="publisher-id">v7i5e129</article-id><article-id pub-id-type="doi">10.2196/resprot.9252</article-id><article-categories><subj-group subj-group-type="heading"><subject>Protocol</subject></subj-group><subj-group subj-group-type="article-type"><subject>Protocol</subject></subj-group></article-categories><title-group><article-title>Design and Rationale for a Parent-Led Intervention to Increase Fruit and Vegetable Intake in Young Childhood Cancer Survivors (Reboot): Protocol for a Pilot Study</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Ferrini</surname><given-names>Krizia</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Joyce</surname><given-names>Brian T</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Lucas</surname><given-names>Alexander</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes" equal-contrib="yes"><name><surname>Touyz</surname><given-names>Lauren</given-names></name><degrees>MPsych (Clin), BSc (Psych)</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>Behavioural Sciences Unit, Kids Cancer Centre</institution><institution>School of Women's and Children's Health</institution><institution>University of New South Wales Sydney</institution><addr-line>Level 1 South, Sydney Children's Hospital, High Street, Randwick</addr-line><addr-line>Sydney, 2031</addr-line><country>Australia</country><phone>61 412331225</phone><email>l.touyz@student.unsw.edu.au</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9294-3070</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Cohen</surname><given-names>Jennifer</given-names></name><degrees>BScNutrition, M NutrDiet, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-7798-1113</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Wakefield</surname><given-names>Claire</given-names></name><degrees>B Psychology (Hons), MPH, PhD (Psych)</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7704-7067</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Grech</surname><given-names>Allison</given-names></name><degrees>B HlthSci, M NutrDiet</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4478-4372</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Garnett</surname><given-names>Sarah</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7552-0789</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Gohil</surname><given-names>Paayal</given-names></name><degrees>BSc (Food Science)</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-