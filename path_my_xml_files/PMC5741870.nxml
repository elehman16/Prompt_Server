trib-type="author"><name><surname>Wu</surname><given-names>Gang</given-names></name><address><email>xhzlwg@163.com</email></address><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Shi</surname><given-names>Chun-Mei</given-names></name><address><email>scmu@hotmail.com</email></address><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Ding</surname><given-names>Ke-Feng</given-names></name><address><email>dingkefeng@126.com</email></address><xref ref-type="aff" rid="Aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Lin</surname><given-names>Li-Zhu</given-names></name><address><email>lizhulin903@21cn.com</email></address><xref ref-type="aff" rid="Aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Jin-Wan</given-names></name><address><email>jwwang@csco.org.cn</email></address><xref ref-type="aff" rid="Aff8">8</xref></contrib><contrib contrib-type="author"><name><surname>Xiong</surname><given-names>Jian-Ping</given-names></name><address><email>jpxiongjx@gmail.com</email></address><xref ref-type="aff" rid="Aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Wu</surname><given-names>Chang-Ping</given-names></name><address><email>newwcp@163.com</email></address><xref ref-type="aff" rid="Aff10">10</xref></contrib><contrib contrib-type="author"><name><surname>Li</surname><given-names>Jin</given-names></name><address><email>fudanlijin@163.com</email></address><xref ref-type="aff" rid="Aff11">11</xref></contrib><contrib contrib-type="author"><name><surname>Liu</surname><given-names>Yun-Peng</given-names></name><address><email>cmuliuyunpeng@hotmail.com</email></address><xref ref-type="aff" rid="Aff12">12</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Dong</given-names></name><address><email>dongwang64@hotmail.com</email></address><xref ref-type="aff" rid="Aff13">13</xref></contrib><contrib contrib-type="author"><name><surname>Ba</surname><given-names>Yi</given-names></name><address><email>yiba99@yahoo.com</email></address><xref ref-type="aff" rid="Aff14">14</xref></contrib><contrib contrib-type="author"><name><surname>Feng</surname><given-names>Jue-Ping</given-names></name><address><email>fengjueping@163.com</email></address><xref ref-type="aff" rid="Aff15">15</xref></contrib><contrib contrib-type="author"><name><surname>Bai</surname><given-names>Yu-Xian</given-names></name><address><email>bai_yuxian@126.com</email></address><xref ref-type="aff" rid="Aff16">16</xref></contrib><contrib contrib-type="author"><name><surname>Bi</surname><given-names>Jing-Wang</given-names></name><address><email>jingwangbi@live.cn</email></address><xref ref-type="aff" rid="Aff17">17</xref></contrib><contrib contrib-type="author"><name><surname>Ma</surname><given-names>Li-Wen</given-names></name><address><email>13001122683@163.com</email></address><xref ref-type="aff" rid="Aff18">18</xref></contrib><contrib contrib-type="author"><name><surname>Lei</surname><given-names>Jian</given-names></name><address><email>13903070123@163.com</email></address><xref ref-type="aff" rid="Aff19">19</xref></contrib><contrib contrib-type="author"><name><surname>Yang</surname><given-names>Qing</given-names></name><address><email>yangqing@hrs.com.cn</email></address><xref ref-type="aff" rid="Aff20">20</xref></contrib><contrib contrib-type="author"><name><surname>Yu</surname><given-names>Hao</given-names></name><address><email>njyuhao@vip.sina.com</email></address><xref ref-type="aff" rid="Aff21">21</xref></contrib><aff id="Aff1"><label>1</label>Department of Medical Oncology, Sun Yat-sen University Cancer Center, State Key Laboratory of Oncology in South China, Collaborative Innovation Center for Cancer Medicine, Guangzhou, 510060 Guangdong P. R. China </aff><aff id="Aff2"><label>2</label><institution-wrap><institution-id institution-id-type="ISNI">0000 0001 0027 0586<