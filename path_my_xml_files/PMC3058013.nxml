<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Trials</journal-id><journal-title-group><journal-title>Trials</journal-title></journal-title-group><issn pub-type="epub">1745-6215</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">21362184</article-id><article-id pub-id-type="pmc">3058013</article-id><article-id pub-id-type="publisher-id">1745-6215-12-61</article-id><article-id pub-id-type="doi">10.1186/1745-6215-12-61</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Impact on mortality and cancer incidence rates of using random invitation from population registers for recruitment to trials</article-title></title-group><contrib-group><contrib contrib-type="author" id="A1"><name><surname>Burnell</surname><given-names>Matthew</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>m.burnell@ucl.ac.uk</email></contrib><contrib contrib-type="author" id="A2"><name><surname>Gentry-Maharaj</surname><given-names>Aleksandra</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>a.gentry-maharaj@ucl.ac.uk</email></contrib><contrib contrib-type="author" id="A3"><name><surname>Ryan</surname><given-names>Andy</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>a.ryan@ucl.ac.uk</email></contrib><contrib contrib-type="author" id="A4"><name><surname>Apostolidou</surname><given-names>Sophia</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>s.apostolidou@ucl.ac.uk</email></contrib><contrib contrib-type="author" id="A5"><name><surname>Habib</surname><given-names>Mariam</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>m.habib@ucl.ac.uk</email></contrib><contrib contrib-