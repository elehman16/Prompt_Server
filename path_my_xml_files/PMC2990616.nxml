<article article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Br J Cancer</journal-id><journal-title-group><journal-title>British Journal of Cancer</journal-title></journal-title-group><issn pub-type="ppub">0007-0920</issn><issn pub-type="epub">1532-1827</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">20940718</article-id><article-id pub-id-type="pmc">2990616</article-id><article-id pub-id-type="pii">6605943</article-id><article-id pub-id-type="doi">10.1038/sj.bjc.6605943</article-id><article-categories><subj-group subj-group-type="heading"><subject>Clinical Study</subject></subj-group></article-categories><title-group><article-title>Phase II randomised trial of chemoradiotherapy with FOLFOX4 or cisplatin plus fluorouracil in oesophageal cancer</article-title><alt-title alt-title-type="running">Chemoradiotherapy in oesophageal cancer with FOLFOX4</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Conroy</surname><given-names>T</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><contrib contrib-type="author"><name><surname>Yatagh&#x000e8;ne</surname><given-names>Y</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Etienne</surname><given-names>P L</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Michel</surname><given-names>P</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Senellart</surname><given-names>H</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Raoul</surname><given-names>J L</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contr