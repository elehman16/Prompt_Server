<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><?properties manuscript?><front><journal-meta><journal-id journal-id-type="nlm-journal-id">8804070</journal-id><journal-id journal-id-type="pubmed-jr-id">3570</journal-id><journal-id journal-id-type="nlm-ta">Eur J Clin Nutr</journal-id><journal-id journal-id-type="iso-abbrev">Eur J Clin Nutr</journal-id><journal-title-group><journal-title>European journal of clinical nutrition</journal-title></journal-title-group><issn pub-type="ppub">0954-3007</issn><issn pub-type="epub">1476-5640</issn></journal-meta><article-meta><article-id pub-id-type="pmid">28294174</article-id><article-id pub-id-type="pmc">5474320</article-id><article-id pub-id-type="doi">10.1038/ejcn.2017.9</article-id><article-id pub-id-type="manuscript">EMS71151</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Docosahexaenoic acid enrichment in NAFLD is associated with improvements in hepatic metabolism and hepatic insulin sensitivity: a pilot study</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Hodson</surname><given-names>Leanne</given-names></name><xref ref-type="aff" rid="A1">1</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Bhatia</surname><given-names>Lokpal</given-names></name><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Scorletti</surname><given-names>Eleonora</given-names></name><xref ref-type="aff" rid="A2">2</xref><xref ref-type="aff" rid="A3">3</xref></contrib><contrib contrib-type="author"><name><surname>Smith</surname><given-names>Debbie E</given-names></name><xref ref-type="aff" rid="A2">2</xref><xref ref-type="aff" rid="A3">3</xref></contrib><contrib contrib-type="author"><name><surname>Jackson</surname><given-names>Nicola C</given-names></name><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Shojaee-Moradie</surname><given-names>Fariba</given-names></name><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Umpleby</surname><given-names>Margot</given-names></name><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Calder</surname><given-names>Philip C</given-names></name><xref ref-type="aff" rid="A2">2</xref><xref ref-type="aff" rid="A3">3</xref></contrib><contrib contrib-type="author"><name><surname>Byrne</surname><given-names>Christopher D</given-names></name><xref ref-type="aff" rid="A2">2</xref><xref ref-type="aff" rid="A3">3</xref></contrib></contrib-group><aff id="A1">
