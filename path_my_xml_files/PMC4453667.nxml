<article article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Br J Cancer</journal-id><journal-id journal-id-type="iso-abbrev">Br. J. Cancer</journal-id><journal-title-group><journal-title>British Journal of Cancer</journal-title></journal-title-group><issn pub-type="ppub">0007-0920</issn><issn pub-type="epub">1532-1827</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">25880004</article-id><article-id pub-id-type="pmc">4453667</article-id><article-id pub-id-type="pii">bjc2015103</article-id><article-id pub-id-type="doi">10.1038/bjc.2015.103</article-id><article-categories><subj-group subj-group-type="heading"><subject>Clinical Study</subject></subj-group></article-categories><title-group><article-title>Randomised phase II trial of S-1 plus oxaliplatin <italic>vs</italic> S-1 in patients with gemcitabine-refractory pancreatic cancer</article-title><alt-title alt-title-type="running">SOX <italic>vs</italic> S-1 in GEM refractory pancreatic cancer</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Ohkawa</surname><given-names>S</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><contrib contrib-type="author"><name><surname>Okusaka</surname><given-names>T</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Isayama</surname><given-names>H</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Fukutomi</surname><given-names>A</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Yamaguchi</surname><given-names>K</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Ikeda</surn