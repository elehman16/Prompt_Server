pe="author"><name><surname>Yin</surname><given-names>Hai-yan</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib id="author-45201958" contrib-type="author"><name><surname>Wu</surname><given-names>Qiaofeng</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib id="author-49154083" contrib-type="author"><name><surname>Peng</surname><given-names>Jia-xi</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib id="author-49154189" contrib-type="author"><name><surname>Zhang</surname><given-names>Cheng-shun</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib id="author-49154200" contrib-type="author"><name><surname>Lv</surname><given-names>Peng</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib id="author-47671516" contrib-type="author"><name><surname>Tang</surname><given-names>Yong</given-names></name><xref ref-type="aff" rid="aff1"/><xref ref-type="corresp" rid="cor1"/></contrib><contrib id="author-49154211" contrib-type="author"><name><surname>Yu</surname><given-names>Shu-guang</given-names></name><xref ref-type="aff" rid="aff1"/></contrib></contrib-group><aff id="aff1">
