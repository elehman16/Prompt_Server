<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Res Protoc</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Res Protoc</journal-id><journal-id journal-id-type="publisher-id">ResProt</journal-id><journal-title-group><journal-title>JMIR Research Protocols</journal-title></journal-title-group><issn pub-type="epub">1929-0748</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29453190</article-id><article-id pub-id-type="pmc">5834752</article-id><article-id pub-id-type="publisher-id">v7i2e11</article-id><article-id pub-id-type="doi">10.2196/resprot.9090</article-id><article-categories><subj-group subj-group-type="heading"><subject>Protocol</subject></subj-group><subj-group subj-group-type="article-type"><subject>Protocol</subject></subj-group></article-categories><title-group><article-title>Testing Behavior Change Techniques to Encourage Primary Care Physicians to Access Cancer Screening Audit and Feedback Reports: Protocol for a Factorial Randomized Experiment of Email Content</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Hohenstein</surname><given-names>Jess</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Berrigan</surname><given-names>David</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Vaisson</surname><given-names>Gratianne</given-names></name><degrees>BSc, MSc</degrees><xref ref-type="aff" rid="aff01">1</xref><address><institution>Department of Epidemiology</institution><institution>Faculty of Medicine</institution><institution>Laval University</institution><addr-line>Pavillon Ferdinand-Vandry 2881</addr-line><addr-line>1050 Avenue de la M&#x000e9;decine</addr-line><addr-line>Quebec City, QC,</addr-line><country>Canada</country><phone>1 418 656 2131 ext 3981</phone><email>gratianne.vaisson.1@ulaval.ca</email></address><xref ref-type="aff" rid="aff02">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2795-6083</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Witteman</surname><given-names>Holly O</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff02">2</xref><xref ref-type="aff" rid="aff03">3</xref><xref ref-type="aff" rid="aff04">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4192-0682</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Bouck</surname><given-names>Zachary</given-names></name><degrees>MPH</degrees><xref ref-type="aff" rid="aff05">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3145-1915</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Bravo</surname><given-names>Caroline A</given-names></name><degrees>MSc</degrees><xref ref-type="aff" rid="aff06">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3941-705X</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Desveaux</surname><given-names>Laura</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff05">5</xref><xref ref-type="aff" rid="aff07">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3429-1865</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Llovet</surname><given-names>Diego</given-names></name><degrees>MA, PhD</degrees><xref ref-type="aff" rid="aff06">6</xref><xref ref-type="aff" rid="aff07">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6911-5143</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Presseau</surname><given-names>Justin</given-names></name><degrees>BA, MRes, PhD</degrees><xref ref-type="aff" rid="aff08">8</xref><xref ref-type="aff" rid="aff09">9</xref><xref ref-type="aff" rid="aff10">10</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2132-0703</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>Saragosa</surname><given-names>Marianne</given-names></name><degrees>RN, MN</degrees><xref ref-type="aff" rid="aff05">5</xref><xref ref-type="aff" rid="aff11">11</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9261-5971</contrib-id></contrib><contrib id="contrib9" contrib-type="author"><name><surname>Taljaard</surname><given-names>Monica</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff08">8</xref><xref ref-type="aff" rid="aff09">9</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3978-8961</contrib-id></contrib><contrib id="contrib10" contrib-type="author"><name><surname>Umar</surname><given-names>Shama</given-names></name><degrees>BA, MSc</degrees><xref ref-type="aff" rid="aff06">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3648-2430</contrib-id></contrib><contrib id="contrib11" contrib-type="author"><name><surname>Grimshaw</surname><given-names>Jeremy M</given-names></name><degrees>MBChB, PhD</degrees><xref ref-type="aff" rid="aff08">8</xref><xref ref-type="aff" rid="aff12">12</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-8015-8243</contrib-id></contrib><contrib id="contrib12" contrib-type="author"><name><surname>Tinmouth</surname><given-names>Jill</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff06">6</xref><xref ref-type="aff" rid="aff13">13</xref><xref ref-type="aff" rid="aff14">14</xref><xref ref-type="aff" rid="aff15">15</xref><contrib-id contri