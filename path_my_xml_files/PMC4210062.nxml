name><xref ref-type="aff" rid="jdi12163-aff-0002"><sup>2</sup></xref></contrib><contrib id="jdi12163-cr-0003" contrib-type="author"><name><surname>Shanmugavel Jeganathan</surname><given-names>Punnaimuthu</given-names></name><xref ref-type="aff" rid="jdi12163-aff-0001"><sup>1</sup></xref></contrib></contrib-group><aff id="jdi12163-aff-0001"><label><sup>1</sup></label><named-content content-type="organisation-division">Department of Physiology</named-content><institution>Kasturba Medical College</institution><institution>Manipal University</institution><named-content content-type="city">Mangalore</named-content><named-content content-type="country-part">Karnataka</named-content><country country="IN">India</country></aff><aff id="jdi12163-aff-0002"><label><sup>2</sup></label><named-content content-type="organisation-division">Department of Medicine</named-content><institution>Kasturba Medical College</institution><institution>Manipal University</institution><named-content content-type="city">Mangalore</named-content><named-content content-type="country-part">Karnataka</named-content><country country="IN">India</country></aff><author-notes><corresp id="correspondenceTo"><label>*</label><bold>Correspondence</bold><break/>
