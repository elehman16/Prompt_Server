<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Nutrients</journal-id><journal-id journal-id-type="iso-abbrev">Nutrients</journal-id><journal-id journal-id-type="publisher-id">nutrients</journal-id><journal-title-group><journal-title>Nutrients</journal-title></journal-title-group><issn pub-type="epub">2072-6643</issn><publisher><publisher-name>MDPI</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">24824288</article-id><article-id pub-id-type="pmc">4042575</article-id><article-id pub-id-type="doi">10.3390/nu6051913</article-id><article-id pub-id-type="publisher-id">nutrients-06-01913</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Urinary Metabolite Profiles in Premature Infants Show Early Postnatal Metabolic Adaptation and Maturation</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Moltu</surname><given-names>Sissel J.</given-names></name><xref ref-type="aff" rid="af1-nutrients-06-01913">1</xref><xref ref-type="aff" rid="af2-nutrients-06-01913">2</xref><xref ref-type="author-notes" rid="fn1-nutrients-06-01913">&#x02020;</xref></contrib><contrib contrib-type="author"><name><surname>Sachse</surname><given-names>Daniel</given-names></name><xref ref-type="aff" rid="af3-nutrients-06-01913">3</xref><xref ref-type="aff" rid="af4-nutrients-06-01913">4</xref><xref ref-type="aff" rid="af5-nutrients-06-01913">5</xref><xref ref-type="author-notes" rid="fn1-nutrients-06-01913">&#x02020;</xref><xref rid="c1-nutrients-06-01913" ref-type="corresp">*</xref></contrib><contrib contrib-type="author"><name><surname>Blakstad</surname><given-names>Elin W.</given-names></name><xref ref-type="aff" rid="af6-nutrients-06-01913">6</xref><xref r