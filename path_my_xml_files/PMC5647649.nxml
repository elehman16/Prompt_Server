<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><?properties manuscript?><front><journal-meta><journal-id journal-id-type="nlm-journal-id">9420840</journal-id><journal-id journal-id-type="pubmed-jr-id">8578</journal-id><journal-id journal-id-type="nlm-ta">Ann Surg Oncol</journal-id><journal-id journal-id-type="iso-abbrev">Ann. Surg. Oncol.</journal-id><journal-title-group><journal-title>Annals of surgical oncology</journal-title></journal-title-group><issn pub-type="ppub">1068-9265</issn><issn pub-type="epub">1534-4681</issn></journal-meta><article-meta><article-id pub-id-type="pmid">18953611</article-id><article-id pub-id-type="pmc">5647649</article-id><article-id pub-id-type="doi">10.1245/s10434-008-0177-7</article-id><article-id pub-id-type="manuscript">NIHMS889433</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>A Randomized, Phase II Study of Preoperative plus Postoperative Imatinib in GIST: Evidence of Rapid Radiographic Response and Temporal Induction of Tumor Cell Apoptosis</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>McAuliffe</surname><given-names>John C.</given-names></name><xref ref-type="aff" rid="A1">1</xref><xref ref-type="aff" rid="A2">2</xref><xref ref-type="aff" rid="A3">3</xref></contrib><contrib contrib-type="author"><name><surname>Hunt</surname><given-names>Kelly K.</given-names></name><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Lazar</surname><given-names>Alexander J. F.</given-names></name><xref ref-type="aff" rid="A3">3</xref><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Choi</surname><given-names>Haesun</given-names></name><xref ref-type="aff" rid="A6">6</xref></contrib><contrib contrib-type="author"><name><surname>Qiao</surname><given-names>Wei</given-names></name><xref ref-type="aff" rid="A7">7</xref></contrib><contrib contrib-type="author"><name><surname>Thall</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="A7">7</xref></contrib><contrib contrib-type="author"><name><surname>Pollock</surname><given-names>Raphael E.</given-names></name><xref ref-type="aff" rid="A3">3</xref><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Benjamin</surname><given-names>Robert S.</given-names></name><xref ref-type="aff" rid="A8">8</xref></contrib><contrib contrib-type="author"><name><surname>Trent</surname><given-names>Jonathan C.</given-names></name><!--<email>jtrent@mdanderson.org</email>--><xref ref-type="aff" rid="A2">2</xref><xref ref-type="aff" rid="A3">3</xref></contrib></contrib-group><aff id="A1">
