<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Endocr Disord</journal-id><journal-title>BMC Endocrine Disorders</journal-title><issn pub-type="epub">1472-6823</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18671870</article-id><article-id pub-id-type="pmc">2518542</article-id><article-id pub-id-type="publisher-id">1472-6823-8-9</article-id><article-id pub-id-type="doi">10.1186/1472-6823-8-9</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title>Family physician and endocrinologist coordination as the basis for diabetes care in clinical practice</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Duran</surname><given-names>Alejandra</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>aduranh@hotmail.com</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Runkle</surname><given-names>Isabelle</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>irunkle.hcsc@salud.madrid.org</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Mat&#x000ed;a</surname><given-names>Pilar</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>mmatia.hcsc@salud.madrid.org</email></contrib><contrib id="A4" contrib-type="author"><name><surname>de Miguel</surname><given-names>Maria P</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>mdemiguel.hcsc@salud.madrid.org</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Garrido</surname><given-names>Sofia</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>sgarrido.gapm07@salud.madrid.org</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Cervera</surname><given-names>Emilio</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>ecervera.gapm07@salud.madrid.org</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Fernandez</surname><given-names>Maria D</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>mdfernandez.hcsc@salud.madrid.org</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Torres</surname><given-names>Pilar</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>mtorres.hcsc@salud.madrid.org</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Lillo</surname><given-names>Tomas</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>tlilllo.hcsc@salud.madrid.org</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Martin</surname><given-names>Patricia</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>pmartinrojasmarcos@yahoo.es</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Cabrerizo</surname><given-names>Lucio</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>lcabrerizo.hcsc@salud.madrid.org</email></contrib><contrib id="A12" contrib-type="author"><name><surname>de la Torre</surname><given-names>Nuria Garcia</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>nurialobo@hotmail.com</email></contrib><contrib id="A13" contrib-type="author"><name><surname>Calle</surname><given-names>Jose R</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jrcalle@telefonica.net</email></contrib><contrib id="A14" contrib-type="author"><name><surname>Ibarra</surname><given-names>Jose</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jibarra.hcsc@salud.madrid.org</email></contrib><contrib id="A15" contrib-type="author"><name><surname>Charro</surname><given-names>Aniceto L</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>acharro.hcsc@salud.madrid.org</email></contrib><contrib id="A16" corresp="yes" contrib-type="author"><name><surname>Calle-Pascual</surname><given-names>Alfonso L</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>acalle.hcsc@salud.madrid.org</email></contrib></contrib-group><aff id="I1"><label>1</label>Servicio de Endocrinolog&#x000ed;a y Nutrici&#x000f3;n, Hospital Cl&#x000ed;nico San Carlos, Madrid, Spain</aff><aff id="I2"><label>2</label>Unidad de Formaci&#x000f3;n e Investigaci&#x000f3;n de Atenci&#x000f3;n Primaria, &#x000c1;rea 7, Madrid, Spain</aff><aff id="I3"><label>3</label>Direcci&#x000f3;n M&#x000e9;dica, coordinaci&#x000f3;n atenci&#x000f3;n primaria-especializada, Hospital Cl&#x000ed;nico San Carlos, Madrid, Spain</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>31</day><month>7</month><year>2008</year></pub-date><volume>8</volume><fpage>9</fpage><lpage>9</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1472-6823/8/9"/><history><date date-type="received"><day>10</day><month>10</month><year>2007</year></date><date date-type="accepted"><day>31</day><month>7</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Duran et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Duran et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
