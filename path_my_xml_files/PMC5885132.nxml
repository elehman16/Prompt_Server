rib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-5344-716X</contrib-id><name><surname>Loparev</surname><given-names>Sergay</given-names></name><xref ref-type="aff" rid="af2-inj-1834996-498"><sup>2</sup></xref></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-7681-5447</contrib-id><name><surname>Gainullina</surname><given-names>Yuliya</given-names></name><xref ref-type="aff" rid="af1-inj-1834996-498"><sup>1</sup></xref></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0003-2680-0260</contrib-id><name><surname>Kosilova</surname><given-names>Liliya</given-names></name><xref ref-type="aff" rid="af3-inj-1834996-498"><sup>3</sup></xref></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-2177-7447</contrib-id><name><surname>Prokofyeva</surname><given-names>Alexandra</given-names></name><xref ref-type="aff" rid="af1-inj-1834996-498"><sup>1</sup></xref></contrib><aff id="af1-inj-1834996-498">
