f1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9288-4071</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Valiente</surname><given-names>Rosa M</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2369-3295</contrib-id></contrib><contrib id="contrib3" contrib-type="author" corresp="yes"><name><surname>Chorot</surname><given-names>Paloma</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>Faculty of Psychology</institution><institution>Universidad Nacional de Educaci&#x000f3;n a Distancia</institution><addr-line>Juan del Rosal, 10</addr-line><addr-line>Madrid, 28040</addr-line><country>Spain</country><phone>34 913987940</phone><fax>34 913987748</fax><email>pchorot@psi.uned.es</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5442-2917</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Ehrenreich-May</surname><given-names>Jill</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7436-5393</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Kennedy</surname><given-names>Sarah M</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-1759-3362</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Sand&#x000ed;n</surname><given-names>Bonifacio</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7206-6410</contrib-id></contrib></contrib-group><aff id="aff1">
