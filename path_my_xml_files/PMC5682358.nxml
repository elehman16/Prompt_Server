 id="author-49815181" contrib-type="author"><name><surname>Huang</surname><given-names>Jun</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-49816372" contrib-type="author"><name><surname>Guan</surname><given-names>Yubao</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-49893395" contrib-type="author"><name><surname>Wang</surname><given-names>Fengnan</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-49893562" contrib-type="author"><name><surname>Xu</surname><given-names>Xin</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-49911308" contrib-type="author"><name><surname>Yang</surname><given-names>Xinyun</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib></contrib-group><aff id="aff1">
