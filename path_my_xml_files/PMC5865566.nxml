<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Ther Clin Risk Manag</journal-id><journal-id journal-id-type="iso-abbrev">Ther Clin Risk Manag</journal-id><journal-id journal-id-type="publisher-id">Therapeutics and Clinical Risk Management</journal-id><journal-title-group><journal-title>Therapeutics and Clinical Risk Management</journal-title></journal-title-group><issn pub-type="ppub">1176-6336</issn><issn pub-type="epub">1178-203X</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29593415</article-id><article-id pub-id-type="pmc">5865566</article-id><article-id pub-id-type="doi">10.2147/TCRM.S153397</article-id><article-id pub-id-type="publisher-id">tcrm-14-565</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Exercise habituation is effective for improvement of periodontal disease status: a prospective intervention study</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Omori</surname><given-names>Shoei</given-names></name><xref ref-type="aff" rid="af1-tcrm-14-565">1</xref><xref ref-type="aff" rid="af2-tcrm-14-565">2</xref></contrib><contrib contrib-type="author"><name><surname>Uchida</surname><given-names>Fumihiko</given-names></name><xref ref-type="aff" rid="af3-tcrm-14-565">3</xref><xref ref-type="corresp" rid="c1-tcrm-14-565"/></contrib><contrib contrib-type="author"><name><surname>Oh</surname><given-names>Sechang</given-names></name><xref ref-type="aff" rid="af4-tcrm-14-565">4</xref><xref ref-type="aff" rid="af5-tcrm-14-565">5</xref></contrib><contrib contrib-type="author"><name><surname>So</surname><given-names>Rina</given-names></name><xref ref-type="aff" rid="af6-tcrm-14-565">6</xref></contrib><contrib contrib-type="author"><name><surname>Tsujimoto</surname><given-names>Takehiko</given-names></name><xref ref-type="aff" rid="af7-tcrm-14-565">7</xref></contrib><contrib contrib-type="author"><name><surname>Yanagawa</surname><given-names>Toru</given-names></name><xref ref-type="aff" rid="af8-tcrm-14-565">8</xref></contrib><contrib contrib-type="author"><name><surname>Sakai</surname><given-names>Satoshi</given-names></name><xref ref-type="aff" rid="af4-tcrm-14-565">4</xref></contrib><contrib contrib-type="author"><name><surname>Shoda</surname><given-names>Junichi</given-names></name><xref ref-type="aff" rid="af4-tcrm-14-565">4</xref><xref ref-type="aff" rid="af5-tcrm-14-565">5</xref></contrib><contrib contrib-type="author"><name><surname>Tanaka</surname><given-names>Kiyoji</given-names></name><xref ref-type="aff" rid="af9-tcrm-14-565">9</xref></contrib><contrib contrib-type="author"><name><surname>Bukawa</surname><given-names>Hiroki</given-names></name><xref ref-type="aff" rid="af8-tcrm-14-565">8</xref></contrib></contrib-group><aff id="af1-tcrm-14-565">
