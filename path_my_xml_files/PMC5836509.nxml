<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><?properties manuscript?><front><journal-meta><journal-id journal-id-type="nlm-journal-id">101717957</journal-id><journal-id journal-id-type="pubmed-jr-id">47109</journal-id><journal-id journal-id-type="nlm-ta">Curr Dev Nutr</journal-id><journal-id journal-id-type="iso-abbrev">Curr Dev Nutr</journal-id><journal-title-group><journal-title>Current developments in nutrition</journal-title></journal-title-group><issn pub-type="epub">2475-2991</issn></journal-meta><article-meta><article-id pub-id-type="pmid">29517074</article-id><article-id pub-id-type="pmc">5836509</article-id><article-id pub-id-type="doi">10.3945/cdn.117.000703</article-id><article-id pub-id-type="manuscript">NIHMS880803</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Influence of Protein Intake, Race, and Age on Responses to a Weight-Reduction Intervention in Obese Women<sup><xref ref-type="fn" rid="FN2">1</xref>,<xref ref-type="fn" rid="FN3">2</xref></sup></article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Bales</surname><given-names>Connie W</given-names></name><xref ref-type="aff" rid="A1">3</xref><xref ref-type="aff" rid="A2">4</xref><xref ref-type="aff" rid="A4">6</xref><xref ref-type="author-notes" rid="FN1">*</xref></contrib><contrib contrib-type="author"><name><surname>Porter Starr</surname><given-names>Kathryn N</given-names></name><xref ref-type="aff" rid="A1">3</xref><xref ref-type="aff" rid="A4">6</xref></contrib><contrib contrib-type="author"><name><surname>Orenduff</surname><given-names>Melissa C</given-names></name><xref ref-type="aff" rid="A1">3</xref></contrib><contrib contrib-type="author"><name><surname>McDonald</surname><given-names>Shelley R</given-names></name><xref ref-type="aff" rid="A1">3</xref><xref ref-type="aff" rid="A2">4</xref><xref ref-type="aff" rid="A4">6</xref></contrib><contrib contrib-type="author"><name><surname>Molnar</surname><given-names>Karen</given-names></name><xref ref-type="aff" rid="A1">3</xref></contrib><contrib contrib-type="author"><name><surname>Jarman</surname><given-names>Aubrey K</given-names></name><xref ref-type="aff" rid="A1">3</xref></contrib><contrib contrib-type="author"><name><surname>Onyenwoke</surname><given-names>Ann</given-names></name><xref ref-type="aff" rid="A1">3</xref></contrib><contrib contrib-type="author"><name><surname>Mulder</surname><given-names>Hillary</given-names></name><xref ref-type="aff" rid="A3">5</xref></contrib><contrib contrib-type="author"><name><surname>Payne</surname><given-names>Martha E</given-names></name><xref ref-type="aff" rid="A5">7</xref></contrib><contrib contrib-type="author"><name><surname>Pieper</surname><given-names>Carl F</given-names></name><xref ref-type="aff" rid="A3">5</xref></contrib></contrib-group><aff id="A1">
