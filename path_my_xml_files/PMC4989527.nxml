/name><address><email>trzaskoma.benjamin@gene.com</email></address><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Ros&#x000e9;n</surname><given-names>Karin</given-names></name><address><email>rosen.karin@gene.com</email></address><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Omachi</surname><given-names>Theodore A.</given-names></name><address><email>omachi.theodore@gene.com</email></address><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Stull</surname><given-names>Donald</given-names></name><address><email>dstull@rti.org</email></address><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Balp</surname><given-names>Maria-Magdalena</given-names></name><address><email>maria-magdalena.balp@novartis.com</email></address><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Murphy</surname><given-names>Thomas</given-names></name><address><email>thomas_murphy_md@hotmail.com</email></address><xref ref-type="aff" rid="Aff6">6</xref></contrib><aff id="Aff1"><label>1</label>Hospital del Mar, Institut Mar d&#x02019;Investigacions M&#x000e8;diques, Universitat Autonoma Barcelona, Barcelona, Spain </aff><aff id="Aff2"><label>2</label>California Allergy and Asthma Medical Group, Inc., 11645 Wilshire Blvd, #1155, Los Angeles, CA 90025 USA </aff><aff id="Aff3"><label>3</label>Medical Affairs, Genentech, Inc., 1 DNA Way, South San Francisco, CA 94080-4990 USA </aff><aff id="Aff4"><label>4</label>Data Analytics and Design Strategy, RTI Health Solutions, Manchester, UK </aff><aff id="Aff5"><label>5</label>HEOR in Global Patient Access, Novartis Pharma AG, 4002 Basel, Switzerland </aff><aff id="Aff6"><label>6</label>National Allergy, Asthma, and Urticaria Centers of Charleston, 7555 Northside Drive, Charleston, SC 29420 USA </aff></contrib-group><pub-date pub-type="epub"><day>18</day><month>8</month><year>2016</year></pub-date><pub-date pub-type="pmc-release"><day>18</day><month>8</month><year>2016</year></pub-date><pub-date pub-type="collection"><year>2016</year></pub-date><volume>6</volume><elocation-id>32</elocation-id><history><date date-type="received"><day>3</day><month>3</month><year>2016</year></date><date date-type="accepted"><day>8</day><month>7</month><year>2016</year></date></history><permissions><copyright-statement>&#x000a9; The Author(s) 2016</copyright-statement><license license-type="OpenAccess"><license-p>
