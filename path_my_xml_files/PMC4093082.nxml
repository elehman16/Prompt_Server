 contrib-type="author"><name><surname>Park</surname><given-names>Keun Kyu</given-names></name><xref ref-type="aff" rid="af2-ajas-25-6-832-13">2</xref></contrib><contrib contrib-type="author"><name><surname>Lee</surname><given-names>Sung Sill</given-names></name><xref ref-type="aff" rid="af3-ajas-25-6-832-13">3</xref></contrib><contrib contrib-type="author"><name><surname>Kim</surname><given-names>Kyoung Hoon</given-names></name><xref rid="c1-ajas-25-6-832-13" ref-type="corresp">*</xref></contrib></contrib-group><aff id="af1-ajas-25-6-832-13"><label>1</label>Department of Animal and Food Sciences, University of Kentucky, 
