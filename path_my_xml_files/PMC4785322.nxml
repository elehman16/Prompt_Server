<article xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="protocol"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">26932144</article-id><article-id pub-id-type="pmc">4785322</article-id><article-id pub-id-type="publisher-id">bmjopen-2015-010956</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2015-010956</article-id><article-categories><subj-group subj-group-type="heading"><subject>Neurology</subject><subj-group><subject>Protocol</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1713</subject><subject>1718</subject></subj-group></article-categories><title-group><article-title>Treatment of optic neuritis with erythropoietin (TONE): a randomised, double-blind, placebo-controlled trial&#x02014;study protocol</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Diem</surname><given-names>Ricarda</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Molnar</surname><given-names>Fanni</given-names></name><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Beisse</surname><given-names>Flemming</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surn