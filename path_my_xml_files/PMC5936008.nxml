<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Int J Chron Obstruct Pulmon Dis</journal-id><journal-id journal-id-type="iso-abbrev">Int J Chron Obstruct Pulmon Dis</journal-id><journal-id journal-id-type="publisher-id">International Journal of COPD</journal-id><journal-title-group><journal-title>International Journal of Chronic Obstructive Pulmonary Disease</journal-title></journal-title-group><issn pub-type="ppub">1176-9106</issn><issn pub-type="epub">1178-2005</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29750027</article-id><article-id pub-id-type="pmc">5936008</article-id><article-id pub-id-type="doi">10.2147/COPD.S166023</article-id><article-id pub-id-type="publisher-id">copd-13-1407</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Efficacy of tiotropium/olodaterol on lung volume, exercise capacity, and physical activity</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Ichinose</surname><given-names>Masakazu</given-names></name><xref ref-type="aff" rid="af1-copd-13-1407">1</xref><xref ref-type="corresp" rid="c1-copd-13-1407"/></contrib><contrib contrib-type="author"><name><surname>Minakata</surname><given-names>Yoshiaki</given-names></name><xref ref-type="aff" rid="af2-copd-13-1407">2</xref></contrib><contrib contrib-type="author"><name><surname>Motegi</surname><given-names>Takashi</given-names></name><xref ref-type="aff" rid="af3-copd-13-1407">3</xref></contrib><contrib contrib-type="author"><name><surname>Ueki</surname><given-names>Jun</given-names></name><xref ref-type="aff" rid="af4-copd-13-1407">4</xref></contrib><contrib contrib-type="author"><name><surname>Gon</surname><given-names>Yasuhiro</given-names></name><xref ref-type="aff" rid="af5-copd-13-1407">5</xref></contrib><contrib contrib-type="author"><name><surname>Seki</surname><given-names>Tetsuo</given-names></name><xref ref-type="aff" rid="af6-copd-13-1407">6</xref></contrib><contrib contrib-type="author"><name><surname>Anzai</surname><given-names>Tatsuhiko</given-names></name><xref ref-type="aff" rid="af7-copd-13-1407">7</xref></contrib><contrib contrib-type="author"><name><surname>Nakamura</surname><given-names>Shuhei</given-names></name><xref ref-type="aff" rid="af6-copd-13-1407">6</xref></contrib><contrib contrib-type="author"><name><surname>Hirata</surname><given-names>Kazuto</given-names></name><xref ref-type="aff" rid="af8-copd-13-1407">8</xref></contrib></contrib-group><aff id="af1-copd-13-1407">
