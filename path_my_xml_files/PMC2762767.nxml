ff id="aff3" rid="aff3"><sup>3</sup><addr-line>Knowledge Translation Program</addr-line><institution>Li Ka Shing Knowledge Institute</institution><addr-line>Toronto</addr-line><addr-line>ON</addr-line><country>Canada</country></aff><aff id="aff2" rid="aff2"><sup>2</sup><addr-line>Division of General Internal Medicine</addr-line><institution>University Health Network</institution><addr-line>Toronto</addr-line><addr-line>ON</addr-line><country>Canada</country></aff><aff id="aff1" rid="aff1"><sup>1</sup><institution>Department of Medicine</institution><institution>University of Toronto</institution><addr-line>Toronto</addr-line><addr-line>ON</addr-line><country>Canada</country></aff><pub-date pub-type="collection"><season>Jan-Mar</season><year>2009</year></pub-date><pub-date pub-type="epub"><day>6</day><month>2</month><year>2009</year></pub-date><volume>11</volume><issue>1</issue><elocation-id>e3</elocation-id><history><date date-type="received"><day>02</day><month>7</month><year>2008</year></date><date date-type="rev-request"><day>27</day><month>8</month><year>2008</year></date><date date-type="rev-recd"><day>03</day><month>11</month><year>2008</year></date><date date-type="accepted"><day>21</day><month>11</month><year>2008</year></date></history><permissions><copyright-statement>&#x000a9; Robert C Wu, Kevin Thorpe, Heather Ross, Vaska Micevski, Christine Marquez, Sharon E Straus.  Originally published in the Journal of Medical Internet Research (http://www.jmir.org), 06.02.2009.  &#x000a0;
