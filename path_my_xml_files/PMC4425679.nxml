ntrib><contrib contrib-type="author"><name><surname>Palmeri</surname><given-names>Cesira</given-names></name><xref ref-type="aff" rid="aff001"/></contrib><contrib contrib-type="author"><name><surname>Raineri</surname><given-names>Santi Maurizio</given-names></name><xref ref-type="aff" rid="aff001"/></contrib><contrib contrib-type="author"><name><surname>Giarratano</surname><given-names>Antonino</given-names></name><xref ref-type="aff" rid="aff001"/></contrib></contrib-group><aff id="aff001">
