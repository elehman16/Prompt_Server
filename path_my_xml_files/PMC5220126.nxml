<article xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Leukemia</journal-id><journal-id journal-id-type="iso-abbrev">Leukemia</journal-id><journal-title-group><journal-title>Leukemia</journal-title></journal-title-group><issn pub-type="ppub">0887-6924</issn><issn pub-type="epub">1476-5551</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27416912</article-id><article-id pub-id-type="pmc">5220126</article-id><article-id pub-id-type="pii">leu2016176</article-id><article-id pub-id-type="doi">10.1038/leu.2016.176</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title>A randomized phase III study of carfilzomib vs low-dose corticosteroids with optional cyclophosphamide in relapsed and refractory multiple myeloma (FOCUS)</article-title><alt-title alt-title-type="running">Carfilzomib vs corticosteroids/cyclophosphamide</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>H&#x000e1;jek</surname><given-names>R</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><contrib contrib-type="author"><name><surname>Masszi</surname><given-names>T</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Petrucci</surname><given-names>M T</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Palumbo</surname><given-names>A</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Rosi&#x000f1;ol</surname><given-names>L</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib con