<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Haematologica</journal-id><journal-id journal-id-type="iso-abbrev">Haematologica</journal-id><journal-id journal-id-type="hwp">haematol</journal-id><journal-id journal-id-type="publisher-id">Haematologica</journal-id><journal-title-group><journal-title>Haematologica</journal-title></journal-title-group><issn pub-type="ppub">0390-6078</issn><issn pub-type="epub">1592-8721</issn><publisher><publisher-name>Ferrata Storti Foundation</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29242294</article-id><article-id pub-id-type="pmc">5830373</article-id><article-id pub-id-type="doi">10.3324/haematol.2017.171199</article-id><article-id pub-id-type="publisher-id">1030531</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject><subj-group subj-group-type="heading"><subject>Cell Therapy &#x00026; Immunotherapy</subject></subj-group></subj-group></article-categories><title-group><article-title><italic>In vivo</italic> IL-12/IL-23p40 neutralization blocks Th1/Th17 response after allogeneic hematopoietic cell transplantation</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Pidala</surname><given-names>Joseph</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref><xref ref-type="corresp" rid="c1-1030531"/></contrib><contrib contrib-type="author"><name><surname>Beato</surname><given-names>Francisca</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref></contrib><contrib contrib-type="author"><name><surname>Kim</surname><given-names>Jongphil</given-names></name><xref ref-type="aff" rid="af3-1030531">3</xref></contrib><contrib contrib-type="author"><name><surname>Betts</surname><given-names>Brian</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Jim</surname><given-names>Heather</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af4-1030531">4</xref></contrib><contrib contrib-type="author"><name><surname>Sagatys</surname><given-names>Elizabeth</given-names></name><xref ref-type="aff" rid="af5-1030531">5</xref></contrib><contrib contrib-type="author"><name><surname>Levine</surname><given-names>John E.</given-names></name><xref ref-type="aff" rid="af6-1030531">6</xref></contrib><contrib contrib-type="author"><name><surname>Ferrara</surname><given-names>James L.M.</given-names></name><xref ref-type="aff" rid="af6-1030531">6</xref></contrib><contrib contrib-type="author"><name><surname>Ozbek</surname><given-names>Umut</given-names></name><xref ref-type="aff" rid="af6-1030531">6</xref></contrib><contrib contrib-type="author"><name><surname>Ayala</surname><given-names>Ernesto</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Davila</surname><given-names>Marco</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Fernandez</surname><given-names>Hugo F.</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Field</surname><given-names>Teresa</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Kharfan-Dabaja</surname><given-names>Mohamed A.</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Khaira</surname><given-names>Divis</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Khimani</surname><given-names>Farhad</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Locke</surname><given-names>Frederick L.</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Mishra</surname><given-names>Asmita</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Nieder</surname><given-names>Michael</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Nishihori</surname><given-names>Taiga</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Perez</surname><given-names>Lia</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Riches</surname><given-names>Marcie</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib><contrib contrib-type="author"><name><surname>Anasetti</surname><given-names>Claudio</given-names></name><xref ref-type="aff" rid="af1-1030531">1</xref><xref ref-type="aff" rid="af2-1030531">2</xref></contrib></contrib-group><aff id="af1-1030531">
