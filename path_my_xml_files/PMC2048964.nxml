<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Endocr Disord</journal-id><journal-title>BMC Endocrine Disorders</journal-title><issn pub-type="epub">1472-6823</issn><publisher><publisher-name>BioMed Central|1</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17868462</article-id><article-id pub-id-type="pmc">2048964</article-id><article-id pub-id-type="publisher-id">1472-6823-7-6</article-id><article-id pub-id-type="doi">10.1186/1472-6823-7-6</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Brief Intervention in Type 1 diabetes &#x02013; Education for Self-efficacy (BITES): Protocol for a randomised control trial to assess biophysical and psychological effectiveness</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>George</surname><given-names>Jyothis T</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>drjtgeorge@yahoo.co.uk</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Valdovinos</surname><given-names>Abel Pe&#x000f1;a</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>abel.pena@imss.gob.mx</email></contrib><contrib id="A3" corresp="yes" contrib-type="author"><name><surname>Thow</surname><given-names>Jonathan C</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jonathan.thow@york.nhs.uk</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Russell</surname><given-names>Ian</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>ian.russell@bangor.ac.uk</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Dromgoole</surname><given-names>Paul</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>paul@betapresentations.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Lomax</surname><given-names>Sarah</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>sarah.lomax@york.nhs.uk</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Torgerson</surname><given-names>David J</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>djt6@york.ac.uk</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Wells</surname><given-names>Tony</given-names></name><xref ref-type="aff" rid="I6">6</xref><email>dianne.roberts@chs.northy.nhs.uk</email></contrib></contrib-group><aff id="I1"><label>1</label>Dept. of Diabetes, York Hospital, York, UK</aff><aff id="I2"><label>2</label>Divisi&#x000f3;n de Innovaci&#x000f3;n en Servicios de Salud, Coordinaci&#x000f3;n de Pol&#x000ed;ticas de Salud, Direcci&#x000f3;n de Prestaciones M&#x000e9;dicas, Instituto Mexicano del Seguro Social, Mexico</aff><aff id="I3"><label>3</label>University of Wales, Bangor, UK</aff><aff id="I4"><label>4</label>Independent Nurse Practitioner, York, UK</aff><aff id="I5"><label>5</label>York Trials Unit, University of York, York, UK</aff><aff id="I6"><label>6</label>Dept. of Psychology, City Hospitals, Sunderland, UK</aff><pub-date pub-type="collection"><year>2007</year></pub-date><pub-date pub-type="epub"><day>14</day><month>9</month><year>2007</year></pub-date><volume>7</volume><fpage>6</fpage><lpage>6</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1472-6823/7/6"/><history><date date-type="received"><day>9</day><month>8</month><year>2007</year></date><date date-type="accepted"><day>14</day><month>9</month><year>2007</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2007 George et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2007</copyright-year><copyright-holder>George et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
