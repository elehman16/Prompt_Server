<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><?properties manuscript?><front><journal-meta><journal-id journal-id-type="nlm-journal-id">0372547</journal-id><journal-id journal-id-type="pubmed-jr-id">1869</journal-id><journal-id journal-id-type="nlm-ta">Br J Nutr</journal-id><journal-id journal-id-type="iso-abbrev">Br. J. Nutr.</journal-id><journal-title-group><journal-title>The British journal of nutrition</journal-title></journal-title-group><issn pub-type="ppub">0007-1145</issn><issn pub-type="epub">1475-2662</issn></journal-meta><article-meta><article-id pub-id-type="pmid">28643618</article-id><article-id pub-id-type="pmc">5654571</article-id><article-id pub-id-type="doi">10.1017/S0007114517001106</article-id><article-id pub-id-type="manuscript">NIHMS912258</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Heat-stabilised rice bran consumption by colorectal cancer survivors modulates stool metabolite profiles and metabolic networks: a randomised controlled trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Brown</surname><given-names>Dustin G.</given-names></name><xref ref-type="aff" rid="A1">1</xref></contrib><contrib contrib-type="author"><name><surname>Borresen</surname><given-names>Erica C.</given-names></name><xref ref-type="aff" rid="A1">1</xref></contrib><contrib contrib-type="author"><name><surname>Brown</surname><given-names>Regina J.</given-names></name><xref ref-type="aff" rid="A2">2</xref><xref ref-type="aff" rid="A3">3</xref></contrib><contrib contrib-type="author"><name><surname>Ryan</surname><given-names>Elizabeth P.</given-names></name><xref ref-type="aff" rid="A1">1</xref><xref ref-type="aff" rid="A3">3</xref><xref rid="FN1" ref-type="author-notes">*</xref></contrib></contrib-group><aff id="A1">
