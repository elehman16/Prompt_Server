<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Prog Orthod</journal-id><journal-id journal-id-type="iso-abbrev">Prog Orthod</journal-id><journal-title-group><journal-title>Progress in Orthodontics</journal-title></journal-title-group><issn pub-type="ppub">1723-7785</issn><issn pub-type="epub">2196-1042</issn><publisher><publisher-name>Springer Berlin Heidelberg</publisher-name><publisher-loc>Berlin/Heidelberg</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27004806</article-id><article-id pub-id-type="pmc">4826863</article-id><article-id pub-id-type="publisher-id">124</article-id><article-id pub-id-type="doi">10.1186/s40510-016-0124-2</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Impact of nanohydroxyapatite on enamel surface roughness and color change after orthodontic debonding</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Ajami</surname><given-names>Shabnam</given-names></name><xref ref-type="aff" rid="Aff1"/></contrib><contrib contrib-type="author"><name><surname>Pakshir</surname><given-names>Hamid Reza</given-names></name><xref ref-type="aff" rid="Aff1"/></contrib><contrib contrib-type="author" corresp="yes"><name><surname>Babanouri</surname><given-names>Neda</given-names></name><address><email>nedababanouri@yahoo.com</email></address><xref ref-type="aff" rid="Aff1"/></contrib><aff id="Aff1">Orthodontic Research Center, Shiraz University of Medical Science, Shiraz, Iran </aff></contrib-group><pub-date pub-type="epub"><day>11</day><month>4</month><year>2016</year></pub-date><pub-date pub-type="pmc-release"><day>11</day><month>4</month><year>2016</year></pub-date><pub-date pub-type="collection"><year>2016<