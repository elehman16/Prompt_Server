<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Cancer</journal-id><journal-title>BMC Cancer</journal-title><issn pub-type="epub">1471-2407</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19055847</article-id><article-id pub-id-type="pmc">2613915</article-id><article-id pub-id-type="publisher-id">1471-2407-8-361</article-id><article-id pub-id-type="doi">10.1186/1471-2407-8-361</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Rationale and design of PROSPECT-CONKO 004: a prospective, randomized trial of simultaneous pancreatic cancer treatment with enoxaparin and chemotherapy)</article-title></title-group><contrib-group><contrib id="A1" equal-contrib="yes" contrib-type="author"><name><surname>Riess</surname><given-names>Hanno</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>hanno.riess@charite.de</email></contrib><contrib id="A2" equal-contrib="yes" corresp="yes" contrib-type="author"><name><surname>Pelzer</surname><given-names>Uwe</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>uwe.pelzer@charite.de</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Hilbig</surname><given-names>Andreas</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>andreas.hilbig@charite.de</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Stieler</surname><given-names>Jens</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jens.stieler@charite.de</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Opitz</surname><given-names>Bernhard</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>b.opitz@krankenhaus-halle-saale.de</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Scholten</surname><given-names>Theo</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>scholten@akh-hagen.de</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Kauschat-Br&#x000fc;ning</surname><given-names>D&#x000f6;rte</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>doerte.kauschat-bruening@sanofi-aventis.com</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Bramlage</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>peter.bramlage@gmx.de</email></contrib><contrib id="A9" contrib-type="author"><name><surname>D&#x000f6;rken</surname><given-names>Bernd</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>bernd.doerken@charite.de</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Oettle</surname><given-names>Helmut</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>helmut.oettle@charite.de</email></contrib></contrib-group><aff id="I1"><label>1</label>Charit&#x000e9; Campus Virchow-Clinic, Medical Clinic Hematology/Oncology, Augustenburger Platz 1, 13353 Berlin, Germany</aff><aff id="I2"><label>2</label>Hospital St. Elisabeth/St. Barbara Halle, Mauerstra&#x000df;e 5, 06110 Halle (Saale), Germany</aff><aff id="I3"><label>3</label>Hospital Hagen, Gr&#x000fc;nstr. 35, 58095 Hagen, Germany</aff><aff id="I4"><label>4</label>Sanofi-Aventis Deutschland GmbH, Potsdamer Strasse 8, 10785 Berlin, Germany</aff><aff id="I5"><label>5</label>Institute for Clinical Pharmacology, Medical Faculty, TU Dresden, Fiedlerstrasse 27, 01307 Dresden, Germany</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>5</day><month>12</month><year>2008</year></pub-date><volume>8</volume><fpage>361</fpage><lpage>361</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1471-2407/8/361"/><history><date date-type="received"><day>3</day><month>11</month><year>2008</year></date><date date-type="accepted"><day>5</day><month>12</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Riess et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Riess et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
