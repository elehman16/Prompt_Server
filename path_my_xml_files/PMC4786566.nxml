eft" rowspan="1" colspan="1">&#x000a0;&#x000a0;&#x000a0;&#x000a0;Back complaints</td><td valign="top" align="center" rowspan="1" colspan="1">9.8</td><td valign="top" align="center" rowspan="1" colspan="1">13.9</td><td valign="top" align="center" rowspan="1" colspan="1">14.3</td></tr><tr><td valign="top" align="left" rowspan="1" colspan="1">&#x000a0;&#x000a0;&#x000a0;&#x000a0;Fibromyalgia</td><td valign="top" align="center" rowspan="1" colspan="1">15.9</td><td valign="top" align="center" rowspan="1" colspan="1">29.1</td><td valign="top" align="center" rowspan="1" colspan="1">15.6</td></tr><tr><td valign="top" align="left" rowspan="1" colspan="1">&#x000a0;&#x000a0;&#x000a0;&#x000a0;Joint complaints</td><td valign="top" align="center" rowspan="1" colspan="1">8.5</td><td valign="top" align="center" rowspan="1" colspan="1">7.6</td><td valign="top" align="center" rowspan="1" colspan="1">9.1</td></tr><tr><td valign="top" align="left" rowspan="1" colspan="1">&#x000a0;&#x000a0;&#x000a0;&#x000a0;Rheumatic disease</td><td valign="top" align="center" rowspan="1" colspan="1">9.8</td><td valign="top" align="center" rowspan="1" colspan="1">7.6</td><td valign="top" align="center" rowspan="1" colspan="1">11.7</td></tr><tr><td valign="top" align="left" rowspan="1" colspan="1">&#x000a0;&#x000a0;&#x000a0;&#x000a0;Neuropathic complaints</td><td valign="top" align="center" rowspan="1" colspan="1">11.0</td><td valign="top" align="center" rowspan="1" colspan="1">6.3</td><td valign="top" align="center" rowspan="1" colspan="1">9.1</td></tr><tr><td valign="top" align="left" rowspan="1" colspan="1">&#x000a0;&#x000a0;&#x000a0;&#x000a0;Other</td><td valign="top" align="center" rowspan="1" colspan="1">30.5</td><td valign="top" align="center" rowspan="1" colspan="1">20.8</td><td valign="top" align="center" rowspan="1" colspan="1">20.7</td></tr><tr><td valign="top" align="left" colspan="4" rowspan="1"><bold>Physical domain measures</bold></td></tr><tr><td valign="top" align="left" rowspan="1" colsp