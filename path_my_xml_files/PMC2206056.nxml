<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Int Soc Sports Nutr</journal-id><journal-title>Journal of the International Society of Sports Nutrition</journal-title><issn pub-type="epub">1550-2783</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17997840</article-id><article-id pub-id-type="pmc">2206056</article-id><article-id pub-id-type="publisher-id">1550-2783-4-18</article-id><article-id pub-id-type="doi">10.1186/1550-2783-4-18</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title>Effects of ingesting protein with various forms of carbohydrate following resistance-exercise on substrate availability and markers of anabolism, catabolism, and immunity</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Kreider</surname><given-names>Richard B</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>richard_kreider@baylor.edu</email></contrib><contrib id="A2" equal-contrib="yes" contrib-type="author"><name><surname>Earnest</surname><given-names>Conrad P</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>conrad.earnest@pbrc.edu</email></contrib><contrib id="A3" equal-contrib="yes" contrib-type="author"><name><surname>Lundberg</surname><given-names>Jennifer</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>jenlundberg@yahoo.com</email></contrib><contrib id="A4" equal-contrib="yes" contrib-type="author"><name><surname>Rasmussen</surname><given-names>Christopher</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>chris_rasmussen@baylor.edu</email></contrib><contrib id="A5" equal-contrib="yes" contrib-type="author"><name><surname>Greenwood</surna