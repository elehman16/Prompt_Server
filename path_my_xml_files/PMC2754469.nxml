 id="A6" contrib-type="author"><name><surname>Auinger</surname><given-names>A</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>annegret.auinger@mri.bund.de</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Laue</surname><given-names>C</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>c.laue@tecura.com</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Winkler</surname><given-names>P</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>winkler-petra@gmx.de</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Schreiber</surname><given-names>S</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>Doris.Bell@cognis.com</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Bell</surname><given-names>D</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>juergen.schrezenmeir@gmx.de</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Schrezenmeir</surname><given-names>J</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>s.schreiber@mucosa.de</email></contrib></contrib-group><aff id="I1"><label>1</label>Max Rubner-Institut, Federal Research Institute of Nutrition and Food, Department of Physiology and Biochemistry of Nutrition, Karlsruhe and Kiel, Germany</aff><aff id="I2"><label>2</label>UKSH Kiel, First Department of Medicine, Kiel, Germany</aff><aff id="I3"><label>3</label>UKSH Kiel, Institute for Clinical Molecular Biology, Kiel, Germany</aff><aff id="I4"><label>4</label>Center of Clinical Research, Tecura GmbH Medizin &#x00026; Biotechnik, Kiel, Germany</aff><aff id="I5"><label>5</label>Nutrition &#x00026; Health - Global R &#x00026; D, Cognis GmbH, Monheim, Germany</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>18</day><month>8</month><year>2009</year></pub-date><volume>8</volume><fpage>35</fpage><lpage>35</lpage><ext-link ext-link-type="uri" xlink:href="http://www.lipidworld.com/content/8/1/35"/><history><date date-type="received"><day>16</day><month>6</month><year>2009</year></date><date date-type="accepted"><day>18</day><month>8</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Herrmann et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Herrmann et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
