<article xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Br J Cancer</journal-id><journal-id journal-id-type="iso-abbrev">Br. J. Cancer</journal-id><journal-title-group><journal-title>British Journal of Cancer</journal-title></journal-title-group><issn pub-type="ppub">0007-0920</issn><issn pub-type="epub">1532-1827</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27092785</article-id><article-id pub-id-type="pmc">4984913</article-id><article-id pub-id-type="pii">bjc201671</article-id><article-id pub-id-type="doi">10.1038/bjc.2016.71</article-id><article-categories><subj-group subj-group-type="heading"><subject>Clinical Study</subject></subj-group></article-categories><title-group><article-title>Adjuvant ovarian function suppression and cognitive function in women with breast cancer</article-title><alt-title alt-title-type="running">Ovarian function suppression and cognitive function</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Phillips</surname><given-names>Kelly-Anne</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref><xref ref-type="aff" rid="aff4">4</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><contrib contrib-type="author"><name><surname>Regan</surname><given-names>Meredith M</given-names></name><xref ref-type="aff" rid="aff5">5</xref><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Ribi</surname><given-names>Karin</given-names></name><xref ref-type="aff" rid="aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Francis</surname><given-names>Prudence A</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref>