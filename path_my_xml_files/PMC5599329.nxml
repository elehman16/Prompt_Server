<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><?properties manuscript?><front><journal-meta><journal-id journal-id-type="nlm-journal-id">0100714</journal-id><journal-id journal-id-type="pubmed-jr-id">6400</journal-id><journal-id journal-id-type="nlm-ta">Pediatr Res</journal-id><journal-id journal-id-type="iso-abbrev">Pediatr. Res.</journal-id><journal-title-group><journal-title>Pediatric research</journal-title></journal-title-group><issn pub-type="ppub">0031-3998</issn><issn pub-type="epub">1530-0447</issn></journal-meta><article-meta><article-id pub-id-type="pmid">28553989</article-id><article-id pub-id-type="pmc">5599329</article-id><article-id pub-id-type="doi">10.1038/pr.2017.130</article-id><article-id pub-id-type="manuscript">NIHMS878743</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Neuroimaging in Former Preterm Children Who Received Erythropoiesis Stimulating Agents</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Phillips</surname><given-names>Authors: John</given-names></name><xref ref-type="aff" rid="A1">1</xref><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Yeo</surname><given-names>Ronald A.</given-names></name><xref ref-type="aff" rid="A3">3</xref></contrib><contrib contrib-type="author"><name><surname>Caprihan</surname><given-names>Arvind</given-names></name><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Cannon</surname><given-names>Daniel C</given-names></name><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Patel</surname><given-names>Shrena</given-names></name><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Winter</surname><given-names>Sarah</given-names></name><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Steffen</surname><given-names>Michael</given-names></name><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Campbell</surname><given-names>Richard</given-names></name><xref ref-type="aff" rid="A6">6</xref></contrib><contrib contrib-type="author"><name><surname>Wiedmeier</surname><given-names>Susan</given-names></name><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Baker</surname><given-names>Shawna</given-names></name><xref ref-type="aff" rid="A7">7</xref></contrib><contrib contrib-type="author"><name><surname>Gonzales</surname><given-names>Sean</given-names></name><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Lowe</surname><given-names>Jean</given-names></name><xref ref-type="aff" rid="A8">8</xref></contrib><contrib contrib-type="author"><name><surname>Ohls</surname><given-names>Robin K.</given-names></name><xref ref-type="aff" rid="A8">8</xref></contrib></contrib-group><aff id="A1">
