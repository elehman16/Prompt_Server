<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Cardiovasc Diabetol</journal-id><journal-title>Cardiovascular Diabetology</journal-title><issn pub-type="epub">1475-2840</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17316429</article-id><article-id pub-id-type="pmc">1819366</article-id><article-id pub-id-type="publisher-id">1475-2840-6-8</article-id><article-id pub-id-type="doi">10.1186/1475-2840-6-8</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Investigation</subject></subj-group></article-categories><title-group><article-title>Pro-oxidant effect of &#x003b1;-tocopherol in patients with Type 2 Diabetes after an oral glucose tolerance test &#x02013; a randomised controlled trial</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Winterbone</surname><given-names>Mark S</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>mark.winterbone@bbsrc.ac.uk</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Sampson</surname><given-names>Mike J</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>mike.sampson@nnuh.nhs.uk</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Saha</surname><given-names>Shikha</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>shikha.saha@bbsrc.ac.uk</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Hughes</surname><given-names>Jackie C</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jackie.brown@bbsrc.ac.uk</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Hughes</surname><given-names>David A</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>davida.hughes@bbsrc.ac.uk</email></contrib></contrib-group><aff id="I1"><label>1</label>Institute of Food Research, Norwich Research Park, Norwich, NR4 7UA, UK</aff><aff id="I2"><label>2</label>Bertram Diabetes Research Unit, Norfolk &#x00026; Norwich University Hospital NHS Trust, Norwich, NR4 7UY, UK</aff><pub-date pub-type="collection"><year>2007</year></pub-date><pub-date pub-type="epub"><day>22</day><month>2</month><year>2007</year></pub-date><volume>6</volume><fpage>8</fpage><lpage>8</lpage><ext-link ext-link-type="uri" xlink:href="http://www.cardiab.com/content/6/1/8"/><history><date date-type="received"><day>19</day><month>2</month><year>2007</year></date><date date-type="accepted"><day>22</day><month>2</month><year>2007</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2007 Winterbone et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2007</copyright-year><copyright-holder>Winterbone et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
