<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Diabetes Investig</journal-id><journal-id journal-id-type="iso-abbrev">J Diabetes Investig</journal-id><journal-id journal-id-type="doi">10.1111/(ISSN)2040-1124</journal-id><journal-id journal-id-type="publisher-id">JDI</journal-id><journal-title-group><journal-title>Journal of Diabetes Investigation</journal-title></journal-title-group><issn pub-type="ppub">2040-1116</issn><issn pub-type="epub">2040-1124</issn><publisher><publisher-name>Wiley-Blackwell</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">24843685</article-id><article-id pub-id-type="pmc">4020235</article-id><article-id pub-id-type="doi">10.1111/jdi.12059</article-id><article-id pub-id-type="publisher-id">JDI12059</article-id><article-categories><subj-group subj-group-type="overline"><subject>Original Article</subject></subj-group><subj-group subj-group-type="heading"><subject>Articles</subject><subj-group subj-group-type="heading"><subject>Clinical Science and Care</subject></subj-group></subj-group></article-categories><title-group><article-title>Efficacy of &#x003b1;&#x02010;glucosidase inhibitors combined with dipeptidyl&#x02010;peptidase&#x02010;4 inhibitor (alogliptin) for glucose fluctuation in patients with type&#x000a0;2 diabetes mellitus by continuous glucose monitoring</article-title><alt-title alt-title-type="left-running-head">Kurozumi <italic>et&#x000a0;al</italic>.</alt-title></title-group><contrib-group><contrib id="jdi12059-cr-0001" contrib-type="author"><name><surname>Kurozumi</surname><given-names>Akira</given-names></name><xref ref-type="aff" rid="jdi12059-aff-0001"><sup>1</sup></xref></contrib><contrib id="jdi12059-cr-0002" contrib-type="author"><name><surname>Okada</surname><given-names>Yosuke</given-names></name><xref ref-type="aff" rid="jdi12059-aff-0001"><sup>1</sup></xref></contrib><contrib id="jdi12059-cr-0003" contrib-type="author"><name><surname>Mori</surname><given-names>Hiroko</given-names></name><xref ref-type="aff" rid="jdi12059-aff-0001"><sup>1</sup></xref></contrib><contrib id="jdi12059-cr-0004" contrib-type="author"><name><surname>Arao</surname><given-names>Tadashi</given-names></name><xref ref-type="aff" rid="jdi12059-aff-0001"><sup>1</sup></xref></contrib><contrib id="jdi12059-cr-0005" contrib-type="author"><name><surname>Tanaka</surname><given-names>Yoshiya</given-names></name><xref ref-type="correspondence-address" rid="correspondenceTo">*</xref><xref ref-type="aff" rid="jdi12059-aff-0001"><sup>1</sup></xref></contrib></contrib-group><aff id="jdi12059-aff-0001"><label><sup>1</sup></label><named-content content-type="organisation-division">First Department of Internal Medicine</named-content><institution>School of Medicine, University of Occupational and Environmental Health Japan</institution><named-content content-type="city">Kitakyushu</named-content><country country="JP">Japan</country></aff><author-notes><corresp id="correspondenceTo"><label>*</label>
