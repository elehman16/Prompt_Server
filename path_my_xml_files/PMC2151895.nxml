<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Crit Care</journal-id><journal-title>Critical Care</journal-title><issn pub-type="ppub">1364-8535</issn><issn pub-type="epub">1466-609X</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17316456</article-id><article-id pub-id-type="pmc">2151895</article-id><article-id pub-id-type="publisher-id">cc5709</article-id><article-id pub-id-type="doi">10.1186/cc5709</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Meperidine and skin surface warming additively reduce the shivering threshold: a volunteer study</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Kimberger</surname><given-names>Oliver</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>study@kimberger.at</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Ali</surname><given-names>Syed Z</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>syedalimd@hotmail.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Markstaller</surname><given-names>Monica</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>markstaller@gmx.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Zmoos</surname><given-names>Sandra</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>stefan.henle@insel.ch</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Lauber</surname><given-names>Rolf</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>rolf.lauber@dkf.unibe.ch</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Hunkeler</surname><given-names>Corinne</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>corinnehunkeler@hotmail.com</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Kurz</surname><given-names>Andrea</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>andrea.kurz@insel.ch</email></contrib></contrib-group><aff id="I1"><label>1</label>Department of Anaesthesiology, University of Bern, CH-3010 Bern, Switzerland</aff><aff id="I2"><label>2</label>Outcomes Research Institute, University of Louisville, 2301 S 3RD St, Louisville, KY 40292-2001, USA</aff><pub-date pub-type="ppub"><year>2007</year></pub-date><pub-date pub-type="epub"><day>23</day><month>2</month><year>2007</year></pub-date><volume>11</volume><issue>1</issue><fpage>R29</fpage><lpage>R29</lpage><ext-link ext-link-type="uri" xlink:href="http://ccforum.com/content/11/1/R29"/><history><date date-type="received"><day>6</day><month>12</month><year>2006</year></date><date date-type="rev-request"><day>30</day><month>1</month><year>2007</year></date><date date-type="rev-recd"><day>12</day><month>2</month><year>2007</year></date><date date-type="accepted"><day>23</day><month>2</month><year>2007</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2007 Kimberger et al.; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2007</copyright-year><copyright-holder>Kimberger et al.; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an open access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
