><subj-group subj-group-type="Discipline-v3"><subject>Biology and Life Sciences</subject><subj-group><subject>Physiology</subject><subj-group><subject>Biological Locomotion</subject><subj-group><subject>Running</subject></subj-group></subj-group></subj-group></subj-group><subj-group subj-group-type="Discipline-v3"><subject>Medicine and Health Sciences</subject><subj-group><subject>Physiology</subject><subj-group><subject>Biological Locomotion</subject><subj-group><subject>Running</subject></subj-group></subj-group></subj-group></subj-group></article-categories><title-group><article-title>Combined sprint and resistance training abrogates age differences in somatotropic hormones</article-title><alt-title alt-title-type="running-head">Training and aging effect on somatotropic hormones</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Sellami</surname><given-names>Maha</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Dhahbi</surname><given-names>Wissem</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Hayes</surname><given-names>Lawrence D.</given-names></name><xref ref-type="aff" rid="aff002"><sup>2</sup></xref></contrib><contrib contrib-type="author"><contrib-id authenticated="true" contrib-id-type="orcid">http://orcid.org/0000-0002-4254-3105</contrib-id><name><surname>Padulo</surname><given-names>Johnny</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref><xref ref-type="aff" rid="aff003"><sup>3</sup></xref><xref ref-type="aff" rid="aff004"><sup>4</sup></xref><xref ref-type="corresp" rid="cor001">*</xref></contrib><contrib contrib-type="author"><name><surname>Rhibi</surname><given-names>Fatma</given-names></name><xref ref-type="aff" rid="aff005"><sup>5</sup></xref></contrib><contrib contrib-type="author"><name><surname>Djemail</surname><given-names>Hanen</