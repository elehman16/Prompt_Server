<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Malar J</journal-id><journal-title>Malaria Journal</journal-title><issn pub-type="epub">1475-2875</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19602253</article-id><article-id pub-id-type="pmc">2720981</article-id><article-id pub-id-type="publisher-id">1475-2875-8-157</article-id><article-id pub-id-type="doi">10.1186/1475-2875-8-157</article-id><article-categories><subj-group subj-group-type="heading"><subject>Methodology</subject></subj-group></article-categories><title-group><article-title>A new tent trap for sampling exophagic and endophagic members of the <italic>Anopheles gambiae </italic>complex</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Govella</surname><given-names>Nicodemus J</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><xref ref-type="aff" rid="I4">4</xref><email>govella@ihi.or.tz</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Chaki</surname><given-names>Prosper P</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><xref ref-type="aff" rid="I4">4</xref><email>pchaki@ihi.or.tz</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Geissbuhler</surname><given-names>Yvonne</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I5">5</xref><email>yvonne99@hotmail.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Kannady</surname><given-names>Khadija</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>kadkhadija@yahoo.co.uk</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Okumu</surname><given-names>Fredros</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>fredros@ihi.or.tz</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Charlwood</surname><given-names>J Derek</given-names></name><xref ref-type="aff" rid="I6">6</xref><email>jdcharlwood@gmail.com</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Anderson</surname><given-names>Robert A</given-names></name><xref ref-type="aff" rid="I7">7</xref><email>r.anderson@uwinnipeg.ca</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Killeen</surname><given-names>Gerry F</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><xref ref-type="aff" rid="I4">4</xref><xref ref-type="aff" rid="I5">5</xref><email>gkilleen@ihi.or.tz</email></contrib></contrib-group><aff id="I1"><label>1</label>Ifakara Health Institute, Coordination Office, PO Box 78373, Kiko Avenue, Mikocheni, Dar es Salaam, United Republic of Tanzania</aff><aff id="I2"><label>2</label>Dar es Salaam City Council, Ministry of Regional Administration and Local Government, United Republic of Tanzania</aff><aff id="I3"><label>3</label>Durham University, School of Biological and Biomedical Sciences, South Road, Durham, DH13LE, UK</aff><aff id="I4"><label>4</label>Liverpool School of Tropical Medicine, Pembroke place, Liverpool, L3QA, UK</aff><aff id="I5"><label>5</label>Swiss Tropical Institute, Department of Public Health and Epidemiology, PO Box 4002, Basel, Switzerland</aff><aff id="I6"><label>6</label>Danish Bilharziaisis Laboratory, 1-D Jaegersborg All&#x000e9;, Charlottenlund, DK 2920, Denmark</aff><aff id="I7"><label>7</label>Biology Department, University of Winnipeg, Winnipeg, Manitoba, R3B 2E9, Canada</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>14</day><month>7</month><year>2009</year></pub-date><volume>8</volume><fpage>157</fpage><lpage>157</lpage><ext-link ext-link-type="uri" xlink:href="http://www.malariajournal.com/content/8/1/157"/><history><date date-type="received"><day>16</day><month>10</month><year>2008</year></date><date date-type="accepted"><day>14</day><month>7</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Govella et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Govella et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
