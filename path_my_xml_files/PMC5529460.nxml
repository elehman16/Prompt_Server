tes" rid="fn1-medscimonit-23-3489">A</xref><xref ref-type="author-notes" rid="fn2-medscimonit-23-3489">B</xref><xref ref-type="author-notes" rid="fn4-medscimonit-23-3489">D</xref><xref ref-type="author-notes" rid="fn5-medscimonit-23-3489">E</xref></contrib><contrib contrib-type="author"><name><surname>Martin</surname><given-names>Amber M.</given-names></name><xref ref-type="aff" rid="af1-medscimonit-23-3489">1</xref><xref ref-type="author-notes" rid="fn2-medscimonit-23-3489">B</xref></contrib><contrib contrib-type="author"><name><surname>Banks</surname><given-names>Christi</given-names></name><xref ref-type="aff" rid="af2-medscimonit-23-3489">2</xref><xref ref-type="author-notes" rid="fn1-medscimonit-23-3489">A</xref><xref ref-type="author-notes" rid="fn2-medscimonit-23-3489">B</xref><xref ref-type="author-notes" rid="fn5-medscimonit-23-3489">E</xref></contrib><contrib contrib-type="author"><name><surname>Ball</surname><given-names>Angel</given-names></name><xref ref-type="aff" rid="af3-medscimonit-23-3489">3</xref><xref ref-type="author-notes" rid="fn1-medscimonit-23-3489">A</xref><xref ref-type="author-notes" rid="fn2-medscimonit-23-3489">B</xref><xref ref-type="author-notes" rid="fn5-medscimonit-23-3489">E</xref></contrib><contrib contrib-type="author"><name><surname>Vannest</surname><given-names>Jennifer</given-names></name><xref ref-type="aff" rid="af2-medscimonit-23-3489">2</xref><xref ref-type="author-notes" rid="fn1-medscimonit-23-3489">A</xref><xref ref-type="author-notes" rid="fn2-medscimonit-23-3489">B</xref><xref ref-type="author-notes" rid="fn5-medscimonit-23-3489">E</xref></contrib><contrib contrib-type="author"><name><surname>Dietz</surname><given-names>Aimee R.</given-names></name><xref ref-type="aff" rid="af3-medscimonit-23-3489">3</xref><xref ref-type="author-notes" rid="fn1-medscimonit-23-3489">A</xref><xref ref-type="author-notes" rid="fn2-medscimonit-23-3489">B</xref><xref ref-type="author-notes" rid="fn5-medscimonit-23-3489">E</xref></contrib><contrib contrib-type="author"><name><surname>Szaflarski</surname><given-names>Jerzy P.</given-names></name><xref ref-type="aff" rid="af1-medscimonit-23-3489">1</xref><xref ref-type="aff" rid="af4-medscimonit-23-3489">4</xref><xref ref-type="aff" rid="af5-medscimonit-23-3489">5</xref><xref ref-type="author-notes" rid="fn1-medscimonit-23-3489">A</xref><xref ref-type="author-notes" rid="fn2-medscimonit-23-3489">B</xref><xref ref-type="author-notes" rid="fn4-medscimonit-23-3489">D</xref><xref ref-type="author-notes" rid="fn5-medscimonit-23-3489">E</xref><xref ref-type="author-notes" rid="fn6-medscimonit-23-3489">F</xref><xref ref-type="author-notes" rid="fn7-medscimonit-23-3489">G</xref></contrib></contrib-group><aff id="af1-medscimonit-23-3489">
