<article xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ</journal-id><journal-id journal-id-type="iso-abbrev">BMJ</journal-id><journal-id journal-id-type="publisher-id">bmj</journal-id><journal-title-group><journal-title>The BMJ</journal-title></journal-title-group><issn pub-type="ppub">0959-8138</issn><issn pub-type="epub">1756-1833</issn><publisher><publisher-name>BMJ Publishing Group Ltd.</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27098105</article-id><article-id pub-id-type="pmc">4849174</article-id><article-id pub-id-type="publisher-id">kotd030418</article-id><article-id pub-id-type="doi">10.1136/bmj.i1855</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Effect of age and sex on efficacy and tolerability of &#x003b2; blockers in patients with heart failure with reduced ejection fraction: individual patient data meta-analysis</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Kotecha</surname><given-names>Dipak</given-names></name><role>clinician scientist in cardiovascular medicine</role><xref ref-type="aff" rid="aff1">1 </xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Manzano</surname><given-names>Luis</given-names></name><role>professor of internal medicine</role><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Krum</surname><given-names>Henry</given-names></name><role>professor of medicine</role><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Rosano</surname><given-names>Giuseppe</given-names></name><