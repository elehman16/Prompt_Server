roup></subj-group></article-categories><title-group><article-title>Comparative Effects of Direct Renin Inhibitor and Angiotensin Receptor Blocker on Albuminuria in Hypertensive Patients with Type 2 Diabetes. A Randomized Controlled Trial</article-title><alt-title alt-title-type="running-head">Comparison of ARB and DRI on Microalbuminuria</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Uzu</surname><given-names>Takashi</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref><xref ref-type="aff" rid="aff002"><sup>2</sup></xref><xref ref-type="corresp" rid="cor001">*</xref></contrib><contrib contrib-type="author"><name><surname>Araki</surname><given-names>Shin-ichi</given-names></name><xref ref-type="aff" rid="aff002"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>Kashiwagi</surname><given-names>Atsunori</given-names></name><xref ref-type="aff" rid="aff003"><sup>3</sup></xref></contrib><contrib contrib-type="author"><name><surname>Haneda</surname><given-names>Masakazu</given-names></name><xref ref-type="aff" rid="aff004"><sup>4</sup></xref></contrib><contrib contrib-type="author"><name><surname>Koya</surname><given-names>Daisuke</given-names></name><xref ref-type="aff" rid="aff005"><sup>5</sup></xref></contrib><contrib contrib-type="author"><name><surname>Yokoyama</surname><given-names>Hiroki</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib><contrib contrib-type="author"><name><surname>Kida</surname><given-names>Yasuo</given-names></name><xref ref-type="aff" rid="aff007"><sup>7</sup></xref></contrib><contrib contrib-type="author"><name><surname>Ikebuchi</surname><given-names>Motoyoshi</given-names></name><xref ref-type="aff" rid="aff008"><sup>8</sup></xref></contrib><contrib contrib-type="author"><name><surname>Nakamura</surname><given-names>Takaaki</given-names></name><xref ref-type="aff" rid="aff009"><sup>9</sup></xref></contrib><contrib contrib-type="