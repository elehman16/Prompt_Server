contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Phillips II</surname><given-names>Gregory</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-8396-1170</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>DuBois</surname><given-names>L. Zachary</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-5459-2863</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Bull</surname><given-names>Sheana S</given-names></name><degrees>MPH, PhD</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-8688-326X</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Mustanski</surname><given-names>Brian</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9222-5116</contrib-id></contrib><contrib id="contrib6" contrib-type="author" corresp="yes"><name><surname>Ybarra</surname><given-names>Michele L</given-names></name><degrees>MPH, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>Center for Innovative Public Health Research</institution><addr-line>555 N. El Camino Real #A347</addr-line><addr-line>San Clemente, CA, 92672</addr-line><country>United States</country><phone>1 877 302 6858 ext 801</phone><fax>1 877 302 6858</fax><email>michele@innovativepublichealth.org</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9127-7440</contrib-id></contrib></contrib-group><aff id="aff1"><sup>1</sup><institution>Center for Innovative Public Health Research</institution><addr-line>San Clemente, CA</addr-line><country>United States</country></aff><aff id="aff2">