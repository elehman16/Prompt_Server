article-categories><title-group><article-title>Pilot, double-blind, randomized, placebo-controlled clinical trial of the supplement food Nyaditum resae<sup>&#x000ae;</sup> in adults with or without latent TB infection: Safety and immunogenicity</article-title><alt-title alt-title-type="running-head">Safety and immunogenicity of the supplement food Nyaditum resae<sup>&#x000ae;</sup></alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Montan&#x000e9;</surname><given-names>Eva</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref><xref ref-type="aff" rid="aff002"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>Barriocanal</surname><given-names>Ana Maria</given-names></name><xref ref-type="aff" rid="aff002"><sup>2</sup></xref><xref ref-type="aff" rid="aff003"><sup>3</sup></xref></contrib><contrib contrib-type="author"><name><surname>Arellano</surname><given-names>Ana Luc&#x000ed;a</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref><xref ref-type="aff" rid="aff002"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>Valderrama</surname><given-names>Angelica</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Sanz</surname><given-names>Yolanda</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Perez-Alvarez</surname><given-names>Nuria</given-names></name><xref ref-type="aff" rid="aff004"><sup>4</sup></xref><xref ref-type="aff" rid="aff005"><sup>5</sup></xref></contrib><contrib contrib-type="author"><name><surname>Cardona</surname><given-names>Paula</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib><contrib contrib-type="author"><name><surname>Vilaplana</surname><given-names>Cristina</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib>