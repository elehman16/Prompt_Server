<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28674122</article-id><article-id pub-id-type="pmc">5734200</article-id><article-id pub-id-type="publisher-id">bmjopen-2016-012221</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2016-012221</article-id><article-categories><subj-group subj-group-type="heading"><subject>Diabetes and Endocrinology</subject><subj-group><subject>Research</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1843</subject></subj-group></article-categories><title-group><article-title>Effectiveness of a targeted lifestyle intervention in primary care on diet and physical activity among South Asians at risk for diabetes: 2-year results of a randomised controlled trial in the Netherlands</article-title></title-group><contrib-group><contrib id="author-48743612" contrib-type="author"><name><surname>Vlaar</surname><given-names>Everlina M A</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-45694215" contrib-type="author"><name><surname>Nierkens</surname><given-names>Vera</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-35992263" contrib-type="author"><name><surname>Nicolaou</surname><given-names>Mary</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-45694129" contrib-type="author"><name><surname>Middelkoop</surname><given-names>Barend J C</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-48743589" contrib-type="author"><name><surname>Busschers</surname><given-names>Wim B</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-23539686" contrib-type="author"><name><surname>Stronks</surname><given-names>Karien</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-48743234" contrib-type="author"><name><surname>van Valkengoed</surname><given-names>Irene G M</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib></contrib-group><aff id="aff1">
