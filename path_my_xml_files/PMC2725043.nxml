<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Trials</journal-id><journal-title>Trials</journal-title><issn pub-type="epub">1745-6215</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19638233</article-id><article-id pub-id-type="pmc">2725043</article-id><article-id pub-id-type="publisher-id">1745-6215-10-61</article-id><article-id pub-id-type="doi">10.1186/1745-6215-10-61</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Ethical and policy issues in cluster randomized trials: rationale and design of a mixed methods research study</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Taljaard</surname><given-names>Monica</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>mtaljaard@ohri.ca</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Weijer</surname><given-names>Charles</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>cweijer@uwo.ca</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Grimshaw</surname><given-names>Jeremy M</given-names></name><xref ref-type="aff" rid="I4">4</xref><xref ref-type="aff" rid="I5">5</xref><email>jgrimshaw@ohri.ca</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Belle Brown</surname><given-names>Judith</given-names></name><xref ref-type="aff" rid="I6">6</xref><email>jbbrown@uwo.ca</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Binik</surname><given-names>Ariella</given-names></name><xref ref-type="aff" rid="I7">7</xref><email>abinik@uwo.ca</email></contrib><contrib id="A6" contrib-type="author"><name><surname>