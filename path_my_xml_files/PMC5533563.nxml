<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Pain Res</journal-id><journal-id journal-id-type="iso-abbrev">J Pain Res</journal-id><journal-id journal-id-type="publisher-id">Journal of Pain Research</journal-id><journal-title-group><journal-title>Journal of Pain Research</journal-title></journal-title-group><issn pub-type="epub">1178-7090</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28769588</article-id><article-id pub-id-type="pmc">5533563</article-id><article-id pub-id-type="doi">10.2147/JPR.S138297</article-id><article-id pub-id-type="publisher-id">jpr-10-1723</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Duloxetine 60 mg for chronic low back pain: post hoc responder analysis of double-blind, placebo-controlled trials</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Alev</surname><given-names>Levent</given-names></name><xref ref-type="aff" rid="af1-jpr-10-1723">1</xref></contrib><contrib contrib-type="author"><name><surname>Fujikoshi</surname><given-names>Shinji</given-names></name><xref ref-type="aff" rid="af2-jpr-10-1723">2</xref></contrib><contrib contrib-type="author"><name><surname>Yoshikawa</surname><given-names>Aki</given-names></name><xref ref-type="aff" rid="af3-jpr-10-1723">3</xref></contrib><contrib contrib-type="author"><name><surname>Enomoto</surname><given-names>Hiroyuki</given-names></name><xref ref-type="aff" rid="af1-jpr-10-1723">1</xref><xref ref-type="corresp" rid="c1-jpr-10-1723"/></contrib><contrib contrib-type="author"><name><surname>Ishida</surname><given-names>Mitsuhiro</given-names></name><xref ref-type="aff" rid="af4-jpr-10-1723">4</xref></contrib><contrib contrib-type="author"><name><surname>Tsuji</surname><given-names>Toshinaga</given-names></name><xref ref-type="aff" rid="af5-jpr-10-1723">5</xref></contrib><contrib contrib-type="author"><name><surname>Ogawa</surname><given-names>Kei</given-names></name><xref ref-type="aff" rid="af1-jpr-10-1723">1</xref></contrib><contrib contrib-type="author"><name><surname>Konno</surname><given-names>Shinichi</given-names></name><xref ref-type="aff" rid="af6-jpr-10-1723">6</xref></contrib></contrib-group><aff id="af1-jpr-10-1723">
