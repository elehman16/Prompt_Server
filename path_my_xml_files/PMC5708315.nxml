<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Open Heart</journal-id><journal-id journal-id-type="iso-abbrev">Open Heart</journal-id><journal-id journal-id-type="hwp">openhrt</journal-id><journal-id journal-id-type="publisher-id">openheart</journal-id><journal-title-group><journal-title>Open Heart</journal-title></journal-title-group><issn pub-type="epub">2053-3624</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29225903</article-id><article-id pub-id-type="pmc">5708315</article-id><article-id pub-id-type="publisher-id">openhrt-2017-000677</article-id><article-id pub-id-type="doi">10.1136/openhrt-2017-000677</article-id><article-categories><subj-group subj-group-type="heading"><subject>Interventional Cardiology</subject></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group><series-title>Original research article</series-title></article-categories><title-group><article-title>Early clinical outcomes as a function of use of newer oral P2Y<sub>12</sub> inhibitors versus clopidogrel in the EUROMAX trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Huber</surname><given-names>Kurt</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-56156571" contrib-type="author"><name><surname>Ducrocq</surname><given-names>Gregory</given-names></name><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Hamm</surname><given-names>Christian W</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><c