<article xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Res Protoc</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Res Protoc</journal-id><journal-id journal-id-type="publisher-id">ResProt</journal-id><journal-title-group><journal-title>JMIR Research Protocols</journal-title></journal-title-group><issn pub-type="epub">1929-0748</issn><publisher><publisher-name>JMIR Publications Inc.</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">26955763</article-id><article-id pub-id-type="pmc">4804104</article-id><article-id pub-id-type="publisher-id">v5i1e37</article-id><article-id pub-id-type="doi">10.2196/resprot.5261</article-id><article-categories><subj-group subj-group-type="heading"><subject>Protocol</subject></subj-group><subj-group subj-group-type="article-type"><subject>Protocol</subject></subj-group></article-categories><title-group><article-title>Primary Care Pathway for Childhood Asthma: Protocol for a Randomized Cluster-Controlled Trial</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Rotter</surname><given-names>Thomas</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Plaisance</surname><given-names>Ariane</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Cave</surname><given-names>Andrew J</given-names></name><degrees>MBChB, MCISc, FRCGP, FCFP</degrees><xref ref-type="aff" rid="aff01">1</xref><address><institution>Department of Family Medicine</institution><institution>University of Alberta</institution><addr-line>6-10 University Terrace</addr-line><addr-line>Edmonton, AB, T6G 2T4</addr-line><country>Canada</country><phone>1 780 492 8102</phone><fax>1 780 492 2593</fax><email>andrew.cave@ualberta.ca</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-8148-4454</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Sharpe</surname><given-names>Heather</given-names></name><degrees>RN, PhD</degrees><xref ref-type="aff" rid="aff01">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5223-5085</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Anselmo</surname><given-names>Mark</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff02">2</xref><xref ref-type="aff" rid="aff03">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3912-9242</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Befus</surname><given-names>A Dean</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff04">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-1611-2897</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Currie</surname><given-names>Gillian</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff02">2</xref><xref ref-type="aff" rid="aff03">3</xref><xref ref-type="aff" rid="aff05">5</xref><xref ref-type="aff" rid="aff06">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4681-9893</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Davey</surname><given-names>Christina</given-names></name><degrees>MA</degrees><xref ref-type="aff" rid="aff01">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6508-7296</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Drummond</surname><given-names>Neil</given-names>