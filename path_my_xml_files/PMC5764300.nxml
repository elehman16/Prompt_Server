></contrib><contrib contrib-type="author"><name><surname>Vahdati-Bolouri</surname><given-names>Mitra</given-names></name><xref ref-type="aff" rid="af4-copd-13-203">4</xref></contrib><contrib contrib-type="author"><name><surname>Locantore</surname><given-names>Nicholas</given-names></name><xref ref-type="aff" rid="af5-copd-13-203">5</xref></contrib><contrib contrib-type="author"><name><surname>Riley</surname><given-names>John H</given-names></name><xref ref-type="aff" rid="af4-copd-13-203">4</xref></contrib></contrib-group><aff id="af1-copd-13-203">
