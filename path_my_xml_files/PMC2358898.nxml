<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Psychiatry</journal-id><journal-title>BMC Psychiatry</journal-title><issn pub-type="epub">1471-244X</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18412944</article-id><article-id pub-id-type="pmc">2358898</article-id><article-id pub-id-type="publisher-id">1471-244X-8-23</article-id><article-id pub-id-type="doi">10.1186/1471-244X-8-23</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Design of a placebo-controlled, randomized study of the efficacy of repetitive transcranial magnetic stimulation for the treatment of chronic tinnitus</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Landgrebe</surname><given-names>Michael</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>michael.landgrebe@medbo.de</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Binder</surname><given-names>Harald</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>binderh@fdm.uni-freiburg.de</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Koller</surname><given-names>Michael</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>michael.koller@klinik.uni-regensburg.de</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Eberl</surname><given-names>Yvonne</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>Yvonne1.Eberl@klinik.uni-regensburg.de</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Kleinjung</surname><given-names>Tobias</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>tobias.kleinjung@klinik.uni-regensburg.de</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Eichhammer</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>peter.eichhammer@medbo.de</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Graf</surname><given-names>Erika</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>erika.graf@uniklinik-freiburg.de</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Hajak</surname><given-names>Goeran</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>goeran.hajak@medbo.de</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Langguth</surname><given-names>Berthold</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>berthold.langguth@medbo.de</email></contrib></contrib-group><aff id="I1"><label>1</label>Department of Psychiatry, Psychosomatics, and Psychotherapy, University of Regensburg, Germany</aff><aff id="I2"><label>2</label>Center for Clinical Studies, University Hospital of Regensburg, Germany</aff><aff id="I3"><label>3</label>Department of Otorhinolarnygology, University Hospital of Regensburg, Germany</aff><aff id="I4"><label>4</label>Institute of Medical Biometry and Medical Informatics, University Medical Center Freiburg, Germany</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>15</day><month>4</month><year>2008</year></pub-date><volume>8</volume><fpage>23</fpage><lpage>23</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1471-244X/8/23"/><history><date date-type="received"><day>11</day><month>3</month><year>2008</year></date><date date-type="accepted"><day>15</day><month>4</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Landgrebe et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Landgrebe et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
