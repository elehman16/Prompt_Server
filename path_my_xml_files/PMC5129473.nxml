<contrib id="art39684-cr-0019" contrib-type="author"><name><surname>Gaffo</surname><given-names>A.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0020" contrib-type="author"><name><surname>Gottschlich</surname><given-names>G.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0021" contrib-type="author"><name><surname>Greenwald</surname><given-names>J.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0022" contrib-type="author"><name><surname>Griffin</surname><given-names>C.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0023" contrib-type="author"><name><surname>Guice</surname><given-names>M.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0024" contrib-type="author"><name><surname>Harper</surname><given-names>W.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0025" contrib-type="author"><name><surname>Hill</surname><given-names>J.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0026" contrib-type="author"><name><surname>Huffman</surname><given-names>C.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0027" contrib-type="author"><name><surname>Huling</surname><given-names>R.</given-names><suffix>Jr.</suffix></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0028" contrib-type="author"><name><surname>Johnson</surname><given-names>F.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0029" contrib-type="author"><name><surname>Kafka</surname><given-names>S.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0030" contrib-type="author"><name><surname>Keane</surname><given-names>J.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0031" contrib-type="author"><name><surname>Kirby</surname><given-names>W.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0032" contrib-type="author"><name><surname>Kirstein</surname><given-names>J.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0033" contrib-type="author"><name><surname>Kolettis</surname><given-names>E.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0034" contrib-type="author"><name><surname>Lefebvre</surname><given-names>G.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0035" contrib-type="author"><name><surname>Mabaquiao</surname><given-names>A.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0036" contrib-type="author"><name><surname>Maynard</surname><given-names>K.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0037" contrib-type="author"><name><surname>McIlwain</surname><given-names>H.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0038" contrib-type="author"><name><surname>McNeill</surname><given-names>R.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0039" contrib-type="author"><name><surname>Mehta</surname><given-names>D.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-cr-0040" contrib-type="author"><name><surname>Montgomery</surname><given-names>R.</given-names></name><xref ref-type="contrib" rid="art39684-cr-0008"/></contrib><contrib id="art39684-