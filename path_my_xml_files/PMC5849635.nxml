<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Transl Stroke Res</journal-id><journal-id journal-id-type="iso-abbrev">Transl Stroke Res</journal-id><journal-title-group><journal-title>Translational Stroke Research</journal-title></journal-title-group><issn pub-type="ppub">1868-4483</issn><issn pub-type="epub">1868-601X</issn><publisher><publisher-name>Springer US</publisher-name><publisher-loc>New York</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28770403</article-id><article-id pub-id-type="pmc">5849635</article-id><article-id pub-id-type="publisher-id">548</article-id><article-id pub-id-type="doi">10.1007/s12975-017-0548-0</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title>Route of Feeding as a Proxy for Dysphagia After Stroke and the Effect of Transdermal Glyceryl Trinitrate: Data from the Efficacy of Nitric Oxide in Stroke Randomised Controlled Trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Woodhouse</surname><given-names>Lisa J</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Scutt</surname><given-names>Polly</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Hamdy</surname><given-names>Shaheen</given-names></name><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Smithard</surname><given-names>David G</given-names></name><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Cohen</surname><given-names>David L</given-names></name><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Roffe</surname><given-names>Christine</given-names></name><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Bereczki</surname><given-names>Daniel</given-names></name><xref ref-type="aff" rid="Aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Berge</surname><given-names>Eivind</given-names></name><xref ref-type="aff" rid="Aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Bladin</surname><given-names>Christopher F</given-names></name><xref ref-type="aff" rid="Aff8">8</xref></contrib><contrib contrib-type="author"><name><surname>Caso</surname><given-names>Valeria</given-names></name><xref ref-type="aff" rid="Aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Christensen</surname><given-names>Hanne K</given-names></name><xref ref-type="aff" rid="Aff10">10</xref></contrib><contrib contrib-type="author"><name><surname>Collins</surname><given-names>R&#x000f3;n&#x000e1;n</given-names></name><xref ref-type="aff" rid="Aff11">11</xref></contrib><contrib contrib-type="author"><name><surname>Czlonkowska</surname><given-names>Anna</given-names></name><xref ref-type="aff" rid="Aff12">12</xref></contrib><contrib contrib-type="author"><name><surname>de Silva</surname><given-names>Asita</given-names></name><xref ref-type="aff" rid="Aff13">13</xref></contrib><contrib contrib-type="author"><name><surname>Etribi</surname><given-names>Anwar</given-names></name><xref ref-type="aff" rid="Aff14">14</xref></contrib><contrib contrib-type="author"><name><surname>Laska</surname><given-names>Ann-Charlotte</given-names></name><xref ref-type="aff" rid="Aff15">15</xref></contrib><contrib contrib-type="author"><name><surname>Ntaios</surname><given-names>George</given-names></name><xref ref-type="aff" rid="Aff16">16</xref></contrib><contrib contrib-type="author"><name><surname>Ozturk</surname><given-names>Serefnur</given-names></name><xref ref-type="a