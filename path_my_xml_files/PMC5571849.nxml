contrib-type="author"><name><surname>Weir</surname><given-names>Matthew R</given-names></name><xref ref-type="aff" rid="af5-ijnrd-10-233">5</xref></contrib><contrib contrib-type="author"><name><surname>Bunnapradist</surname><given-names>Suphamai</given-names></name><xref ref-type="aff" rid="af6-ijnrd-10-233">6</xref></contrib><contrib contrib-type="author"><name><surname>Finn</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="af1-ijnrd-10-233">1</xref></contrib><contrib contrib-type="author"><name><surname>Liu</surname><given-names>Jiankang</given-names></name><xref ref-type="aff" rid="af1-ijnrd-10-233">1</xref></contrib><contrib contrib-type="author"><name><surname>Claggett</surname><given-names>Brian</given-names></name><xref ref-type="aff" rid="af1-ijnrd-10-233">1</xref></contrib><contrib contrib-type="author"><name><surname>Monaco</surname><given-names>Anthony P</given-names></name><xref ref-type="aff" rid="af3-ijnrd-10-233">3</xref><xref ref-type="aff" rid="af4-ijnrd-10-233">4</xref></contrib></contrib-group><aff id="af1-ijnrd-10-233">
