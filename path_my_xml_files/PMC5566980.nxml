author"><name><surname>Landew&#x000e9;</surname><given-names>Robert</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Machado</surname><given-names>Pedro M</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Maksymowych</surname><given-names>Walter P</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Davies</surname><given-names>Owen</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>Hoepken</surname><given-names>Bengt</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>Nurminen</surname><given-names>Tommi</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>Stach</surname><given-names>Christian</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>van der Heijde</surname><given-names>D&#x000e9;sir&#x000e9;e</given-names></name><xref ref-type="aff" rid="af8">8</xref></contrib></contrib-group><aff id="af1">
