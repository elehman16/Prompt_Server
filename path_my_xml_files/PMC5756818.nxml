></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0001-8127-7738</contrib-id><name><surname>Choi</surname><given-names>Don Kyoung</given-names></name><xref ref-type="aff" rid="af3-inj-1734956-478"><sup>3</sup></xref></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-8933-6656</contrib-id><name><surname>Kwon</surname><given-names>Ohseong</given-names></name><xref ref-type="aff" rid="af3-inj-1734956-478"><sup>3</sup></xref></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-3419-8428</contrib-id><name><surname>Na</surname><given-names>Hae Ri</given-names></name><xref ref-type="aff" rid="af4-inj-1734956-478"><sup>4</sup></xref></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-4691-6159</contrib-id><name><surname>Cho</surname><given-names>Sung Tae</given-names></name><xref ref-type="corresp" rid="c1-inj-1734956-478"/><xref ref-type="aff" rid="af3-inj-1734956-478"><sup>3</sup></xref></contrib><aff id="af1-inj-1734956-478">
