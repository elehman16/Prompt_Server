<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Cancer</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Cancer</journal-id><journal-id journal-id-type="publisher-id">JC</journal-id><journal-title-group><journal-title>JMIR cancer</journal-title></journal-title-group><issn pub-type="epub">2369-1999</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28935620</article-id><article-id pub-id-type="pmc">5629346</article-id><article-id pub-id-type="publisher-id">v3i2e13</article-id><article-id pub-id-type="doi">10.2196/cancer.7495</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Paper</subject></subj-group><subj-group subj-group-type="article-type"><subject>Original Paper</subject></subj-group></article-categories><title-group><article-title>A Lifestyle Intervention via Email in Minority Breast Cancer Survivors: Randomized Parallel-Group Feasibility Study</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Dean</surname><given-names>Lorraine</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Valle</surname><given-names>Carmina</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Paxton</surname><given-names>Raheem J</given-names></name><degrees>MSc, PhD</degrees><xref ref-type="aff" rid="aff01">1</xref><address><institution>Department of Community Medicine and Population Health</institution><institution>The University of Alabama</institution><addr-line>1132 Northeast Medical Bldg</addr-line><addr-line>Tuscaloosa, AL, 35401</addr-line><country>United States</country><phone>1 205 348 6857</phone><email>rpaxton@ua.edu</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9430-7051</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Hajek</surname><given-names>Richard</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff02">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6248-5283</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Newcomb</surname><given-names>Patricia</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff03">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5306-8899</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Dobhal</surname><given-names>Megha</given-names></name><degrees>MPH</degrees><xref ref-type="aff" rid="aff04">4</xref><xref ref-type="aff" rid="aff05">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6447-0648</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Borra</surname><given-names>Sujana</given-names></name><degrees>MPH</degrees><xref ref-type="aff" rid="aff04">4</xref><xref ref-type="aff" rid="aff05">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3433-3981</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Taylor</surname><given-names>Wendell C</given-names></name><degrees>MPH, PhD</degrees><xref ref-type="aff" rid="aff06">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6196-3025</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Parra-Medina</surname><given-names>Deborah</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff07">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-