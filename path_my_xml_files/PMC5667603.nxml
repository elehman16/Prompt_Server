<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Exerc Rehabil</journal-id><journal-id journal-id-type="iso-abbrev">J Exerc Rehabil</journal-id><journal-title-group><journal-title>Journal of Exercise Rehabilitation</journal-title></journal-title-group><issn pub-type="ppub">2288-176X</issn><issn pub-type="epub">2288-1778</issn><publisher><publisher-name>Korean Society of Exercise Rehabilitation</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29114531</article-id><article-id pub-id-type="pmc">5667603</article-id><article-id pub-id-type="doi">10.12965/jer.1735010.505</article-id><article-id pub-id-type="publisher-id">jer-13-5-559</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title>Effects of a short-term detraining period on muscle functionality and cognition of strength-trained older women: a preliminary report</article-title></title-group><contrib-group><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">https://orcid.org/0000-0001-7482-9514</contrib-id><name><surname>Coelho</surname><given-names>H&#x000e9;lio Jos&#x000e9;</given-names><suffix>J&#x000fa;nior</suffix></name><xref ref-type="aff" rid="af1-jer-13-5-559">1</xref><xref rid="c1-jer-13-5-559" ref-type="corresp">*</xref></contrib><contrib contrib-type="author"><name><surname>Rodrigues</surname><given-names>Bruno</given-names></name><xref ref-type="aff" rid="af1-jer-13-5-559">1</xref></contrib><contrib contrib-type="author"><name><surname>de Oliveira Gon&#x000e7;alves</surname><given-names>Ivan</given-names></name><xref ref-type="aff" rid="af2-jer-13-5-559">2</xref></contrib><contrib contrib-type="author"><name><surname>Uchida</surname><given-names>Marco Carlos</given-names></name><xref ref-type="aff" rid="af1-jer-13-5-559">1</xref></contrib></contrib-group><aff id="af1-jer-13-5-559">
