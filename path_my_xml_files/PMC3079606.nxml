tr><tr><td align="center">15</td><td align="center">F</td><td align="center">75</td><td align="center">Diabetic nephropathy</td><td align="center">N</td><td align="center">Y</td><td align="center">Y</td><td align="center">Y</td><td align="center">Y</td><td align="center">Y</td><td align="center">48.0</td><td align="center">1.8</td><td align="center">3.5</td><td align="center">1.56</td></tr><tr><td align="center">16</td><td align="center">M</td><td align="center">77</td><td align="center">Proliferative glomerulonephritis</td><td align="center">Y</td><td align="center">N</td><td align="center">Y</td><td align="center">N</td><td align="center">N</td><td align="center">Y</td><td align="center">80.5</td><td align="center">1.8</td><td align="center">3.0</td><td align="center">1.07</td></tr><tr><td align="center">17</td><td align="center">M</td><td align="center">72</td><td align="center">Diabetic nephropathy</td><td align="center">Y</td><td align="center">Y</td><td align="center">N</td><td align="center">N</td><td align="center">N</td><td align="center">Y</td><td align="center">89.5</td><td align="center">1.8</td><td align="center">4.0</td><td align="center">1.37</td></tr><tr><td align="center">18</td><td align="center">F</td><td align="center">65</td><td align="center">Diabetic nephropathy</td><td align="center">Y</td><td align="center">Y</td><td align="center">Y</td><td align="center">Y</td><td align="center">Y</td><td align="center">Y</td><td align="center">48.0</td><td align="center">1.8</td><td align="center">3.5</td><td align="center">2.08</td></tr><tr><td align="center">19</td><td align="center">F</td><td align="center">56</td><td align="center">Nephroangiosclerosis</td><td align="center">N</td><td align="center">N</td><td align="center">Y</td><td align="center">Y</td><td align="center">N</td><td align="center">Y</td><td align="center">65.0</td><td align="center">1.8</td><td align="center">3.5</td><td align="center">2.07</td></tr><tr><td align="center">20</td><td a