n><institution>Makerere University</institution><addr-line>Old Mulago Hospital Complex</addr-line><addr-line>Mulago Hill Road</addr-line><addr-line>Kampala, 256</addr-line><country>Uganda</country><phone>256 788994050</phone><email>ethelmpungu@yahoo.com</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6857-8931</contrib-id></contrib><contrib id="contrib2" contrib-type="author" equal-contrib="yes"><name><surname>Musisi</surname><given-names>Seggane</given-names></name><degrees>FRCP (C)</degrees><xref ref-type="aff" rid="aff01">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3454-0000</contrib-id></contrib><contrib id="contrib3" contrib-type="author" equal-contrib="yes"><name><surname>Wamala</surname><given-names>Kizito</given-names></name><degrees>MSc</degrees><xref ref-type="aff" rid="aff02">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4800-7052</contrib-id></contrib><contrib id="contrib4" contrib-type="author" equal-contrib="yes"><name><surname>Okello</surname><given-names>James</given-names></name><degrees>MMED (Psych), PhD</degrees><xref ref-type="aff" rid="aff03">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2634-3063</contrib-id></contrib><contrib id="contrib5" contrib-type="author" equal-contrib="yes"><name><surname>Ndyanabangi</surname><given-names>Sheila</given-names></name><degrees>MBChB, MPH</degrees><xref ref-type="aff" rid="aff04">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9859-1686</contrib-id></contrib><contrib id="contrib6" contrib-type="author" equal-contrib="yes"><name><surname>Mojtabai</surname><given-names>Ramin</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff05">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2134-4043</contrib-id></contrib><contrib id="contrib7" contrib-type="author" equal-contrib="yes"><name><surname>Nachega</surname><given-names>Jean</given-names>