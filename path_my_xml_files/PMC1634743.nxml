<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Health Qual Life Outcomes</journal-id><journal-title>Health and Quality of Life Outcomes</journal-title><issn pub-type="epub">1477-7525</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17034640</article-id><article-id pub-id-type="pmc">1634743</article-id><article-id pub-id-type="publisher-id">1477-7525-4-80</article-id><article-id pub-id-type="doi">10.1186/1477-7525-4-80</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Patient-reported outcomes in a trial of exenatide and insulin glargine for the treatment of type 2 diabetes</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Secnik Boye</surname><given-names>Kristina</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>boye_kristina_secnik@lilly.com</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Matza</surname><given-names>Louis S</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>louis.matza@unitedbiosource.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Oglesby</surname><given-names>Alan</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>oglesby_alan@lilly.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Malley</surname><given-names>Karen</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>kgmalley@comcast.net</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Kim</surname><given-names>Sunny</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>skim@fiu.edu</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Hayes</surname><given-names>Risa P</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>hayes_clarice@lilly.com</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Brodows</surname><given-names>Robert</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>brodows_robert_g@lilly.com</email></contrib></contrib-group><aff id="I1"><label>1</label>Eli Lilly and Company, Indianapolis, IN 46285, USA</aff><aff id="I2"><label>2</label>Center for Health Outcomes Research at UBC, Bethesda, MD 20814, USA</aff><aff id="I3"><label>3</label>Malley Research Programming, Inc., Rockville, MD, USA</aff><aff id="I4"><label>4</label>School of Public Health, Florida International University, USA</aff><pub-date pub-type="collection"><year>2006</year></pub-date><pub-date pub-type="epub"><day>11</day><month>10</month><year>2006</year></pub-date><volume>4</volume><fpage>80</fpage><lpage>80</lpage><ext-link ext-link-type="uri" xlink:href="http://www.hqlo.com/content/4/1/80"/><history><date date-type="received"><day>21</day><month>4</month><year>2006</year></date><date date-type="accepted"><day>11</day><month>10</month><year>2006</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2006 Secnik Boye et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2006</copyright-year><copyright-holder>Secnik Boye et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
