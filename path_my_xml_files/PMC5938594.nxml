<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Med Internet Res</journal-id><journal-id journal-id-type="iso-abbrev">J. Med. Internet Res</journal-id><journal-id journal-id-type="publisher-id">JMIR</journal-id><journal-title-group><journal-title>Journal of Medical Internet Research</journal-title></journal-title-group><issn pub-type="ppub">1439-4456</issn><issn pub-type="epub">1438-8871</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29685870</article-id><article-id pub-id-type="pmc">5938594</article-id><article-id pub-id-type="publisher-id">v20i4e136</article-id><article-id pub-id-type="doi">10.2196/jmir.9293</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Paper</subject></subj-group><subj-group subj-group-type="article-type"><subject>Original Paper</subject></subj-group></article-categories><title-group><article-title>Effectiveness of an Internet- and App-Based Intervention for College Students With Elevated Stress: Randomized Controlled Trial</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Frazier</surname><given-names>Patricia</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Baron</surname><given-names>Kelly</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Furukawa</surname><given-names>Toshi</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Harrer</surname><given-names>Mathias</given-names></name><degrees>BSc</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>Clinical Psychology and Psychotherapy</institution><institution>Friedrich-Alexander-University Erlangen-Nuremberg</institution><addr-line>N&#x000e4;gelsbachstra&#x000df;e 25a</addr-line><addr-line>Erlangen, 91052</addr-line><country>Germany</country><phone>49 1708237654</phone><email>mathias.harrer@fau.de</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7016-2687</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Adam</surname><given-names>Sophia Helen</given-names></name><degrees>MSc</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4824-8163</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Fleischmann</surname><given-names>Rebecca Jessica</given-names></name><degrees>BSc</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8425-6411</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Baumeister</surname><given-names>Harald</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2040-661X</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Auerbach</surname><given-names>Randy</given-names></name><degrees>PhD, ABPP</degrees><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2319-4744</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Bruffaerts</surname><given-names>Ronny</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-0330-3694</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Cuijpers</surname><given-names>Pim</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff5">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-5497-2743</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>Kessler</surname><given-names>Ronald C</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff6">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4831-2305</contrib-id></contrib><contrib id="contrib9" contrib-type="author"><name><surname>Berking</surname><given-names>Matthias</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-5903-4748</contrib-id></contrib><contrib id="contrib10" contrib-type="author"><name><surname>Lehr</surname><given-names>Dirk</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff7">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5560-3605</contrib-id></contrib><contrib id="contrib11" contrib-type="author"><name><surname>Ebert</surname><given-names>David Daniel</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6820-0146</contrib-id></contrib></contrib-group><aff id="aff1">
