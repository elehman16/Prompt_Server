<article xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Aging (Albany NY)</journal-id><journal-id journal-id-type="iso-abbrev">Aging (Albany NY)</journal-id><journal-id journal-id-type="publisher-id">Aging</journal-id><journal-id journal-id-type="publisher-id">ImpactJ</journal-id><journal-title-group><journal-title>Aging (Albany NY)</journal-title></journal-title-group><issn pub-type="epub">1945-4589</issn><publisher><publisher-name>Impact Journals LLC</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">26953895</article-id><article-id pub-id-type="pmc">4931840</article-id><article-id pub-id-type="publisher-id">100901</article-id><article-id pub-id-type="doi">10.18632/aging.100901</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Paper</subject></subj-group></article-categories><title-group><article-title>Exercise: a &#x0201c;new drug&#x0201d; for elderly patients with chronic heart failure</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Antonicelli</surname><given-names>Roberto</given-names></name><xref ref-type="aff" rid="A1"><sup>1</sup></xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Spazzafumo</surname><given-names>Liana</given-names></name><xref ref-type="aff" rid="A2"><sup>2</sup></xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Scalvini</surname><given-names>Simonetta</given-names></name><xref ref-type="aff" rid="A3"><sup>3</sup></xref></contrib><contrib contrib-type="author"><name><surname>Olivieri</surname><given-names>Fabiola</given-names></name><xref ref-type="aff" rid="A4"><sup>4</sup></xref><xref ref-type="aff" rid="A5"><sup>5</sup></xref></contrib>