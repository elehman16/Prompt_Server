><contrib contrib-type="author"><name><surname>Dzul-Manzanilla</surname><given-names>Felipe</given-names></name><xref ref-type="aff" rid="af3">c</xref></contrib><contrib contrib-type="author"><name><surname>Gutierrez-Castro</surname><given-names>Cipriano</given-names></name><xref ref-type="aff" rid="af3">c</xref></contrib><contrib contrib-type="author"><name><surname>Arredondo-Jim&#x000e9;nez</surname><given-names>Juan I.</given-names></name><xref ref-type="aff" rid="af4">d</xref></contrib><contrib contrib-type="author"><name><surname>S&#x000e1;nchez-Tejeda</surname><given-names>Gustavo</given-names></name><xref ref-type="aff" rid="af4">d</xref></contrib><contrib contrib-type="author"><name><surname>Vazquez-Prokopec</surname><given-names>Gonzalo</given-names></name><xref ref-type="aff" rid="af6">f</xref></contrib><contrib contrib-type="author"><name><surname>Ranson</surname><given-names>Hilary</given-names></name><xref ref-type="aff" rid="af5">e</xref></contrib><contrib contrib-type="author"><name><surname>Lenhart</surname><given-names>Audrey</given-names></name><xref ref-type="aff" rid="af7">g</xref></contrib><contrib contrib-type="author"><name><surname>Sommerfeld</surname><given-names>Johannes</given-names></name><xref ref-type="aff" rid="af8">h</xref></contrib><contrib contrib-type="author"><name><surname>McCall</surname><given-names>Philip J.</given-names></name><xref ref-type="aff" rid="af5">e</xref></contrib><contrib contrib-type="author"><name><surname>Kroeger</surname><given-names>Axel</given-names></name><xref ref-type="aff" rid="af8">h</xref></contrib><contrib contrib-type="author"><name><surname>Manrique-Saide</surname><given-names>Pablo</given-names></name><xref ref-type="aff" rid="af2">b</xref><xref ref-type="corresp" rid="cor1">*</xref></contrib><aff id="af1"><label>a</label><addr-line>Servicios de Salud de Yucat&#x000e1;n, Gobierno del Estado de Yucat&#x000e1;n, M&#x000e9;rida C.P. 97000, Mexico</addr-line></aff><aff id="af2"><label>b</label><addr-li