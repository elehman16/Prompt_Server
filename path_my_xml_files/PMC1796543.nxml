<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Transl Med</journal-id><journal-title>Journal of Translational Medicine</journal-title><issn pub-type="epub">1479-5876</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17257431</article-id><article-id pub-id-type="pmc">1796543</article-id><article-id pub-id-type="publisher-id">1479-5876-5-7</article-id><article-id pub-id-type="doi">10.1186/1479-5876-5-7</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Immune-mediated changes in actinic keratosis following topical treatment with imiquimod 5% cream</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Torres</surname><given-names>Abel</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>abelt@aol.com</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Storey</surname><given-names>Leslie</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>lesliestorey13@yahoo.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Anders</surname><given-names>Makala</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>makalaanders@yahoo.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Miller</surname><given-names>Richard L</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>rlmiller1@MMM.com</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Bulbulian</surname><given-names>Barbara J</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>bjbulbulian@MMM.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Jin</surname><given-names>Jizhong</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>jjin1@MMM.com</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Raghavan</surname><given-names>Shalini</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>sraghavan1@MMM.com</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Lee</surname><given-names>James</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>jlee107@cntus.jnj.com</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Slade</surname><given-names>Herbert B</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>hbslade@comcast.net</email></contrib><contrib id="A10" corresp="yes" contrib-type="author"><name><surname>Birmachu</surname><given-names>Woubalem</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>wbirmachu@comcast.net</email></contrib></contrib-group><aff id="I1"><label>1</label>Dermatology Office, Loma Linda University Medical Center, Loma Linda, California, USA</aff><aff id="I2"><label>2</label>Pharmacology, 3M Pharmaceuticals, St Paul, Minnesota, USA</aff><aff id="I3"><label>3</label>Medical &#x00026; Scientific Affairs, 3M Pharmaceuticals, St Paul, Minnesota, USA</aff><pub-date pub-type="collection"><year>2007</year></pub-date><pub-date pub-type="epub"><day>26</day><month>1</month><year>2007</year></pub-date><volume>5</volume><fpage>7</fpage><lpage>7</lpage><ext-link ext-link-type="uri" xlink:href="http://www.translational-medicine.com/content/5/1/7"/><history><date date-type="received"><day>11</day><month>6</month><year>2006</year></date><date date-type="accepted"><day>26</day><month>1</month><year>2007</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2007 Torres et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2007</copyright-year><copyright-holder>Torres et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
