<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Med Internet Res</journal-id><journal-id journal-id-type="iso-abbrev">J. Med. Internet Res</journal-id><journal-id journal-id-type="publisher-id">JMIR</journal-id><journal-title-group><journal-title>Journal of Medical Internet Research</journal-title></journal-title-group><issn pub-type="ppub">1439-4456</issn><issn pub-type="epub">1438-8871</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29739738</article-id><article-id pub-id-type="pmc">5964299</article-id><article-id pub-id-type="publisher-id">v20i5e166</article-id><article-id pub-id-type="doi">10.2196/jmir.9579</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Paper</subject></subj-group><subj-group subj-group-type="article-type"><subject>Original Paper</subject></subj-group></article-categories><title-group><article-title>Effects of Treatment Length and Chat-Based Counseling in a Web-Based Intervention for Cannabis Users: Randomized Factorial Trial</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Tait</surname><given-names>Robert</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Schaub</surname><given-names>Michael</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Jonas</surname><given-names>Benjamin</given-names></name><degrees>Dipl-Psych</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>Delphi Gesellschaft f&#x000fc;r Forschung, Beratung und Projektentwicklung</institution><addr-line>Kaiserdamm 8</addr-line><addr-line>Berlin, 14057</addr-line><country>Germany</country><phone>49 30 39 40 97 82</phone><fax>49 30 39 40 97 89</fax><email>jonas@delphi.de</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9146-1501</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Tensil</surname><given-names>Marc-Dennan</given-names></name><degrees>Dipl-Psych</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8901-8914</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Tossmann</surname><given-names>Peter</given-names></name><degrees>PhD, Dipl-Psych</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8175-9507</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Str&#x000fc;ber</surname><given-names>Evelin</given-names></name><degrees>Dipl-Soz</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2955-0199</contrib-id></contrib></contrib-group><aff id="aff1">
