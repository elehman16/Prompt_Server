ame><xref ref-type="aff" rid="aff2">b</xref></contrib><contrib contrib-type="author"><name><surname>H&#x000f6;rnsten</surname><given-names>Carl</given-names></name><xref ref-type="aff" rid="aff2">b</xref></contrib><contrib contrib-type="author"><name><surname>Holmberg</surname><given-names>Henrik</given-names></name><xref ref-type="aff" rid="aff3">c</xref></contrib><contrib contrib-type="author"><name><surname>Lundin-Olsson</surname><given-names>Lillemor</given-names></name><xref ref-type="aff" rid="aff1">a</xref></contrib><contrib contrib-type="author"><name><surname>Lindel&#x000f6;f</surname><given-names>Nina</given-names></name><xref ref-type="aff" rid="aff1">a</xref><xref ref-type="aff" rid="aff2">b</xref></contrib><contrib contrib-type="author"><name><surname>Nordstr&#x000f6;m</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="aff2">b</xref></contrib><contrib contrib-type="author"><name><surname>Gustafson</surname><given-names>Yngve</given-names></name><xref ref-type="aff" rid="aff2">b</xref></contrib><contrib contrib-type="author"><name><surname>Rosendahl</surname><given-names>Erik</given-names></name><xref ref-type="aff" rid="aff1">a</xref><xref ref-type="aff" rid="aff2">b</xref></contrib><aff id="aff1">
