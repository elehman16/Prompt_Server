nente Washington Health Research Institute</institution><addr-line>1730 Minor Ave</addr-line><addr-line>Suite 1600</addr-line><addr-line>Seattle, WA, 98101</addr-line><country>United States</country><phone>1 206 287 2532</phone><fax>1 206 287 2871</fax><email>rosenberg.d@ghc.org</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-1017-8833</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Lee</surname><given-names>Amy K</given-names></name><degrees>MPH</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5713-5374</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Anderson</surname><given-names>Melissa</given-names></name><degrees>MS</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3886-1394</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Renz</surname><given-names>Anne</given-names></name><degrees>MPH</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4481-0403</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Matson</surname><given-names>Theresa E</given-names></name><degrees>MPH</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4551-7981</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Kerr</surname><given-names>Jacqueline</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2341-8138</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Arterburn</surname><given-names>David</given-names></name><degrees>MD, MPH</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5208-8492</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>McClure</surname><given-names>Jennifer B</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2094-9147</contrib-id></contrib></contrib-group><aff id="aff1">
