<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Stroke Vasc Neurol</journal-id><journal-id journal-id-type="iso-abbrev">Stroke Vasc Neurol</journal-id><journal-id journal-id-type="hwp">svnbmj</journal-id><journal-id journal-id-type="publisher-id">svn</journal-id><journal-title-group><journal-title>Stroke and Vascular Neurology</journal-title></journal-title-group><issn pub-type="epub">2059-8696</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29507779</article-id><article-id pub-id-type="pmc">5829919</article-id><article-id pub-id-type="publisher-id">svn-2017-000104</article-id><article-id pub-id-type="doi">10.1136/svn-2017-000104</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group></article-categories><title-group><article-title>Ginkgo biloba extract improved cognitive and neurological functions of acute ischaemic stroke: a randomised controlled trial</article-title></title-group><contrib-group><contrib id="author-56582117" contrib-type="author"><name><surname>Li</surname><given-names>Shanshan</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-56582148" contrib-type="author"><name><surname>Zhang</surname><given-names>Xinjiang</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-56582190" contrib-type="author"><name><surname>Fang</surname><given-names>Qi</given-names></name><xref ref-type="aff" rid