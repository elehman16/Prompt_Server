<article xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Ann Rheum Dis</journal-id><journal-id journal-id-type="iso-abbrev">Ann. Rheum. Dis</journal-id><journal-id journal-id-type="hwp">annrheumdis</journal-id><journal-id journal-id-type="publisher-id">ard</journal-id><journal-title-group><journal-title>Annals of the Rheumatic Diseases</journal-title></journal-title-group><issn pub-type="ppub">0003-4967</issn><issn pub-type="epub">1468-2060</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">25995322</article-id><article-id pub-id-type="pmc">4893100</article-id><article-id pub-id-type="publisher-id">annrheumdis-2014-207001</article-id><article-id pub-id-type="doi">10.1136/annrheumdis-2014-207001</article-id><article-categories><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group><subj-group subj-group-type="heading"><subject>Clinical and Epidemiological Research</subject></subj-group><series-title>Extended report</series-title></article-categories><title-group><article-title>Efficacy of sildenafil on ischaemic digital ulcer healing in systemic sclerosis: the placebo-controlled SEDUCE study</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Hachulla</surname><given-names>Eric</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Hatron</surname><given-names>Pierre-Yves</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Carpentier</surname><given-names>Patrick</given-names></name><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Agard</surname><given-names>Christian</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Chatelus</surname><given-names>Emmanuel</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Jego</surname><given-names>Patrick</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Mouthon</surname><given-names>Luc</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>Queyrel</surname><given-names>Viviane</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>Fauchais</surname><given-names>Anne-Laure</given-names></name><xref ref-type="aff" rid="af8">8</xref></contrib><contrib contrib-type="author"><name><surname>Michon-Pasturel</surname><given-names>Ulrique</given-names></name><xref ref-type="aff" rid="af9">9</xref></contrib><contrib contrib-type="author"><name><surname>Jaussaud</surname><given-names>Roland</given-names></name><xref ref-type="aff" rid="af10">10</xref></contrib><contrib contrib-type="author"><name><surname>Mathian</surname><given-names>Alexis</given-names></name><xref ref-type="aff" rid="af11">11</xref></contrib><contrib contrib-type="author"><name><surname>Granel</surname><given-names>Brigitte</given-names></name><xref ref-type="aff" rid="af12">12</xref></contrib><contrib contrib-type="author"><name><surname>Diot</surname><given-names>Elisabeth</given-names></name><xref ref-type="aff" rid="af13">13</xref></contrib><contrib contrib-type="author"><name><surname>Farge-Bancel</surname><given-names>Dominique</given-names></name><xref ref-type="aff" rid="af14">14</xref></contrib><contrib contrib-type="author"><name><surname>Mekinian</surname><given-names>Ars&#x000e8;ne</given-names></name><xref ref-typ