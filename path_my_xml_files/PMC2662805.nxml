<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Proteome Sci</journal-id><journal-title>Proteome Science</journal-title><issn pub-type="epub">1477-5956</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19291297</article-id><article-id pub-id-type="pmc">2662805</article-id><article-id pub-id-type="publisher-id">1477-5956-7-7</article-id><article-id pub-id-type="doi">10.1186/1477-5956-7-7</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Discovery and identification of potential biomarkers of pediatric Acute Lymphoblastic Leukemia</article-title></title-group><contrib-group><contrib id="A1" equal-contrib="yes" contrib-type="author"><name><surname>Shi</surname><given-names>Linan</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>shiii@126.com</email></contrib><contrib id="A2" equal-contrib="yes" contrib-type="author"><name><surname>Zhang</surname><given-names>Jun</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>zhumin306@126.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Wu</surname><given-names>Peng</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>wpwpwp_@163.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Feng</surname><given-names>Kai</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>fkjiafp@sina.com.cn</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Li</surname><given-names>Jing</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>jing_li_66@126.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Xie</surname><given-names>Zhensheng</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>xie_zs2002@yahoo.com.cn</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Xue</surname><given-names>Peng</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>xuepeng@moon.ibp.ac.cn</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Cai</surname><given-names>Tanxi</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>caitanxi@163.com</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Cui</surname><given-names>Ziyou</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>cuiziyou@sohu.com</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Chen</surname><given-names>Xiulan</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>xiulanchen04@hotmail.com</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Hou</surname><given-names>Junjie</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>houjj_pro@163.com</email></contrib><contrib id="A12" corresp="yes" contrib-type="author"><name><surname>Zhang</surname><given-names>Jianzhong</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>zhangjz55@sina.com</email></contrib><contrib id="A13" corresp="yes" contrib-type="author"><name><surname>Yang</surname><given-names>Fuquan</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>fqyang@ibp.ac.cn</email></contrib></contrib-group><aff id="I1"><label>1</label>Proteomic Platform, Institute of Biophysics, Chinese Academy of Sciences, Beijing 100101, PR China</aff><aff id="I2"><label>2</label>Graduate University of the Chinese Academy of Sciences, Beijing 100101, PR China</aff><aff id="I3"><label>3</label>Center for Experimental Medicine, 306 Hospital of PLA, Beijing 100101, PR China</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>16</day><month>3</month><year>2009</year></pub-date><volume>7</volume><fpage>7</fpage><lpage>7</lpage><ext-link ext-link-type="uri" xlink:href="http://www.proteomesci.com/content/7/1/7"/><history><date date-type="received"><day>24</day><month>10</month><year>2008</year></date><date date-type="accepted"><day>16</day><month>3</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Shi et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Shi et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
