<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Eur Heart J</journal-id><journal-id journal-id-type="iso-abbrev">Eur. Heart J</journal-id><journal-id journal-id-type="publisher-id">eurheartj</journal-id><journal-id journal-id-type="hwp">ehj</journal-id><journal-title-group><journal-title>European Heart Journal</journal-title></journal-title-group><issn pub-type="ppub">0195-668X</issn><issn pub-type="epub">1522-9645</issn><publisher><publisher-name>Oxford University Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">24639423</article-id><article-id pub-id-type="pmc">4019912</article-id><article-id pub-id-type="doi">10.1093/eurheartj/ehu096</article-id><article-id pub-id-type="publisher-id">ehu096</article-id><article-categories><subj-group subj-group-type="heading"><subject><italic>FAST</italic>Track Clinical Research</subject><subj-group subj-group-type="heading"><subject>Coronary Artery Disease</subject></subj-group></subj-group><series-title>Fast Track</series-title></article-categories><title-group><article-title>Intravenous sodium nitrite in acute ST-elevation myocardial infarction: a randomized controlled trial (NIAMI)</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Siddiqi</surname><given-names>Nishat</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Neil</surname><given-names>Christopher</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Bruce</surname><given-names>Margaret</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>MacLennan</surname><given-names>Graeme</given-names></name><xref ref-type="aff" rid="af1">1</xref></