<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Intensive Care Med</journal-id><journal-id journal-id-type="iso-abbrev">Intensive Care Med</journal-id><journal-title-group><journal-title>Intensive Care Medicine</journal-title></journal-title-group><issn pub-type="ppub">0342-4642</issn><issn pub-type="epub">1432-1238</issn><publisher><publisher-name>Springer Berlin Heidelberg</publisher-name><publisher-loc>Berlin/Heidelberg</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29663044</article-id><article-id pub-id-type="pmc">5924665</article-id><article-id pub-id-type="publisher-id">5141</article-id><article-id pub-id-type="doi">10.1007/s00134-018-5141-9</article-id><article-categories><subj-group subj-group-type="heading"><subject>Seven-Day Profile Publication</subject></subj-group></article-categories><title-group><article-title>Procalcitonin algorithm to guide initial antibiotic therapy in acute exacerbations of COPD admitted to the ICU: a randomized multicenter study</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3898-6296</contrib-id><name><surname>Daubin</surname><given-names>C&#x000e9;dric</given-names></name><address><phone>33 2 31 06 47 16</phone><email>daubin-c@chu-caen.fr</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Valette</surname><given-names>Xavier</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Thiolli&#x000e8;re</surname><given-names>Fabrice</given-names></name><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Mira</surname><given-names>Jean-Paul</given-names></name><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Hazera</surname><given-names>Pascal</given-names></name><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Annane</surname><given-names>Djillali</given-names></name><xref ref-type="aff" rid="Aff5">5</xref><xref ref-type="aff" rid="Aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Labbe</surname><given-names>Vincent</given-names></name><xref ref-type="aff" rid="Aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Floccard</surname><given-names>Bernard</given-names></name><xref ref-type="aff" rid="Aff8">8</xref></contrib><contrib contrib-type="author"><name><surname>Fournel</surname><given-names>Fran&#x000e7;ois</given-names></name><xref ref-type="aff" rid="Aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Terzi</surname><given-names>Nicolas</given-names></name><xref ref-type="aff" rid="Aff10">10</xref><xref ref-type="aff" rid="Aff11">11</xref></contrib><contrib contrib-type="author"><name><surname>Du Cheyron</surname><given-names>Damien</given-names></name><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Parienti</surname><given-names>Jean-Jacques</given-names></name><xref ref-type="aff" rid="Aff9">9</xref><xref ref-type="aff" rid="Aff12">12</xref></contrib><contrib contrib-type="author"><collab>for the BPCTrea Study Group<contrib-group><contrib contrib-type="author"><name><surname>Djillali</surname><given-names>Annane</given-names></name></contrib><contrib contrib-type="author"><name><surname>Julien</surname><given-names>Bohe</given-names></name></contrib><contrib contrib-type="author"><name><surname>Mehdi</surname><given-names>Bousta</given-names></name></contrib><contrib contrib-type="author"><name><surname>Jennifer</surname><given-names>Brunet</given-names></name></contrib><contrib contrib-type="author"><name><surname>Olivier</surname><given-names>Cabon</given-names></name></contrib><contrib contrib-type="author"><name><surname>Pierre</surname><given-names>Charbonneau</given-names></name></contrib><contrib contrib-type="author"><name><surname>C&#x000e9;dric</surname><given-names>Daubin</given-names></name></contrib><contrib contrib-type="author"><name><surname>Fabrice</surname><given-names>Daviaud</given-names></name></contrib><contrib contrib-type="author"><name><surname>Damien</surname><given-names>Du Cheyron</given-names></name></contrib><contrib contrib-type="author"><name><surname>Muriel</surname><given-names>Fartoukh</given-names></name></contrib><contrib contrib-type="author"><name><surname>Bernard</surname><given-names>Floccard</given-names></name></contrib><contrib contrib-type="author"><name><surname>Pascal</surname><given-names>Hazera</given-names></name></contrib><contrib contrib-type="author"><name><surname>Vincent</surname><given-names>Labbe</given-names></name></contrib><contrib contrib-type="author"><name><surname>Jean-Paul</surname><given-names>Mira</given-names></name></contrib><contrib contrib-type="author"><name><surname>Andr&#x000e9;a</surname><given-names>Polito</given-names></name></contrib><contrib contrib-type="author"><name><surname>Michel</surname><given-names>Ramakers</given-names></name></contrib><contrib contrib-type="author"><name><surname>Jean-Philippe</surname><given-names>Rigaud</given-names></name></contrib><contrib contrib-type="author"><name><surname>Bertrand</surname><given-names>Sauneuf</given-names></name></contrib><contrib contrib-type="author"><name><surname>Am&#x000e9;lie</surname><given-names>Seguin</given-names></name></contrib><contrib contrib-type="author"><name><surname>Fabrice</surname><given-names>Thiolli&#x000e8;re</given-names></name></contrib><contrib contrib-type="author"><name><surname>Xavier</surname><given-names>Valette</given-names></name></contrib></contrib-group></collab></contrib><aff id="Aff1"><label>1</label><institution-wrap