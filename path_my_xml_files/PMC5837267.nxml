"><name><surname>Arashi</surname><given-names>Hiroyuki</given-names></name><xref ref-type="aff" rid="ehx162-aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Yamaguchi</surname><given-names>Junichi</given-names></name><xref ref-type="aff" rid="ehx162-aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Nakao</surname><given-names>Koichi</given-names></name><xref ref-type="aff" rid="ehx162-aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Tobaru</surname><given-names>Tetsuya</given-names></name><xref ref-type="aff" rid="ehx162-aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Tanaka</surname><given-names>Hiroyuki</given-names></name><xref ref-type="aff" rid="ehx162-aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Oka</surname><given-names>Toshiaki</given-names></name><xref ref-type="aff" rid="ehx162-aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Endoh</surname><given-names>Yasuhiro</given-names></name><xref ref-type="aff" rid="ehx162-aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Saito</surname><given-names>Katsumi</given-names></name><xref ref-type="aff" rid="ehx162-aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Uchida</surname><given-names>Tatsuro</given-names></name><xref ref-type="aff" rid="ehx162-aff8">8</xref></contrib><contrib contrib-type="author"><name><surname>Matsui</surname><given-names>Kunihiko</given-names></name><xref ref-type="aff" rid="ehx162-aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Ogawa</surname><given-names>Hiroshi</given-names></name><xref ref-type="aff" rid="ehx162-aff1">1</xref><xref ref-type="corresp" rid="ehx162-cor1"/><!--<email>mogawa@hij.twmu.ac.jp</email>--></contrib></contrib-group><aff id="ehx162-aff1">
