ontrib contrib-type="author"><name><surname>Queiroz</surname><given-names>Alexandre Oliveira</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Cataneo</surname><given-names>Daniele Cristina</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><aff id="aff1">
