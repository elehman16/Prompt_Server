<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Cancer</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Cancer</journal-id><journal-id journal-id-type="publisher-id">JC</journal-id><journal-title-group><journal-title>JMIR cancer</journal-title></journal-title-group><issn pub-type="epub">2369-1999</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28954716</article-id><article-id pub-id-type="pmc">5637068</article-id><article-id pub-id-type="publisher-id">v3i2e16</article-id><article-id pub-id-type="doi">10.2196/cancer.7166</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Paper</subject></subj-group><subj-group subj-group-type="article-type"><subject>Original Paper</subject></subj-group></article-categories><title-group><article-title>Comparison of Internet and Telephone Interventions for Weight Loss Among Cancer Survivors: Randomized Controlled Trial and Feasibility Study</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Reeves</surname><given-names>Marina</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Folta</surname><given-names>Sara</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Ritvo</surname><given-names>Paul</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author"><name><surname>Cox</surname><given-names>Matthew</given-names></name><degrees>MS, PhD</degrees><xref ref-type="aff" rid="aff01">1</xref><contrib-id contrib-id