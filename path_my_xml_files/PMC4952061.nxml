<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Pregnancy Childbirth</journal-id><journal-id journal-id-type="iso-abbrev">BMC Pregnancy Childbirth</journal-id><journal-title-group><journal-title>BMC Pregnancy and Childbirth</journal-title></journal-title-group><issn pub-type="epub">1471-2393</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27435163</article-id><article-id pub-id-type="pmc">4952061</article-id><article-id pub-id-type="publisher-id">954</article-id><article-id pub-id-type="doi">10.1186/s12884-016-0954-4</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Metformin in women with type 2 diabetes in pregnancy (MiTy): a multi-center randomized controlled trial</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Feig</surname><given-names>Denice S.</given-names></name><address><phone>416-586-8590</phone><email>d.feig@utoronto.ca</email></address><xref ref-type="aff" rid="Aff1"/><xref ref-type="aff" rid="Aff2"/><xref ref-type="aff" rid="Aff3"/></contrib><contrib contrib-type="author"><name><surname>Murphy</surname><given-names>Kellie</given-names></name><address><email>kmurphy@mtsinai.on.ca</email></address><xref ref-type="aff" rid="Aff1"/><xref ref-type="aff" rid="Aff2"/></contrib><contrib contrib-type="author"><name><surname>Asztalos</surname><given-names>Elizabeth</given-names></name><address><email>elizabeth.asztalos@sunnybrook.ca</email></address><xref ref-type="aff" rid="Aff4"/><xref ref-type="aff" rid="Aff7"/></contrib><contrib contrib-type="author"><name><surname>Tomlinson</surname><given-names>George</given-names></name><address><email>george.tomlinson@utoronto.ca</email></address><xref ref-type="aff" rid="Aff5"/><xref ref-type="aff" rid="Aff6"/></contrib><contrib contrib-type="author"><name><surname>Sanchez</surname><given-names>Johanna</given-names></name><address><email>Johanna.sanchez@sunnybrook.ca</email></address><xref ref-type="aff" rid="Aff7"/><xref ref-type="aff" rid="Aff8"/></contrib><contrib contrib-type="author"><name><surname>Zinman</surname><given-names>Bernard</given-names></name><address><email>zinman@lunenfeld.ca</email></address><xref ref-type="aff" rid="Aff1"/><xref ref-type="aff" rid="Aff2"/><xref ref-type="aff" rid="Aff3"/></contrib><contrib contrib-type="author"><name><surname>Ohlsson</surname><given-names>Arne</given-names></name><address><email>aohlsson@mtsinai.on.ca</email></address><xref ref-type="aff" rid="Aff1"/><xref ref-type="aff" rid="Aff9"/></contrib><contrib contrib-type="author"><name><surname>Ryan</surname><given-names>Edmond A.</given-names></name><address><email>edmond.ryan@ualberta.ca</email></address><xref ref-type="aff" rid="Aff10"/></contrib><contrib contrib-type="author"><name><surname>Fantus</surname><given-names>I. George</given-names></name><address><email>gfantus@mtsinai.on.ca</email></address><xref ref-type="aff" rid="Aff1"/><xref ref-type="aff" rid="Aff2"/><xref ref-type="aff" rid="Aff3"/></contrib><contrib contrib-type="author"><name><surname>Armson</surname><given-names>Anthony B.</given-names></name><address><email>anthony.armson@iwk.nshealth.ca</email></address><xref ref-type="aff" rid="Aff11"/></contrib><contrib contrib-type="author"><name><surname>Lipscombe</surname><given-names>Lorraine L.</given-names></name><address><email>lorraine.lipscombe@wchospital.ca</email></address><xref ref-type="aff" rid="Aff3"/><xref ref-type="aff" rid="Aff12"/></contrib><contrib contrib-type="author"><name><surname>Barrett</surname><given-names>Jon F.R.</given-names></name><address><email>jon.barrett@sunnybrook.ca</email></address><xref ref-type="aff" rid="Aff4"/><xref ref-type="aff" rid="Aff7"/><xref ref-type="aff" rid="Aff8"/></contrib><contrib contrib-type="author"><collab>on behalf of the MiTy Collaborative Group<contrib-group><contrib contrib-type="author"><name><surname>Carson</surname><given-names>George</given-names></name></contrib><contrib contrib-type="author"><name><surname>Williams</surname><given-names>Suzanne</given-names></name></contrib><contrib contrib-type="author"><name><surname>Kelly</surname><given-names>Sheila</given-names></name></contrib><contrib contrib-type="author"><name><surname>Clark</surname><given-names>Heather</given-names></name></contrib><contrib contrib-type="author"><name><surname>Donat</surname><given-names>Diane</given-names></name></contrib><contrib contrib-type="author"><name><surname>Gandhi</surname><given-names>Shital</given-names></name></contrib><contrib contrib-type="author"><name><surname>Cleave</surname><given-names>Barbara</given-names></name></contrib><contrib contrib-type="author"><name><surname>Strom</surname><given-names>Michele</given-names></name></contrib><contrib contrib-type="author"><name><surname>Donovan</surname><given-names>Lois</given-names></name></contrib><contrib contrib-type="author"><name><surname>Oldford</surname><given-names>Carolyn</given-names></name></contrib><contrib contrib-type="author"><name><surname>Young</surname><given-names>Catherine</given-names></name></contrib><contrib contrib-type="author"><name><surname>Galway</surname><given-names>Brenda</given-names></name></contrib><contrib contrib-type="author"><name><surname>Parsons</surname><given-names>Minnie</given-names></name></contrib><contrib contrib-type="author"><name><surname>Godbout</surname><given-names>Ariane</given-names></name></contrib><contrib contrib-type="author"><name><surname>Mahone</surname><given-names>Michele</given-names></name></contrib><contrib contrib-type="author"><name><surname>Weber</surname><given-names>Florence</given-names></name></contrib><contrib contrib-type="author"><name><surname>Bedard</surname><given-names>Marie-Josee</given-names></name></contrib><contrib contrib-type="author"><name><surname>Wo</surname><given-names>Bi Lan</given-names></name></contrib><contrib contrib-type="author"><name><surname>Daigle</surname><given-names>Sylvie</given-names></name></contrib><contrib contrib-type="author"><name><surname>Hanna</surname><given-names>Amir</given-names></name></contrib><contrib contrib-type="author"><name><surname>Wolfs</surname><given-names>Maria</given-names></name></contrib><contrib contrib-type="author"><name><surname>De Souza</surname><given-names>Leanne</given-names></name></contrib><contrib contrib-type="author"><name><surname>Houlden</surname><given-names>Robyn</given-names></name></contrib><contrib contrib-type="author"><name><surname>Breen</surname><given-names>Adriana</given-names></name></contrib><contrib contrib-type="author"><name><surname>Kader</surname><given-names>Tina</given-names></name></contrib><contrib contrib-type="author"><name><surname>Cefis</surname><given-names>Luca</given-names></name></contrib><contrib contrib-type="author"><name><surname>Keely</surname><given-names>Erin</given-names></name></contrib><contrib contrib-type="author"><name><surname>Champagne</surname><given-names>Josee</given-names></name></contrib><contrib contrib-type="author"><name><surname>Klinke</surname><given-names>Jennifer</given-names></name></contrib><contrib contrib-type="author"><name><surname>Lee</surname><given-names>Julie</given-names></name></contrib><contrib contrib-type="author"><name><surname>Subrt</surname><given-names>Peter</given-names></name></contrib><contrib contrib-type="author"><name><surname>Carr</surname><given-names>Francina</given-names></name></contrib><contrib contrib-type="author"><name><surname>Young</surname><given-names>Sharon</given-names></name></contrib><contrib contrib-type="author"><name><surname>Subrt</surname><given-names>Peter</given-names></name