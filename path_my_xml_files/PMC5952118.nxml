<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Med Internet Res</journal-id><journal-id journal-id-type="iso-abbrev">J. Med. Internet Res</journal-id><journal-id journal-id-type="publisher-id">JMIR</journal-id><journal-title-group><journal-title>Journal of Medical Internet Research</journal-title></journal-title-group><issn pub-type="ppub">1439-4456</issn><issn pub-type="epub">1438-8871</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29712630</article-id><article-id pub-id-type="pmc">5952118</article-id><article-id pub-id-type="publisher-id">v20i4e159</article-id><article-id pub-id-type="doi">10.2196/jmir.8514</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Paper</subject></subj-group><subj-group subj-group-type="article-type"><subject>Original Paper</subject></subj-group></article-categories><title-group><article-title>Efficacy of a Community-Based Technology-Enabled Physical Activity Counseling Program for People With Knee Osteoarthritis: Proof-of-Concept Study</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Nero</surname><given-names>H&#x000e5;kan</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Sch&#x000e4;fer</surname><given-names>Axel</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Li</surname><given-names>Linda C</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><address><institution>Arthritis Research Canada</institution><addr-line>5591 No. 3 Road</addr-line><addr-line>Richmond, BC, V6X 2C7</addr-line><country>Canada</country><phone>1 604 207 4020</phone><email>lli@arthritisresearch.ca</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6280-0511</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Sayre</surname><given-names>Eric C</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4526-278X</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Xie</surname><given-names>Hui</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-0526-9204</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Falck</surname><given-names>Ryan S</given-names></name><degrees>MSc</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4224-3871</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Best</surname><given-names>John R</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-5775-7053</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Liu-Ambrose</surname><given-names>Teresa</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8297-5308</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Grewal</surname><given-names>Navi</given-names></name><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5718-8804</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>Hoens</surname><given-names>Alison M</given-names></name><degrees>BSc (PT), MSc</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9533-9079</contrib-id></contrib><contrib id="contrib9" contrib-type="author"><name><surname>Noonan</surname><given-names>Greg</given-names></name><degrees>BSc (PT)</degrees><xref ref-type="aff" rid="aff4">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9266-6253</contrib-id></contrib><contrib id="contrib10" contrib-type="author"><name><surname>Feehan</surname><given-names>Lynne M</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5758-4241</contrib-id></contrib></contrib-group><aff id="aff1">
