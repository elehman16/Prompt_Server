<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Med</journal-id><journal-title>BMC Medicine</journal-title><issn pub-type="epub">1741-7015</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19832967</article-id><article-id pub-id-type="pmc">2768737</article-id><article-id pub-id-type="publisher-id">1741-7015-7-60</article-id><article-id pub-id-type="doi">10.1186/1741-7015-7-60</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title>Suicidal ideation during treatment of depression with escitalopram and nortriptyline in Genome-Based Therapeutic Drugs for Depression (GENDEP): a clinical trial</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Perroud</surname><given-names>Nader</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>nader.perroud@hcuge.ch</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Uher</surname><given-names>Rudolf</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>rudolf.uher@iop.kcl.ac.uk</email></contrib><contrib id="A3" deceased="yes" contrib-type="author"><name><surname>Marusic</surname><given-names>Andrej</given-names></name><xref ref-type="aff" rid="I2">2</xref></contrib><contrib id="A4" contrib-type="author"><name><surname>Rietschel</surname><given-names>Marcella</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>Marcella.Rietschel@zi-mannheim.de</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Mors</surname><given-names>Ole</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>OM@psykiatri.aaa.dk</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Henigsbe