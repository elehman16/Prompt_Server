<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Clin Interv Aging</journal-id><journal-id journal-id-type="iso-abbrev">Clin Interv Aging</journal-id><journal-id journal-id-type="publisher-id">Clinical Interventions in Aging</journal-id><journal-title-group><journal-title>Clinical Interventions in Aging</journal-title></journal-title-group><issn pub-type="ppub">1176-9092</issn><issn pub-type="epub">1178-1998</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28744114</article-id><article-id pub-id-type="pmc">5513809</article-id><article-id pub-id-type="doi">10.2147/CIA.S133838</article-id><article-id pub-id-type="publisher-id">cia-12-1103</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Acute effects of power and resistance exercises on hemodynamic measurements of older women</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Coelho-J&#x000fa;nior</surname><given-names>H&#x000e9;lio Jos&#x000e9;</given-names></name><xref ref-type="aff" rid="af1-cia-12-1103">1</xref><xref ref-type="aff" rid="af2-cia-12-1103">2</xref><xref ref-type="corresp" rid="c1-cia-12-1103"/></contrib><contrib contrib-type="author"><name><surname>Irigoyen</surname><given-names>Maria-Cl&#x000e1;udia</given-names></name><xref ref-type="aff" rid="af3-cia-12-1103">3</xref></contrib><contrib contrib-type="author"><name><surname>Aguiar</surname><given-names>Samuel da Silva</given-names></name><xref ref-type="aff" rid="af2-cia-12-1103">2</xref><xref ref-type="aff" rid="af4-cia-12-1103">4</xref></contrib><contrib contrib-type="author"><name><surname>Gon&#x000e7;alves</surname><given-names>Ivan de Oliveira</given-names></name><xref ref-type="aff" rid="af2-cia-12-1103">2</xref><xref ref-type="aff" rid="af5-cia-12-1103">5</xref></contrib><contrib contrib-type="author"><name><surname>C&#x000e2;mara</surname><given-names>Niels Olsen Saraiva</given-names></name><xref ref-type="aff" rid="af6-cia-12-1103">6</xref></contrib><contrib contrib-type="author"><name><surname>Cenedeze</surname><given-names>Marco Antonio</given-names></name><xref ref-type="aff" rid="af7-cia-12-1103">7</xref></contrib><contrib contrib-type="author"><name><surname>Asano</surname><given-names>Ricardo Yukio</given-names></name><xref ref-type="aff" rid="af2-cia-12-1103">2</xref><xref ref-type="aff" rid="af8-cia-12-1103">8</xref></contrib><contrib contrib-type="author"><name><surname>Rodrigues</surname><given-names>Bruno</given-names></name><xref ref-type="aff" rid="af1-cia-12-1103">1</xref></contrib><contrib contrib-type="author"><name><surname>Uchida</surname><given-names>Marco Carlos</given-names></name><xref ref-type="aff" rid="af1-cia-12-1103">1</xref></contrib></contrib-group><aff id="af1-cia-12-1103">
