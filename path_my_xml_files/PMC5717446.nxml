valuation</institution><institution>University of Toronto</institution><addr-line>155 College Street, 4th Floor, Suite 425</addr-line><addr-line>Toronto, ON,</addr-line><country>Canada</country><phone>1 416 669 9295</phone><email>emily.seto@utoronto.ca</email></address><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8723-5915</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Ware</surname><given-names>Patrick</given-names></name><degrees>MPH</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5325-9871</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Logan</surname><given-names>Alexander G</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff3">3</xref><xref ref-type="aff" rid="aff4">4</xref><xref ref-type="aff" rid="aff5">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3419-5761</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Cafazzo</surname><given-names>Joseph A</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff6">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3114-4440</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Chapman</surname><given-names>Kenneth R</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff7">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2498-9859</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Segal</surname><given-names>Phillip</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff8">8</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-5022-908X</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Ros