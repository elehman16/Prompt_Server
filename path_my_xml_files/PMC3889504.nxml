contrib><contrib contrib-type="author"><name><surname>Bijlsma</surname><given-names>J. W. J.</given-names></name><xref ref-type="aff" rid="Aff6"/></contrib><contrib contrib-type="author"><name><surname>Egberts</surname><given-names>A. C. G.</given-names></name><xref ref-type="aff" rid="Aff1"/><xref ref-type="aff" rid="Aff7"/></contrib><contrib contrib-type="author" corresp="yes"><name><surname>Bouvy</surname><given-names>M. L.</given-names></name><address><phone>+31-30-2537324</phone><fax>+31-30-2539166</fax><email>m.l.bouvy@uu.nl</email></address><xref ref-type="aff" rid="Aff1"/><xref ref-type="aff" rid="Aff8"/></contrib><aff id="Aff1"><label/>Utrecht Institute for Pharmaceutical Sciences, Division of Pharmacoepidemiology &#x00026; Clinical Pharmacology, Utrecht University, Universiteitsweg 99, 3584 CG Utrecht, The Netherlands </aff><aff id="Aff2"><label/>MRC Lifecourse Epidemiology Unit, Southampton General Hospital, Southampton, UK </aff><aff id="Aff3"><label/>Department of Clinical Pharmacy &#x00026; Toxicology, Maastricht University Medical Centre, Maastricht, The Netherlands </aff><aff id="Aff4"><label/>Maastricht University/CAPHRI, Maastricht, The Netherlands </aff><aff id="Aff5"><label/>Pharmacy DeLeij, Tilburg, The Netherlands </aff><aff id="Aff6"><label/>Department of Rheumatology &#x00026; Clinical Immunology, University Medical Centre, Utrecht, The Netherlands </aff><aff id="Aff7"><label/>Hospital Pharmacy, University Medical Centre, Utrecht, The Netherlands </aff><aff id="Aff8"><label/>SIR Institute for Pharmacy Practice and Policy, Leiden, The Netherlands </aff></contrib-group><pub-date pub-type="epub"><day>13</day><month>11</month><year>2013</year></pub-date><pub-date pub-type="pmc-release"><day>13</day><month>11</month><year>2013</year></pub-date><pub-date pub-type="ppub"><year>2014</year></pub-date><volume>25</volume><issue>1</issue><fpage>385</fpage><lpage>392</lpage><history><date date-type="received"><day>27</day><month>6</month><year>2013</year>