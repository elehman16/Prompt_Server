r"><name><surname>Yin</surname><given-names>Xiao-Fan</given-names></name><xref ref-type="aff" rid="af1-ijn-12-7197">1</xref><xref ref-type="aff" rid="af2-ijn-12-7197">2</xref></contrib><contrib contrib-type="author"><name><surname>Bian</surname><given-names>Chong</given-names></name><xref ref-type="aff" rid="af1-ijn-12-7197">1</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Hui-Ren</given-names></name><xref ref-type="aff" rid="af1-ijn-12-7197">1</xref></contrib><contrib contrib-type="author"><name><surname>Ma</surname><given-names>Yi-Qun</given-names></name><xref ref-type="aff" rid="af1-ijn-12-7197">1</xref></contrib><contrib contrib-type="author"><name><surname>Li</surname><given-names>Xiang-Qian</given-names></name><xref ref-type="aff" rid="af1-ijn-12-7197">1</xref></contrib><contrib contrib-type="author"><name><surname>Li</surname><given-names>Xi-Lei</given-names></name><xref ref-type="aff" rid="af1-ijn-12-7197">1</xref><xref ref-type="corresp" rid="c1-ijn-12-7197"/></contrib><contrib contrib-type="author"><name><surname>Dong</surname><given-names>Jian</given-names></name><xref ref-type="aff" rid="af1-ijn-12-7197">1</xref><xref ref-type="corresp" rid="c1-ijn-12-7197"/></contrib></contrib-group><aff id="af1-ijn-12-7197">
