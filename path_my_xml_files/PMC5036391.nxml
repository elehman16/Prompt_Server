<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><?properties manuscript?><front><journal-meta><journal-id journal-id-type="nlm-journal-id">101592543</journal-id><journal-id journal-id-type="pubmed-jr-id">40654</journal-id><journal-id journal-id-type="nlm-ta">J Yoga Phys Ther</journal-id><journal-id journal-id-type="iso-abbrev">J Yoga Phys Ther</journal-id><journal-title-group><journal-title>Journal of yoga &#x00026; physical therapy</journal-title></journal-title-group><issn pub-type="epub">2157-7595</issn></journal-meta><article-meta><article-id pub-id-type="pmid">27683623</article-id><article-id pub-id-type="pmc">5036391</article-id><article-id pub-id-type="doi">10.4172/2157-7595.1000245</article-id><article-id pub-id-type="manuscript">NIHMS811703</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Differences between Men and Women Enrolling in Smoking Cessation Programs Using Yoga as a Complementary Therapy</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Thind</surname><given-names>Herpreet</given-names></name><xref ref-type="aff" rid="A1">1</xref></contrib><contrib contrib-type="author"><name><surname>Jennings</surname><given-names>Ernestine</given-names></name><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Fava</surname><given-names>Joseph L</given-names></name><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Sillice</surname><given-names>Marie A</given-names></name><xref ref-type="aff" rid="A3">3</xref></contrib><contrib contrib-type="author"><name><surname>Becker</surname><given-names>Bruce M</given-names></name><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Hartman</surname><given-names>Sheri J</given-names></name><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Bock</surname><given-names>Beth C</given-names></name><xref ref-type="aff" rid="A3">3</xref><xref ref-type="corresp" rid="CR1">*</xref></contrib></contrib-group><aff id="A1"><label>1</label>Department of Community Health and Sustainability, College of Health Sciences, University of Massachusetts Lowell, USA
