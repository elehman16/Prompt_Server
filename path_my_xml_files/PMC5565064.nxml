rib contrib-type="author"><name><surname>Litchfield</surname><given-names>Robert B.</given-names></name><degrees>MD, FRSC</degrees><xref ref-type="aff" rid="aff3-2325967117S00355">3</xref></contrib><contrib contrib-type="author"><name><surname>Willits</surname><given-names>Kevin</given-names></name><degrees>MD, FRCS</degrees><xref ref-type="aff" rid="aff4-2325967117S00355">4</xref></contrib><contrib contrib-type="author"><name><surname>Birmingham</surname><given-names>Trevor</given-names></name><degrees>PhD, PT</degrees><xref ref-type="aff" rid="aff5-2325967117S00355">5</xref></contrib></contrib-group><aff id="aff1-2325967117S00355">
