<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Arthritis Res Ther</journal-id><journal-title>Arthritis Research &#x00026; Therapy</journal-title><issn pub-type="ppub">1478-6354</issn><issn pub-type="epub">1478-6362</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19630944</article-id><article-id pub-id-type="pmc">2745797</article-id><article-id pub-id-type="publisher-id">ar2774</article-id><article-id pub-id-type="doi">10.1186/ar2774</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title>Identification of progressors in osteoarthritis by combining biochemical and MRI-based markers</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Dam</surname><given-names>Erik B</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>erikdam@nordicbioscience.com</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Loog</surname><given-names>Marco</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><email>marco.loog@gmail.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Christiansen</surname><given-names>Claus</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cc@nordicbioscience.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Byrjalsen</surname><given-names>Inger</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>ib@nordicbioscience.com</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Folkesson</surname><given-names>Jenny</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>jenny.folkesson@gmail.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Nielsen</surname><given-names>Mads</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><email>madsn@diku.dk</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Qazi</surname><given-names>Arish A</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>arish@diku.dk</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Pettersen</surname><given-names>Paola C</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>Paola.Pettersen@ccbr.com</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Garnero</surname><given-names>Patrick</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>patrick.garnero@synarc.com</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Karsdal</surname><given-names>Morten A</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>mk@nordicbioscience.com</email></contrib></contrib-group><aff id="I1"><label>1</label>Nordic Bioscience, Herlev Hovedgade 207, 2730 Herlev, Denmark</aff><aff id="I2"><label>2</label>University of Copenhagen, Department of Computer Science, Universitetsparken 1, 2100 Copenhagen, Denmark</aff><aff id="I3"><label>3</label>Delft University of Technology, Faculty of Electrical Engineering, Mathematics, and Computer Science, Mekelweg 4, 2628 CD Delft, The Netherlands</aff><aff id="I4"><label>4</label>Center for Clinical and Basic Research, Ballerup Byvej 222, 2750 Ballerup, Denmark</aff><aff id="I5"><label>5</label>CCBR-Synarc, Molecular Markers, Rue Montbrillant 16, 69003 Lyon, France</aff><pub-date pub-type="ppub"><year>2009</year></pub-date><pub-date pub-type="epub"><day>24</day><month>7</month><year>2009</year></pub-date><volume>11</volume><issue>4</issue><fpage>R115</fpage><lpage>R115</lpage><ext-link ext-link-type="uri" xlink:href="http://arthritis-research.com/content/11/4/R115"/><history><date date-type="received"><day>6</day><month>2</month><year>2009</year></date><date date-type="rev-request"><day>14</day><month>4</month><year>2009</year></date><date date-type="rev-recd"><day>22</day><month>5</month><year>2009</year></date><date date-type="accepted"><day>24</day><month>7</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Dam et al.; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Dam et al.; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an open access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
