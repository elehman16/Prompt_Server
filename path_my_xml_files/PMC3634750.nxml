rticle-title><alt-title alt-title-type="running-head">TCI in Bronchoscopy Sedation</alt-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Lin</surname><given-names>Ting-Yu</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Lo</surname><given-names>Yu-Lun</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author"><name><surname>Hsieh</surname><given-names>Chung-Hsing</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author"><name><surname>Ni</surname><given-names>Yung-Lun</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Tsai-Yu</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author"><name><surname>Lin</surname><given-names>Horng-Chyuan</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Chun-Hua</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author"><name><surname>Yu</surname><given-names>Chih-Teng</given-names></name><xref ref-type="aff" rid="aff1"/></contrib><contrib contrib-type="author"><name><surname>Kuo</surname><given-names>Han-Pin</given-names></name><xref ref-type="aff" rid="aff1"/><xref ref-type="corresp" rid="cor1">
