trib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Legerstee</surname><given-names>Jeroen S</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6793-1123</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Dierckx</surname><given-names>Bram</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5045-9894</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Staals</surname><given-names>Lonneke M</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7867-971X</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Berghmans</surname><given-names>Johan</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-3835-562X</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>van der Schroeff</surname><given-names>Marc P</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff4">4</xref><xref ref-type="aff" rid="aff5">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-1360-8782</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Wijnen</surname><given-names>Rene MH</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff6">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7266-9713</contrib-id></contrib><contrib id="contrib8" contrib-type="author" corresp="yes"><name><surname>Utens</surname><given-names>Elisabeth MWJ</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>Departmen