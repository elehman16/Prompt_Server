me><xref ref-type="aff" rid="aff002"><sup>2</sup></xref><xref ref-type="aff" rid="aff005"><sup>5</sup></xref></contrib><contrib contrib-type="author"><name><surname>Rodriguez</surname><given-names>Diego</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Rodriguez</surname><given-names>Ana</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>Bonet</surname><given-names>Josep Maria</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib><contrib contrib-type="author"><name><surname>Trapero-Bertran</surname><given-names>Marta</given-names></name><xref ref-type="aff" rid="aff007"><sup>7</sup></xref><xref ref-type="aff" rid="aff008"><sup>8</sup></xref></contrib><contrib contrib-type="author"><name><surname>Hidalgo</surname><given-names>Pablo</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib><contrib contrib-type="author"><name><surname>S&#x000e0;nchez</surname><given-names>Norman</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib><contrib contrib-type="author"><contrib-id authenticated="true" contrib-id-type="orcid">http://orcid.org/0000-0002-5909-676X</contrib-id><name><surname>de Sanjos&#x000e9;</surname><given-names>Silvia</given-names></name><xref ref-type="aff" rid="aff009"><sup>9</sup></xref><xref ref-type="aff" rid="aff010"><sup>10</sup></xref><xref ref-type="corresp" rid="cor001">*</xref></contrib></contrib-group><aff id="aff001"><label>1</label><addr-line>Atenci&#x000f3; a la Salut Sexual i Reproductiva (ASSIR) SAP Cerdanyola&#x02013;Ripollet, Institut Catal&#x000e0; de la Salut, Ripollet, Spain</addr-line></aff><aff id="aff002"><label>2</label><addr-line>Unitat de Suport a la Recerca Metropolitana Nord. Institut de Investigaci&#x000f3; en Atenci&#x000f3; Prim&#x000e0;ria (IDIAP) Jordi Gol. Sabadell, Spain</addr-line></aff><aff id="aff003"><label>3</label><addr-line>Departament de Medicina, Universitat de Barcelona. Barcelona, Spain</addr-line></aff><aff id="aff004"><label>4</label><addr-line>Grup de Recerca en Atenci&#x000f3; Sexual i Reproductiva IDIAP Jordi Gol. Sabadell, Spain</addr-line></aff><aff id="aff005"><label>5</label><addr-line>Departament de Infermeria. Universitat Aut&#x000f2;noma de Barcelona. Bellaterra, Cerdanyola, Spain</addr-line></aff><aff id="aff006"><label>6</label><addr-line>Servei d&#x02019;Atenci&#x000f3; Prim&#x000e0;ria SAP Vall&#x000e9;s Occidental. Institut Catal&#x000e0; de la Salut, Sabadell, Spain</addr-line></aff><aff id="aff007"><label>7</label><addr-line>Center for Research in Economics and Health (CRES). University Pompeu Fabra, Barcelona, Spain</addr-line></aff><aff id="aff008"><label>8</label><addr-line>Universidad de Castilla La Mancha. Ciudad Real, Spain</addr-line></aff><aff id="aff009"><label>9</label><addr-line>Cancer Epidemiology Research Programme | IDIBELL | Catalan Institute of Oncology, L&#x02019;Hospitalet de Llobregat, Spain</addr-line></aff><aff id="aff010"><label>10</label><addr-line>Centro de Investigaci&#x000f3;n Biom&#x000e9;dica en Red en Epidemiologia y Salud P&#x000fa;blica CIBERESP, Barcelona, Spain</addr-line></aff><contrib-group><contrib contrib-type="editor"><name><surname>Perno</surname><given-names>Carlo Federico</given-names></name><role>Editor</role><xref ref-type="aff" rid="edit1"/></contrib></contrib-group><aff id="edit1"><addr-line>Universita degli Studi di Roma Tor Vergata, ITALY</addr-line></aff><author-notes><fn fn-type="COI-statement" id="coi001"><p><bold>Competing Interests: </bold>All authors have declared that they have no competing interests.</p></fn><fn fn-type="con"><p><list list-type="simple"><list-item><p><bold>Conceptualization:</bold> AA JMM.</p></list-item><list-item><p><bold>Data curation:</bold> AA NS JMM.</p></list-item><list-item><p><bold>Formal analysis:</bold> AA JMM SDS MT.</p></list-item><l