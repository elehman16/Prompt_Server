<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><?properties manuscript?><front><journal-meta><journal-id journal-id-type="nlm-journal-id">101264860</journal-id><journal-id journal-id-type="pubmed-jr-id">32902</journal-id><journal-id journal-id-type="nlm-ta">Obesity (Silver Spring)</journal-id><journal-id journal-id-type="iso-abbrev">Obesity (Silver Spring)</journal-id><journal-title-group><journal-title>Obesity (Silver Spring, Md.)</journal-title></journal-title-group><issn pub-type="ppub">1930-7381</issn><issn pub-type="epub">1930-739X</issn></journal-meta><article-meta><article-id pub-id-type="pmid">28558160</article-id><article-id pub-id-type="pmc">5513190</article-id><article-id pub-id-type="doi">10.1002/oby.21864</article-id><article-id pub-id-type="manuscript">NIHMS865351</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Randomized Trial Reveals that Physical Activity and Energy Expenditure are associated with Weight and Body Composition after RYGB</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Carnero</surname><given-names>Elvis Alvarez</given-names></name><degrees>Ph.D.</degrees><xref ref-type="aff" rid="A1">1</xref></contrib><contrib contrib-type="author"><name><surname>Dubis</surname><given-names>Gabriel S.</given-names></name><degrees>M.S.</degrees><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Hames</surname><given-names>Kazanna C.</given-names></name><degrees>Ph.D.</degrees><xref ref-type="aff" rid="A3">3</xref><xref rid="FN2" ref-type="author-notes">*</xref><xref ref-type="aff" rid="A4">4</xref></contrib><contrib contrib-type="author"><name><surname>Jakicic</surname><given-names>John M.</given-names></name><degrees>Ph.D.</degrees><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Houmard</surname><given-names>Joseph A.</given-names></name><degrees>Ph.D.</degrees><xref ref-type="aff" rid="A2">2</xref></contrib><contrib contrib-type="author"><name><surname>Coen</surname><given-names>Paul M.</given-names></name><degrees>Ph.D.</degrees><xref ref-type="aff" rid="A1">1</xref><xref rid="FN2" ref-type="author-notes">*</xref><xref ref-type="aff" rid="A4">4</xref><xref ref-type="aff" rid="A5">5</xref></contrib><contrib contrib-type="author"><name><surname>Goodpaster</surname><given-names>Bret H.</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="A1">1</xref><xref rid="FN2" ref-type="author-notes">*</xref><xref ref-type="aff" rid="A4">4</xref></contrib></contrib-group><aff id="A1">
