<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Ther Clin Risk Manag</journal-id><journal-id journal-id-type="iso-abbrev">Ther Clin Risk Manag</journal-id><journal-id journal-id-type="publisher-id">Therapeutics and Clinical Risk Management</journal-id><journal-title-group><journal-title>Therapeutics and Clinical Risk Management</journal-title></journal-title-group><issn pub-type="ppub">1176-6336</issn><issn pub-type="epub">1178-203X</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29042788</article-id><article-id pub-id-type="pmc">5633319</article-id><article-id pub-id-type="doi">10.2147/TCRM.S144659</article-id><article-id pub-id-type="publisher-id">tcrm-13-1309</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Decreased frequency of adenoidectomy by a 12-week nasal budesonide treatment</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Hong</surname><given-names>Haiyu</given-names></name><xref ref-type="aff" rid="af1-tcrm-13-1309">1</xref><xref ref-type="author-notes" rid="fn1-tcrm-13-1309">*</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Chen</surname><given-names>Fenghong</given-names></name><xref ref-type="aff" rid="af2-tcrm-13-1309">2</xref><xref ref-type="author-notes" rid="fn1-tcrm-13-1309">*</xref></contrib><contrib contrib-type="author"><name><surname>Zheng</surname><given-names>Xiaobin</given-names></name><xref ref-type="aff" rid="af3-tcrm-13-1309">3</xref></contrib><contrib contrib-type="author"><name><surname>Liao</surname><given-names>Wei</given-names></name><xref ref-type="aff" rid="af1-tcrm-13-1309">1</xref></contrib><contrib contrib-type="author"><name><surname>Liao</surname><given-names>Zhenpeng</given-names></name><xref ref-type="aff" rid="af1-tcrm-13-1309">1</xref></contrib><contrib contrib-type="author"><name><surname>Cao</surname><given-names>Yujie</given-names></name><xref ref-type="aff" rid="af2-tcrm-13-1309">2</xref></contrib><contrib contrib-type="author"><name><surname>He</surname><given-names>Haixin</given-names></name><xref ref-type="aff" rid="af1-tcrm-13-1309">1</xref></contrib><contrib contrib-type="author"><name><surname>Zhu</surname><given-names>Zhe</given-names></name><xref ref-type="aff" rid="af4-tcrm-13-1309">4</xref><xref ref-type="aff" rid="af5-tcrm-13-1309">5</xref></contrib><contrib contrib-type="author"><name><surname>Fan</surname><given-names>Yunping</given-names></name><xref ref-type="aff" rid="af1-tcrm-13-1309">1</xref><xref ref-type="corresp" rid="c1-tcrm-13-1309"/></contrib></contrib-group><aff id="af1-tcrm-13-1309">
