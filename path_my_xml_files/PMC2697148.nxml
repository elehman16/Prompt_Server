<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Nutr Metab (Lond)</journal-id><journal-title>Nutrition &#x00026; Metabolism</journal-title><issn pub-type="epub">1743-7075</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19490608</article-id><article-id pub-id-type="pmc">2697148</article-id><article-id pub-id-type="publisher-id">1743-7075-6-25</article-id><article-id pub-id-type="doi">10.1186/1743-7075-6-25</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Effect of oral acetyl L-carnitine arginate on resting and postprandial blood biomarkers in pre-diabetics</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Bloomer</surname><given-names>Richard J</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>rbloomer@memphis.edu</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Fisher-Wellman</surname><given-names>Kelsey H</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>kfshrwll@memphis.edu</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Tucker</surname><given-names>Patrick S</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>patricktucker2004@yahoo.com</email></contrib></contrib-group><aff id="I1"><label>1</label>Cardiorespiratory/Metabolic Laboratory, The University of Memphis, Memphis, Tennessee 38152, USA</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>2</day><month>6</month><year>2009</year></pub-date><volume>6</volume><fpage>25</fpage><lpage>25</lpage><ext-link ext-link-type="uri" xlink:href="http://www.nutritionandmetabolism.com/content/6/1/25"/><history><date date-type="received"><day>5</day><month>1</month><year>2009</year></date><date date-type="accepted"><day>2</day><month>6</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Bloomer et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Bloomer et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
