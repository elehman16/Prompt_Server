<article xmlns:ali="http://www.niso.org/schemas/ali/1.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Thorax</journal-id><journal-id journal-id-type="iso-abbrev">Thorax</journal-id><journal-id journal-id-type="hwp">thoraxjnl</journal-id><journal-id journal-id-type="publisher-id">thorax</journal-id><journal-title-group><journal-title>Thorax</journal-title></journal-title-group><issn pub-type="ppub">0040-6376</issn><issn pub-type="epub">1468-3296</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">25841237</article-id><article-id pub-id-type="pmc">4453631</article-id><article-id pub-id-type="publisher-id">thoraxjnl-2014-206670</article-id><article-id pub-id-type="doi">10.1136/thoraxjnl-2014-206670</article-id><article-categories><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group><subj-group subj-group-type="heading"><subject>Chronic Obstructive Pulmonary Disease</subject></subj-group><series-title>Original article</series-title></article-categories><title-group><article-title>Glycopyrronium once-daily significantly improves lung function and health status when combined with salmeterol/fluticasone in patients with COPD: the GLISTEN study&#x02014;a randomised controlled trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Frith</surname><given-names>Peter A</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Thompson</surname><given-names>Philip J</given-names></name><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Ratnavadivel</surname><given-names>Rajeev</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Chang</surname><given-names>Catherina L</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Bremner</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Day</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>Frenzel</surname><given-names>Christina</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>Kurstjens</surname><given-names>Nicol</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><collab>the Glisten Study Group</collab></contrib></contrib-group><contrib-group><contrib contrib-type="collab"><name><surname>Waddell</surname><given-names>Ainslie</given-names></name></contrib><contrib contrib-type="collab"><name><surname>Daniel</surname><given-names>Alexander</given-names></name></contrib><contrib contrib-type="collab"><name><surname>Khoussousi</surname><given-names>Alireza</given-names></name></contrib><contrib contrib-type="collab"><name><surname>Springfield</surname><given-names>Andrew</given-names></name></contrib><contrib contrib-type="collab"><name><surname>Veale</surname><given-names>Andrew</given-names></name></contrib><contrib contrib-type="collab"><name><surname>Graham</surname><given-names>Anthony Neil</given-names></name></contrib><contrib contrib-type="collab"><name><surname>Gallagher</surname><given-names>Brad</given-names></name></contrib><contrib contrib-type="collab"><name><surname>Pande</surname><given-names>Braj Raj</given-names></name></contrib><contrib contrib-type="collab"><name><surname>O'Kane</surname><given-names>Brendan</given-names></name></contrib><contrib contrib-type="collab"><name><s