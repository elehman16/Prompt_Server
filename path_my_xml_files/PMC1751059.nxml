<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Crit Care</journal-id><journal-title>Critical Care</journal-title><issn pub-type="ppub">1364-8535</issn><issn pub-type="epub">1466-609X</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17032443</article-id><article-id pub-id-type="pmc">1751059</article-id><article-id pub-id-type="publisher-id">cc5065</article-id><article-id pub-id-type="doi">10.1186/cc5065</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Dobutamine reverses the vasopressin-associated impairment in cardiac index and systemic oxygen supply in ovine endotoxemia</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Ertmer</surname><given-names>Christian</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>ertmerc@gmx.net</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Morelli</surname><given-names>Andrea</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>Andrea.Morelli@uniroma1.it</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Bone</surname><given-names>Hans-Georg</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>bone@uni-muenster.de</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Stubbe</surname><given-names>Henning Dirk</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>hstubbe@uni-muenster.de</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Schepers</surname><given-names>Ralf</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>ralf.schepers@shudokan.de</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Van Aken</surname><given-names>Hugo</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>hva@anit.uni-muenster.de</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Lange</surname><given-names>Matthias</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>lange-m@anit.uni-muenster.de</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Br&#x000f6;king</surname><given-names>Katrin</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>k.broeking@freenet.de</email></contrib><contrib id="A9" contrib-type="author"><name><surname>L&#x000fc;cke</surname><given-names>Martin</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>martin.lucke@uni-muenster.de</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Traber</surname><given-names>Daniel L</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>dltraber@utmb.edu</email></contrib><contrib id="A11" corresp="yes" contrib-type="author"><name><surname>Westphal</surname><given-names>Martin</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>martin.westphal@gmx.net</email></contrib></contrib-group><aff id="I1"><label>1</label>Department of Anesthesiology and Intensive Care, University Hospital of Muenster, Albert-Schweitzer-Strasse 33, 48149 Muenster, Germany</aff><aff id="I2"><label>2</label>Department of Anesthesiology and Intensive Care, University of Rome 'La Sapienza', 00185 Rome, Italy</aff><aff id="I3"><label>3</label>Central Animal Research Facility, University Hospital of Muenster, Muenster, Germany, University Hospital of Muenster, Albert-Schweitzer-Strasse 33, 48149 Muenster, Germany</aff><aff id="I4"><label>4</label>Investigational Intensive Care Unit, University of Texas Medical Branch, 301 University Boulevard, Galveston TX 77555, USA</aff><pub-date pub-type="ppub"><year>2006</year></pub-date><pub-date pub-type="epub"><day>10</day><month>10</month><year>2006</year></pub-date><volume>10</volume><issue>5</issue><fpage>R144</fpage><lpage>R144</lpage><ext-link ext-link-type="uri" xlink:href="http://ccforum.com/content/10/5/R144"/><history><date date-type="received"><day>21</day><month>7</month><year>2006</year></date><date date-type="rev-request"><day>23</day><month>8</month><year>2006</year></date><date date-type="rev-recd"><day>5</day><month>10</month><year>2006</year></date><date date-type="accepted"><day>10</day><month>10</month><year>2006</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2006 Ertmer et al.; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2006</copyright-year><copyright-holder>Ertmer et al.; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an open access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
