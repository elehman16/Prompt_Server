ddress><institution>University of Antwerp</institution><institution>Global Health Institute</institution><addr-line>Doornstraat 331</addr-line><addr-line>Antwerp,</addr-line><country>Belgium</country><phone>32 486 92 01 49</phone><fax>32 32 651 836</fax><email>robert.colebunders@uantwerpen.be</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-1919-1340</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Mandro</surname><given-names>Michel</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4425-0517</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Mukendi</surname><given-names>Deby</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9229-5034</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Dolo</surname><given-names>Housseini</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4234-3488</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Suykerbuyk</surname><given-names>Patrick</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2294-4665</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Van Oijen</surname><given-names>Marieke</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff1">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-7473-8601</contrib-id></contrib></contrib-group><aff id="aff1">
