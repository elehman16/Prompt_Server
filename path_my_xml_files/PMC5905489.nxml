<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Pain Res</journal-id><journal-id journal-id-type="iso-abbrev">J Pain Res</journal-id><journal-id journal-id-type="publisher-id">Journal of Pain Research</journal-id><journal-title-group><journal-title>Journal of Pain Research</journal-title></journal-title-group><issn pub-type="epub">1178-7090</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29695926</article-id><article-id pub-id-type="pmc">5905489</article-id><article-id pub-id-type="doi">10.2147/JPR.S152475</article-id><article-id pub-id-type="publisher-id">jpr-11-743</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Pre-emptive multimodal analgesia with tramadol and ketamine&#x02013;lidocaine infusion for suppression of central sensitization in a dog model of ovariohysterectomy</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Kaka</surname><given-names>Ubedullah</given-names></name><xref ref-type="aff" rid="af1-jpr-11-743">1</xref><xref ref-type="aff" rid="af2-jpr-11-743">2</xref></contrib><contrib contrib-type="author"><name><surname>Rahman</surname><given-names>Nor-Alimah</given-names></name><xref ref-type="aff" rid="af1-jpr-11-743">1</xref></contrib><contrib contrib-type="author"><name><surname>Abubakar</surname><given-names>Adamu Abdul</given-names></name><xref ref-type="aff" rid="af1-jpr-11-743">1</xref><xref ref-type="aff" rid="af3-jpr-11-743">3</xref></contrib><contrib contrib-type="author"><name><surname>Goh</surname><given-names>Yong Meng</given-names></name><xref ref-type="aff" rid="af2-jpr-11-743">2</xref><xref ref-type="aff" rid="af4-jpr-11-743">4</xref></contrib><contrib contrib-type="author"><name><surname>Fakurazi</surname><given-names>Sharida</given-names></name><xref ref-type="aff" rid="af5-jpr-11-743">5</xref><xref ref-type="aff" rid="af6-jpr-11-743">6</xref></contrib><contrib contrib-type="author"><name><surname>Omar</surname><given-names>Mohamed Ariff</given-names></name><xref ref-type="aff" rid="af4-jpr-11-743">4</xref></contrib><contrib contrib-type="author"><name><surname>Chen</surname><given-names>Hui Cheng</given-names></name><xref ref-type="aff" rid="af1-jpr-11-743">1</xref><xref ref-type="corresp" rid="c1-jpr-11-743"/></contrib></contrib-group><aff id="af1-jpr-11-743">
