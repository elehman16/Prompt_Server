<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Diabetes Investig</journal-id><journal-id journal-id-type="iso-abbrev">J Diabetes Investig</journal-id><journal-id journal-id-type="doi">10.1111/(ISSN)2040-1124</journal-id><journal-id journal-id-type="publisher-id">JDI</journal-id><journal-title-group><journal-title>Journal of Diabetes Investigation</journal-title></journal-title-group><issn pub-type="ppub">2040-1116</issn><issn pub-type="epub">2040-1124</issn><publisher><publisher-name>Wiley-Blackwell</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">24843631</article-id><article-id pub-id-type="pmc">4019288</article-id><article-id pub-id-type="doi">10.1111/j.2040-1124.2012.00238.x</article-id><article-id pub-id-type="publisher-id">JDI238</article-id><article-categories><subj-group subj-group-type="overline"><subject>Original Article</subject></subj-group><subj-group subj-group-type="heading"><subject>Articles</subject><subj-group subj-group-type="heading"><subject>Clinical Science and Care</subject></subj-group></subj-group></article-categories><title-group><article-title>Efficacy and safety of exenatide once&#x02010;weekly vs exenatide twice&#x02010;daily in <styled-content style="fixed-case">A</styled-content>sian patients with type 2 diabetes mellitus</article-title><alt-title alt-title-type="left-running-head">Ji <italic>et&#x000a0;al</italic>.</alt-title></title-group><contrib-group><contrib id="jdi238-cr-0001" contrib-type="author"><name><surname>Ji</surname><given-names>Linong</given-names></name><xref ref-type="correspondence-address" rid="correspondenceTo">*</xref><xref ref-type="aff" rid="jdi238-aff-0001"><sup>1</sup></xref></contrib><contrib id="jdi238-cr-0002" contrib-type="author"><name><surname>Onishi</surname><given-names>Yukiko</given-names></name><xref ref-type="aff" rid="jdi238-aff-0002"><sup>2</sup></xref></contrib><contrib id="jdi238-cr-0003" contrib-type="author"><name><surname>Ahn</surname><given-names>Chul Woo</given-names></name><xref ref-type="aff" rid="jdi238-aff-0003"><sup>3</sup></xref></contrib><contrib id="jdi238-cr-0004" contrib-type="author"><name><surname>Agarwal</surname><given-names>Pankaj</given-names></name><xref ref-type="aff" rid="jdi238-aff-0004"><sup>4</sup></xref></contrib><contrib id="jdi238-cr-0005" contrib-type="author"><name><surname>Chou</surname><given-names>Chien&#x02010;Wen</given-names></name><xref ref-type="aff" rid="jdi238-aff-0005"><sup>5</sup></xref></contrib><contrib id="jdi238-cr-0006" contrib-type="author"><name><surname>Haber</surname><given-names>Harry</given-names></name><xref ref-type="aff" rid="jdi238-aff-0006"><sup>6</sup></xref></contrib><contrib id="jdi238-cr-0007" contrib-type="author"><name><surname>Guerrettaz</surname><given-names>Kelly</given-names></name><xref ref-type="aff" rid="jdi238-aff-0007"><sup>7</sup></xref></contrib><contrib id="jdi238-cr-0008" contrib-type="author"><name><surname>Boardman</surname><given-names>Marilyn K</given-names></name><xref ref-type="aff" rid="jdi238-aff-0008"><sup>8</sup></xref></contrib></contrib-group><aff id="jdi238-aff-0001"><label><sup>1</sup></label><institution>Peking University People's Hospital</institution><named-content content-type="city">Beijing</named-content><country>China</country></aff><aff id="jdi238-aff-0002"><label><sup>2</sup></label><institution>The Institute for Adult Diseases</institution><institution>Asahi Life Foundation</institution><named-content content-type="city">Tokyo</named-content><country>Japan</country></aff><aff id="jdi238-aff-0003"><label><sup>3</sup></label><institution>Gangnam Severance Hospital</institution><named-content content-type="city">Seoul</named-content><country>Korea</country></aff><aff id="jdi238-aff-0004"><label><sup>4</sup></label><institution>Hormone Care &#x00026; Research Center</institution><named-content content-type="city">Ghaziabad</named-content><country>India</country></aff><aff id="jdi238-aff-0005"><label><sup>5</sup></label><institution>Chi&#x02010;Mei Medical Center</institution><named-content content-type="city">Tainan</named-content><country>Taiwan</country></aff><aff id="jdi238-aff-0006"><label><sup>6</sup></label><institution>PharmaNet/i3</institution><named-content content-type="city">Ann Arbor</named-content><named-content content-type="country-part">Michigan</named-content><country country="US">USA</country></aff><aff id="jdi238-aff-0007"><label><sup>7</sup></label><institution>PharmaNet/i3</institution><named-content content-type="city">Blue Bell</named-content><named-content content-type="country-part">Pennsylvania</named-content><country country="US">USA</country></aff><aff id="jdi238-aff-0008"><label><sup>8</sup></label><institution>Eli Lilly and Company</institution><named-content content-type="city">Indianapolis</named-content><named-content content-type="country-part">Indiana</named-content><country country="US">USA</country></aff><author-notes><corresp id="correspondenceTo"><label>*</label>
