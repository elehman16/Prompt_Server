tr><td align="left">Alpha-linoleic (ALA)</td><td align="center">C18:3 n-3</td><td align="center">0.8</td><td align="center">0.2</td><td align="center">28.0</td><td align="center">1.2</td><td align="center">0.06</td><td align="center">1.90</td><td align="center">0.23</td></tr><tr><td align="left">Gondoic</td><td align="center">C20:1 n-9</td><td align="center">0.2</td><td align="center">5.6</td><td align="center">0</td><td align="center">0.5</td><td align="center"/><td align="center"/><td align="center"/></tr><tr><td align="left">Stearidonic (SDA)</td><td align="center">C18:4 n-3</td><td align="center">0.1</td><td align="center">0</td><td align="center">12.2</td><td align="center">4.5</td><td align="center">&#x0003c;0.01</td><td align="center">0.83</td><td align="center">0.86</td></tr><tr><td align="left">Dihommo-&#x003b3;-linolenic (DGLA)</td><td align="center">C20:3 n-6</td><td align="center">0</td><td align="center">0</td><td align="center">0</td><td align="center">0.1</td><td align="center"/><td align="center"/><td align="center"/></tr><tr><td align="left">Behenic</td><td align="center">C22:0</td><td align="center">0.1</td><td align="center">0.3</td><td align="center">0</td><td align="center">1.3</td><td align="center"/><td align="center"/><td align="center"/></tr><tr><td align="left">Arachidonic (ARA)</td><td align="center">C20:4 n-6</td><td align="center">0.2</td><td align="center">0.1</td><td align="center">0</td><td align="center">0</td><td align="center"/><td align="center"/><td align="center"/></tr><tr><td align="left">Erucic</td><td align="center">C22:1 n-9</td><td align="center">0</td><td align="center">4.3</td><td align="center">0.1</td><td align="center">0</td><td align="center"/><td align="center"/><td align="center"/></tr><tr><td align="left">Eicosopentaenoic (EPA)</td><td align="center">C20:5 n-3</td><td align="center">0.1</td><td align="center">0</td><td align="center">0</td><td align="center">18.0</td><td align="center">&#x0003c;0.01</td><td align="