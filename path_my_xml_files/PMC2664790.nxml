<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Surg</journal-id><journal-title>BMC Surgery</journal-title><issn pub-type="epub">1471-2482</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19284647</article-id><article-id pub-id-type="pmc">2664790</article-id><article-id pub-id-type="publisher-id">1471-2482-9-4</article-id><article-id pub-id-type="doi">10.1186/1471-2482-9-4</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Transanal endoscopic microsurgery versus endoscopic mucosal resection for large rectal adenomas (TREND-study)</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>van den Broek</surname><given-names>Frank JC</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>f.j.vandenbroek@amc.uva.nl</email></contrib><contrib id="A2" contrib-type="author"><name><surname>de Graaf</surname><given-names>Eelco JR</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>edgraaf@ysl.nl</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Dijkgraaf</surname><given-names>Marcel GW</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>m.g.dijkgraaf@amc.uva.nl</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Reitsma</surname><given-names>Johannes B</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>j.reitsma@amc.uva.nl</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Haringsma</surname><given-names>Jelle</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>j.haringsma@erasmusmc.nl</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Timmer</surname><given-names>Robin</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>r.timmer@antonius.net</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Weusten</surname><given-names>Bas LAM</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>b.weusten@antonius.net</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Gerhards</surname><given-names>Michael F</given-names></name><xref ref-type="aff" rid="I6">6</xref><email>m.f.gerhards@olvg.nl</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Consten</surname><given-names>Esther CJ</given-names></name><xref ref-type="aff" rid="I7">7</xref><email>ecj.consten@meandermc.nl</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Schwartz</surname><given-names>Matthijs P</given-names></name><xref ref-type="aff" rid="I8">8</xref><email>mp.schwartz@meandermc.nl</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Boom</surname><given-names>Maarten J</given-names></name><xref ref-type="aff" rid="I9">9</xref><email>mboom@flevoziekenhuis.nl</email></contrib><contrib id="A12" contrib-type="author"><name><surname>Derksen</surname><given-names>Erik J</given-names></name><xref ref-type="aff" rid="I10">10</xref><email>ej.derksen@slz.nl</email></contrib><contrib id="A13" contrib-type="author"><name><surname>Bijnen</surname><given-names>A Bart</given-names></name><xref ref-type="aff" rid="I11">11</xref><email>a.b.bijnen@mca.nl</email></contrib><contrib id="A14" contrib-type="author"><name><surname>Davids</surname><given-names>Paul HP</given-names></name><xref ref-type="aff" rid="I12">12</xref><email>pdavids@diakhuis.nl</email></contrib><contrib id="A15" contrib-type="author"><name><surname>Hoff</surname><given-names>Christiaan</given-names></name><xref ref-type="aff" rid="I13">13</xref><email>c.hoff@znb.nl</email></contrib><contrib id="A16" contrib-type="author"><name><surname>van Dullemen</surname><given-names>Hendrik M</given-names></name><xref ref-type="aff" rid="I14">14</xref><email>h.m.dullemen@int.umcg.nl</email></contrib><contrib id="A17" contrib-type="author"><name><surname>Heine</surname><given-names>G Dimitri N</given-names></name><xref ref-type="aff" rid="I15">15</xref><email>g.d.n.heine@mca.nl</email></contrib><contrib id="A18" contrib-type="author"><name><surname>van der Linde</surname><given-names>Klaas</given-names></name><xref ref-type="aff" rid="I16">16</xref><email>k.v.d.linde@znb.nl</email></contrib><contrib id="A19" contrib-type="author"><name><surname>Jansen</surname><given-names>Jeroen M</given-names></name><xref ref-type="aff" rid="I17">17</xref><email>j.m.jansen@olvg.nl</email></contrib><contrib id="A20" contrib-type="author"><name><surname>Mallant-Hent</surname><given-names>Rosalie CH</given-names></name><xref ref-type="aff" rid="I18">18</xref><email>rmallant@flevoziekenhuis.nl</email></contrib><contrib id="A21" contrib-type="author"><name><surname>Breumelhof</surname><given-names>Ronald</given-names></name><xref ref-type="aff" rid="I19">19</xref><email>rbreumelhof@diakhuis.nl</email></contrib><contrib id="A22" contrib-type="author"><name><surname>Geldof</surname><given-names>Han</given-names></name><xref ref-type="aff" rid="I20">20</xref><email>han@hgeldof.nl</email></contrib><contrib id="A23" contrib-type="author"><name><surname>Hardwick</surname><given-names>James CH</given-names></name><xref ref-type="aff" rid="I21">21</xref><email>j.c.h.hardwick@lumc.nl</email></contrib><contrib id="A24" contrib-type="author"><name><surname>Doornebosch</surname><given-names>Pascal G</given-names></name><xref ref-type="aff" rid="I22">22</xref><email>p.doornebosch@ysl.nl</email></contrib><contrib id="A25" contrib-type="author"><name><surname>Depla</surname><given-names>Annekatrien CTM</given-names></name><xref ref-type="aff" rid="I23">23</xref><email>actm.depla@slz.nl</email></contrib><contrib id="A26" contrib-type="author"><name><surname>Ernst</surname><given-names>Miranda F</given-names></name><xre