b-type="author"><name><surname>Ivanov</surname><given-names>Stefan</given-names></name><xref ref-type="aff" rid="af2-dddt-12-1093">2</xref></contrib><contrib contrib-type="author"><name><surname>Uddin</surname><given-names>Mohib</given-names></name><xref ref-type="aff" rid="af2-dddt-12-1093">2</xref><xref ref-type="aff" rid="af3-dddt-12-1093">3</xref></contrib></contrib-group><aff id="af1-dddt-12-1093">
