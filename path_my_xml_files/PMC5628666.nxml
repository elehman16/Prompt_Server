<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Int J Chron Obstruct Pulmon Dis</journal-id><journal-id journal-id-type="iso-abbrev">Int J Chron Obstruct Pulmon Dis</journal-id><journal-id journal-id-type="publisher-id">International Journal of COPD</journal-id><journal-title-group><journal-title>International Journal of Chronic Obstructive Pulmonary Disease</journal-title></journal-title-group><issn pub-type="ppub">1176-9106</issn><issn pub-type="epub">1178-2005</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29033560</article-id><article-id pub-id-type="pmc">5628666</article-id><article-id pub-id-type="doi">10.2147/COPD.S141974</article-id><article-id pub-id-type="publisher-id">copd-12-2795</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Proactive palliative care for patients with COPD (PROLONG): a pragmatic cluster controlled trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Duenk</surname><given-names>RG</given-names></name><xref ref-type="aff" rid="af1-copd-12-2795">1</xref><xref ref-type="corresp" rid="c1-copd-12-2795"/></contrib><contrib contrib-type="author"><name><surname>Verhagen</surname><given-names>C</given-names></name><xref ref-type="aff" rid="af1-copd-12-2795">1</xref></contrib><contrib contrib-type="author"><name><surname>Bronkhorst</surname><given-names>EM</given-names></name><xref ref-type="aff" rid="af2-copd-12-2795">2</xref></contrib><contrib contrib-type="author"><name><surname>van Mierlo</surname><given-names>PJWB</given-names></name><xref ref-type="aff" rid="af3-copd-12-2795">3</xref><xref ref-type="aff" rid="af4-copd-12-2795">4</xref></contrib><contrib contrib-type="author"><name><surname>Broeders</surname><given-names>MEAC</given-names></name><xref ref-type="aff" rid="af5-copd-12-2795">5</xref></contrib><contrib contrib-type="author"><name><surname>Collard</surname><given-names>SM</given-names></name><xref ref-type="aff" rid="af6-copd-12-2795">6</xref></contrib><contrib contrib-type="author"><name><surname>Dekhuijzen</surname><given-names>PNR</given-names></name><xref ref-type="aff" rid="af7-copd-12-2795">7</xref></contrib><contrib contrib-type="author"><name><surname>Vissers</surname><given-names>KCP</given-names></name><xref ref-type="aff" rid="af1-copd-12-2795">1</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Heijdra</surname><given-names>Y</given-names></name><xref ref-type="aff" rid="af7-copd-12-2795">7</xref><xref ref-type="author-notes" rid="fn1-copd-12-2795">*</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Engels</surname><given-names>Y</given-names></name><xref ref-type="aff" rid="af1-copd-12-2795">1</xref><xref ref-type="author-notes" rid="fn1-copd-12-2795">*</xref></contrib></contrib-group><aff id="af1-copd-12-2795">
