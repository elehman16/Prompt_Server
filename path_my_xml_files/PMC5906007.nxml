<disp-formula id="pone.0195867.e008"><alternatives><graphic xlink:href="pone.0195867.e008.jpg" id="pone.0195867.e008g" mimetype="image" position="anchor" orientation="portrait"/><mml:math id="M8"><mml:mrow><mml:mtext>Arterial</mml:mtext><mml:mspace width="2pt"/><mml:mtext>oxygen</mml:mtext><mml:mspace width="2pt"/><mml:mtext>content</mml:mtext><mml:mspace width="2pt"/><mml:mrow><mml:mo>(</mml:mo><mml:mrow><mml:msub><mml:mrow><mml:mtext>CaO</mml:mtext></mml:mrow><mml:mtext>2</mml:mtext></mml:msub><mml:msup><mml:mrow><mml:mtext>-mL</mml:mtext><mml:mspace width="2pt"/><mml:mtext>dL</mml:mtext></mml:mrow><mml:mrow><mml:mtext>-1</mml:mtext></mml:mrow></mml:msup></mml:mrow><mml:mo>)</mml:mo></mml:mrow><mml:mo>=</mml:mo><mml:mrow><mml:mo>(</mml:mo><mml:mrow><mml:mn>1.39</mml:mn><mml:mspace width="2pt"/><mml:mo>&#x000d7;</mml:mo><mml:mspace width="2pt"/><mml:msub><mml:mrow><mml:mtext>Hba</mml:mtext><mml:mspace width="2pt"/><mml:mo>&#x000d7;</mml:mo><mml:mspace width="2pt"/><mml:mtext>SaO</mml:mtext></mml:mrow><mml:mtext>2</mml:mtext></mml:msub></mml:mrow><mml:mo>)</mml:mo></mml:mrow><mml:mo>+</mml:mo><mml:mrow><mml:mo>(</mml:mo><mml:mrow><mml:mtext>0</mml:mtext><mml:msub><mml:mrow><mml:mn>.003</mml:mn><mml:mspace width="2pt"/><mml:mo>&#x000d7;</mml:mo><mml:mspace width="2pt"/><mml:mtext>PaO</mml:mtext></mml:mrow><mml:mtext>2</mml:mtext></mml:msub></mml:mrow><mml:mo>)</mml:mo></mml:mrow><mml:mo>,</mml:mo></mml:mrow></mml:math></alternatives></disp-formula>
