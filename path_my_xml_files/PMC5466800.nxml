<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Cancer</journal-id><journal-id journal-id-type="iso-abbrev">BMC Cancer</journal-id><journal-title-group><journal-title>BMC Cancer</journal-title></journal-title-group><issn pub-type="epub">1471-2407</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28599628</article-id><article-id pub-id-type="pmc">5466800</article-id><article-id pub-id-type="publisher-id">3360</article-id><article-id pub-id-type="doi">10.1186/s12885-017-3360-z</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>TRIBE-2: a phase III, randomized, open-label, strategy trial in unresectable metastatic colorectal cancer patients by the GONO group</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Cremolini</surname><given-names>Chiara</given-names></name><address><email>chiaracremolini@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Marmorino</surname><given-names>Federica</given-names></name><address><email>federica.marmorino@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Loupakis</surname><given-names>Fotios</given-names></name><address><email>fotiosloupakis@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Masi</surname><given-names>Gianluca</given-names></name><address><email>gl.masi@tin.it</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Antoniotti</surname><given-names>Carlotta</given-names></name><address><email>carlottantoniotti@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Salvatore</surname><given-names>Lisa</given-names></name><address><email>salvatore.lisa82@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Schirripa</surname><given-names>Marta</given-names></name><address><email>martaschirripa@gmail.com</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Boni</surname><given-names>Luca</given-names></name><address><email>luca.boni.ge@gmail.com</email></address><xref ref-type="aff" rid="Aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Zagonel</surname><given-names>Vittorina</given-names></name><address><email>vittorina.zagonel@ioveneto.it</email></address><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Lonardi</surname><given-names>Sara</given-names></name><address><email>sara.lonardi@ioveneto.it</email></address><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Aprile</surname><given-names>Giuseppe</given-names></name><address><email>aprile.giuseppe@aoud.sanita.fvg.it</email></address><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Tamburini</surname><given-names>Emiliano</given-names></name><address><email>emilianotamburini@yahoo.it</email></address><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Ricci</surname><given-names>Vincenzo</given-names></name><address><email>ricci.v@ospedale.cuneo.it</email></address><xref ref-type="aff" rid="Aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Ronzoni</surname><given-na