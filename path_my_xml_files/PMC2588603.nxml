<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Nutr Metab (Lond)</journal-id><journal-title>Nutrition &#x00026; Metabolism</journal-title><issn pub-type="epub">1743-7075</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18983673</article-id><article-id pub-id-type="pmc">2588603</article-id><article-id pub-id-type="publisher-id">1743-7075-5-29</article-id><article-id pub-id-type="doi">10.1186/1743-7075-5-29</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Enhancement of a modified Mediterranean-style, low glycemic load diet with specific phytochemicals improves cardiometabolic risk factors in subjects with metabolic syndrome and hypercholesterolemia in a randomized trial</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Lerman</surname><given-names>Robert H</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>BobLerman@metagenics.com</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Minich</surname><given-names>Deanna M</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>DeannaMinich@metagenics.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Darland</surname><given-names>Gary</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>GaryDarland@metagenics.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Lamb</surname><given-names>Joseph J</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>JosephLamb@metagenics.com</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Schiltz</surname><given-names>Barbara</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>nutrbarb@gmail.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Babish</surname><given-names>John G</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jgb7@cornell.edu</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Bland</surname><given-names>Jeffrey S</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>JeffBland@metagenics.com</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Tripp</surname><given-names>Matthew L</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>MatthewTripp@metagenics.com</email></contrib></contrib-group><aff id="I1"><label>1</label>Functional Medicine Research Center, MetaProteomics, LLC, 9770 44th Ave. NW, Ste 100, Gig Harbor, WA 98332, USA</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>4</day><month>11</month><year>2008</year></pub-date><volume>5</volume><fpage>29</fpage><lpage>29</lpage><ext-link ext-link-type="uri" xlink:href="http://www.nutritionandmetabolism.com/content/5/1/29"/><history><date date-type="received"><day>9</day><month>7</month><year>2008</year></date><date date-type="accepted"><day>4</day><month>11</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Lerman et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Lerman et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
