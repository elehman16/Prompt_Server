</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Asazawa</surname><given-names>Kyoko</given-names><suffix> PhD</suffix></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Kato</surname><given-names>Yoshihiro</given-names><suffix> BS</suffix></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Yamaguchi</surname><given-names>Atsuko</given-names><suffix> MS</suffix></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Inoue</surname><given-names>Asako</given-names><suffix> BS</suffix></name><xref ref-type="aff" rid="aff2">2</xref></contrib></contrib-group><aff id="aff1">
