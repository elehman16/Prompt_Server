<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Nutr Metab (Lond)</journal-id><journal-title>Nutrition &#x00026; Metabolism</journal-title><issn pub-type="epub">1743-7075</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18289377</article-id><article-id pub-id-type="pmc">2265719</article-id><article-id pub-id-type="publisher-id">1743-7075-5-6</article-id><article-id pub-id-type="doi">10.1186/1743-7075-5-6</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Eggs modulate the inflammatory response to carbohydrate restricted diets in overweight men</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Ratliff</surname><given-names>Joseph C</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jose516@hotmail.com</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Mutungi</surname><given-names>Gisella</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>gmgm218@hotmail.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Puglisi</surname><given-names>Michael J</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>michael.puglisi@uconn.edu</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Volek</surname><given-names>Jeff S</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>jeff.volek@uconn.edu</email></contrib><contrib id="A5" corresp="yes" contrib-type="author"><name><surname>Fernandez</surname><given-names>Maria Luz</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>maria-luz.fernandez@uconn.edu</email></contrib></contrib-group><aff id="I1"><label>1</label>Department of Nutritional Sciences, University of Connecticut, Storrs, CT, USA</aff><aff id="I2"><label>2</label>Department of Kinesiology, University of Connecticut, Storrs, CT, USA</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>20</day><month>2</month><year>2008</year></pub-date><volume>5</volume><fpage>6</fpage><lpage>6</lpage><ext-link ext-link-type="uri" xlink:href="http://www.nutritionandmetabolism.com/content/5/1/6"/><history><date date-type="received"><day>2</day><month>10</month><year>2007</year></date><date date-type="accepted"><day>20</day><month>2</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Ratliff et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Ratliff et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
