<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Pain Res</journal-id><journal-id journal-id-type="iso-abbrev">J Pain Res</journal-id><journal-id journal-id-type="publisher-id">Journal of Pain Research</journal-id><journal-title-group><journal-title>Journal of Pain Research</journal-title></journal-title-group><issn pub-type="epub">1178-7090</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29184436</article-id><article-id pub-id-type="pmc">5673045</article-id><article-id pub-id-type="doi">10.2147/JPR.S139856</article-id><article-id pub-id-type="publisher-id">jpr-10-2515</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Continuous wound infusion with chloroprocaine in a pig model of surgical lesion: drug absorption and effects on inflammatory response</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Allegri</surname><given-names>Massimo</given-names></name><xref ref-type="aff" rid="af1-jpr-10-2515">1</xref><xref ref-type="aff" rid="af2-jpr-10-2515">2</xref></contrib><contrib contrib-type="author"><name><surname>Bugada</surname><given-names>Dario</given-names></name><xref ref-type="aff" rid="af1-jpr-10-2515">1</xref><xref ref-type="aff" rid="af2-jpr-10-2515">2</xref><xref ref-type="aff" rid="af3-jpr-10-2515">3</xref></contrib><contrib contrib-type="author"><name><surname>De Gregori</surname><given-names>Manuela</given-names></name><xref ref-type="aff" rid="af2-jpr-10-2515">2</xref><xref ref-type="aff" rid="af4-jpr-10-2515">4</xref></contrib><contrib contrib-type="author"><name><surname>Avanzini</surname><given-names>Maria A</given-names></name><xref ref-type="aff" rid="af5-jpr-10-2515">5</xref></contrib><contrib contrib-type="author"><name><surname>De Silvestri</surname><given-names>Annalisa</given-names></name><xref ref-type="aff" rid="af6-jpr-10-2515">6</xref></contrib><contrib contrib-type="author"><name><surname>Petroni</surname><given-names>Anna</given-names></name><xref ref-type="aff" rid="af7-jpr-10-2515">7</xref></contrib><contrib contrib-type="author"><name><surname>Sala</surname><given-names>Angelo</given-names></name><xref ref-type="aff" rid="af7-jpr-10-2515">7</xref><xref ref-type="aff" rid="af8-jpr-10-2515">8</xref></contrib><contrib contrib-type="author"><name><surname>Filisetti</surname><given-names>Claudia</given-names></name><xref ref-type="aff" rid="af9-jpr-10-2515">9</xref><xref ref-type="aff" rid="af10-jpr-10-2515">10</xref><xref ref-type="aff" rid="af11-jpr-10-2515">11</xref></contrib><contrib contrib-type="author"><name><surname>Icaro Cornaglia</surname><given-names>Antonia</given-names></name><xref ref-type="aff" rid="af12-jpr-10-2515">12</xref></contrib><contrib contrib-type="author"><name><surname>Cobianchi</surname><given-names>Lorenzo</given-names></name><xref ref-type="aff" rid="af13-jpr-10-2515">13</xref><xref ref-type="aff" rid="af14-jpr-10-2515">14</xref><xref ref-type="corresp" rid="c1-jpr-10-2515"/></contrib></contrib-group><aff id="af1-jpr-10-2515">
