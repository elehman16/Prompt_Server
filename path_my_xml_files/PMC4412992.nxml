/name><xref ref-type="aff" rid="af3-ajas-28-5-632">2</xref></contrib><contrib contrib-type="author"><name><surname>Oh</surname><given-names>Young Kyoon</given-names></name><xref ref-type="aff" rid="af3-ajas-28-5-632">2</xref></contrib><contrib contrib-type="author"><name><surname>Lee</surname><given-names>Sang Suk</given-names></name><xref ref-type="aff" rid="af4-ajas-28-5-632">3</xref></contrib><contrib contrib-type="author"><name><surname>Kim</surname><given-names>Jeong Hoon</given-names></name><xref ref-type="aff" rid="af5-ajas-28-5-632">4</xref></contrib><contrib contrib-type="author"><name><surname>Park</surname><given-names>Keun Kyu</given-names></name><xref ref-type="aff" rid="af2-ajas-28-5-632">1</xref><xref rid="c1-ajas-28-5-632" ref-type="corresp">*</xref></contrib></contrib-group><aff id="af1-ajas-28-5-632">
