<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">ESMO Open</journal-id><journal-id journal-id-type="iso-abbrev">ESMO Open</journal-id><journal-id journal-id-type="hwp">esmoopen</journal-id><journal-id journal-id-type="publisher-id">esmoopen</journal-id><journal-title-group><journal-title>ESMO Open</journal-title></journal-title-group><issn pub-type="epub">2059-7029</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29104762</article-id><article-id pub-id-type="pmc">5663255</article-id><article-id pub-id-type="publisher-id">esmoopen-2017-000238</article-id><article-id pub-id-type="doi">10.1136/esmoopen-2017-000238</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group></article-categories><title-group><article-title>Safety and efficacy of intra-arterial hepatic chemotherapy with doxorubicin-loaded nanoparticles in hepatocellular carcinoma</article-title></title-group><contrib-group><contrib id="author-56488773" contrib-type="author"><name><surname>Merle</surname><given-names>Philippe</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-56488863" contrib-type="author"><name><surname>Camus</surname><given-names>Philippe</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-56488902" contrib-type="author"><name><surname>Abergel</surname><given-names>Armand</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-56488981" contrib-type="author"><name><surname>Pageaux</surname><given-names>Georges Philippe</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-56489001" contrib-type="author"><name><surname>Masliah</surname><given-names>Claude</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-56489034" contrib-type="author"><name><surname>Bronowicki</surname><given-names>Jean Pierre</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib id="author-56489062" contrib-type="author"><name><surname>Zarski</surname><given-names>Jean Pierre</given-names></name><xref ref-type="aff" rid="aff7">7</xref></contrib><contrib id="author-56489085" contrib-type="author"><name><surname>Pelletier</surname><given-names>Gilles</given-names></name><xref ref-type="aff" rid="aff8">8</xref></contrib><contrib id="author-56489134" contrib-type="author"><name><surname>Bouattour</surname><given-names>Mohamed</given-names></name><xref ref-type="aff" rid="aff9">9</xref></contrib><contrib id="author-56489151" contrib-type="author"><name><surname>Farloux</surname><given-names>Laetitia</given-names></name><xref ref-type="aff" rid="aff10">10</xref></contrib><contrib id="author-56489166" contrib-type="author"><name><surname>Dorval</surname><given-names>Etienne</given-names></name><xref ref-type="aff" rid="aff11">11</xref></contrib><contrib id="author-56489185" contrib-type="author"><name><surname>verset</surname><given-names>Gilles</given-names></name><xref ref-type="aff" rid="aff12">12</xref></contrib><contrib id="author-56489213" contrib-type="author"><name><surname>Si-Ahmed</surname><given-names>Si-Nafa</given-names></name><xref ref-type="aff" rid="aff13">13</xref></contrib><contrib id="author-56489241" contrib-type="author"><name><surname>Doffoel</surname><given-names>Michel</given-names></name><xref ref-type="aff" rid="aff14">14</xref></contrib><contrib id="author-56489259" contrib-type="author"><name><surname>Couzigou</surname><given-names>Patrice</given-names></name><xref ref-type="aff" rid