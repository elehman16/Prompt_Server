<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28674125</article-id><article-id pub-id-type="pmc">5734217</article-id><article-id pub-id-type="publisher-id">bmjopen-2016-013295</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2016-013295</article-id><article-categories><subj-group subj-group-type="heading"><subject>Diabetes and Endocrinology</subject><subj-group><subject>Research</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1843</subject></subj-group></article-categories><title-group><article-title>The effect of guided self-determination on self-management in persons with type 1 diabetes mellitus and HbA<sub>1c</sub> &#x02265;64 mmol/mol: a group-based randomised controlled trial</article-title></title-group><contrib-group><contrib id="author-50159696" contrib-type="author"><name><surname>Mohn</surname><given-names>Jannike</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-44933356" contrib-type="author"><name><surname>Graue</surname><given-names>Marit</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-50168980" contrib-type="author"><name><surname>Assmus</surname><given-names>J&#x000f5;rg</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-49856182" contrib-type="author"><name><surname>Zoffmann</surname><given-names>Vibeke</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib id="author-50169040" contrib-type="author"><name><surname>Thordarson</surname><given-names>Hrafnkell</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-49856137" contrib-type="author"><name><surname>Peyrot</surname><given-names>Mark</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff7">7</xref></contrib><contrib id="author-49856103" contrib-type="author"><name><surname>Rokne</surname><given-names>Berit</given-names></name><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff8">8</xref></contrib></contrib-group><aff id="aff1">
