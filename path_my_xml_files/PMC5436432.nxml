<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Addict Sci Clin Pract</journal-id><journal-id journal-id-type="iso-abbrev">Addict Sci Clin Pract</journal-id><journal-title-group><journal-title>Addiction Science &#x00026; Clinical Practice</journal-title></journal-title-group><issn pub-type="ppub">1940-0632</issn><issn pub-type="epub">1940-0640</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28514963</article-id><article-id pub-id-type="pmc">5436432</article-id><article-id pub-id-type="publisher-id">80</article-id><article-id pub-id-type="doi">10.1186/s13722-017-0080-2</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Patient-centered primary care for adults at high risk for AUDs: the Choosing Healthier Drinking Options In primary CarE (CHOICE) trial</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Bradley</surname><given-names>Katharine A.</given-names></name><address><phone>206-287-2151</phone><email>Bradley.K@ghc.org</email></address><xref ref-type="aff" rid="Aff1">1</xref><xref ref-type="aff" rid="Aff3">3</xref><xref ref-type="aff" rid="Aff4">4</xref><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Ludman</surname><given-names>Evette Joy</given-names></name><address><email>Ludman.E@ghc.org</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Chavez</surname><given-names>Laura J.</given-names></name><address><email>Chavez.105@osu.edu</email></address><xref ref-type="aff" rid="Aff6">6</xref><xref ref-type="aff" rid="Aff7">7</xref></contrib><contrib contrib-type="author"><name><surname>Bobb</surname><given-names>Jennifer F.</given-names></name><address><email>Bobb.J@ghc.org</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Ruedebusch</surname><given-names>Susan J.</given-names></name><address><email>Susan.Ruedebusch@va.gov</email></address><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Achtmeyer</surname><given-names>Carol E.</given-names></name><address><email>Carol.Achtmeyer@va.org</email></address><xref ref-type="aff" rid="Aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Merrill</surname><given-names>Joseph O.</given-names></name><address><email>Joem@uw.edu</email></address><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Saxon</surname><given-names>Andrew J.</given-names></name><address><email>Andrew.Saxon@va.org</email></address><xref ref-type="aff" rid="Aff2">2</xref><xref ref-type="aff" rid="Aff8">8</xref></contrib><contrib contrib-type="author"><name><surname>Caldeiro</surname><given-names>Ryan M.</given-names></name><address><email>Caldeiro.R@ghc.org</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Greenberg</surname><given-names>Diane M.</given-names></name><address><email>Diane.Greenberg@va.org</email></address><xref ref-type="aff" rid="Aff2">2</xref><xref ref-type="aff" rid="Aff8">8</xref><xref ref-type="aff" rid="Aff9">9</xref></contrib><contrib contrib-type="author"><name><surname>Lee</surname><given-names>Amy K.</given-names></name><address><email>Lee.Amy@ghc.org</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Richards</surname><given-names>Julie E.</given-names></name><address><email>Richards.JE@ghc.org</email></address><xref ref-type="aff" rid="Aff1">1</x