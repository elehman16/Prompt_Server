<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Infect Drug Resist</journal-id><journal-id journal-id-type="iso-abbrev">Infect Drug Resist</journal-id><journal-id journal-id-type="publisher-id">Infection and Drug Resistance</journal-id><journal-title-group><journal-title>Infection and Drug Resistance</journal-title></journal-title-group><issn pub-type="epub">1178-6973</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29379306</article-id><article-id pub-id-type="pmc">5759846</article-id><article-id pub-id-type="doi">10.2147/IDR.S146961</article-id><article-id pub-id-type="publisher-id">idr-11-029</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Retrospective analysis of relationships among the dose regimen, trough concentration, efficacy, and safety of teicoplanin in Chinese patients with moderate&#x02013;severe Gram-positive infections</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes"><name><surname>Zhou</surname><given-names>Lijuan</given-names></name><xref ref-type="aff" rid="af1-idr-11-029">1</xref><xref ref-type="author-notes" rid="fn1-idr-11-029">*</xref></contrib><contrib contrib-type="author"><name><surname>Gao</surname><given-names>Yanqiu</given-names></name><xref ref-type="aff" rid="af2-idr-11-029">2</xref></contrib><contrib contrib-type="author"><name><surname>Cao</surname><given-names>Wei</given-names></name><xref ref-type="aff" rid="af1-idr-11-029">1</xref></contrib><contrib contrib-type="author"><name><surname>Liu</surname><given-names>Jia</given-names></name><xref ref-type="aff" rid="af1-idr-11-029">1</xref></contrib><contrib contrib-type="author"><name><surname>Guan</surname><given-names>Hongya</given-names></name><xref ref-type="aff" rid="af1-idr-11-029">1</xref></contrib><contrib contrib-type="author"><name><surname>Zhang</surname><given-names>Hua</given-names></name><xref ref-type="aff" rid="af2-idr-11-029">2</xref></contrib><contrib contrib-type="author"><name><surname>Shi</surname><given-names>Yun</given-names></name><xref ref-type="aff" rid="af3-idr-11-029">3</xref><xref ref-type="corresp" rid="c1-idr-11-029"/></contrib><contrib contrib-type="author"><name><surname>Lv</surname><given-names>Wenying</given-names></name><xref ref-type="aff" rid="af4-idr-11-029">4</xref></contrib><contrib contrib-type="author" equal-contrib="yes"><name><surname>Cheng</surname><given-names>Long</given-names></name><xref ref-type="aff" rid="af5-idr-11-029">5</xref><xref ref-type="author-notes" rid="fn1-idr-11-029">*</xref><xref ref-type="corresp" rid="c2-idr-11-029"/></contrib></contrib-group><aff id="af1-idr-11-029">
