<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Surg</journal-id><journal-title>BMC Surgery</journal-title><issn pub-type="epub">1471-2482</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18215253</article-id><article-id pub-id-type="pmc">2248566</article-id><article-id pub-id-type="publisher-id">1471-2482-8-2</article-id><article-id pub-id-type="doi">10.1186/1471-2482-8-2</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>PORTAL: Pilot study on the safety and tolerance of preoperative melatonin application in patients undergoing major liver resection: a double-blind randomized placebo-controlled trial</article-title></title-group><contrib-group><contrib id="A1" equal-contrib="yes" corresp="yes" contrib-type="author"><name><surname>Schemmer</surname><given-names>Peter</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>Peter.Schemmer@med.uni-heidelberg.de</email></contrib><contrib id="A2" equal-contrib="yes" contrib-type="author"><name><surname>Nickkholgh</surname><given-names>Arash</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>Arash.Nickkholgh@med.uni-heidelberg.de</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Schneider</surname><given-names>Heinz</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>hschneider@healthecon.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Sobirey</surname><given-names>Michael</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>Michael.Sobirey@nutri-fit.de</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Weigand</surname><given-names>Markus</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>Markus.Weigand@med.uni-heidelberg.de</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Koch</surname><given-names>Moritz</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>Moritz.Koch@med.uni-heidelberg.de</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Weitz</surname><given-names>J&#x000fc;rgen</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>Juergen.Weitz@med.uni-heidelberg.de</email></contrib><contrib id="A8" contrib-type="author"><name><surname>B&#x000fc;chler</surname><given-names>Markus W</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>Markus.Buechler@med.uni-heidelberg.de</email></contrib></contrib-group><aff id="I1"><label>1</label>Department of General Surgery, Ruprecht-Karls University, Heidelberg, Germany</aff><aff id="I2"><label>2</label>Department of Anesthesiology, Ruprecht-Karls University, Heidelberg, Germany</aff><aff id="I3"><label>3</label>HealthEcon AG, Basel, Switzerland</aff><aff id="I4"><label>4</label>Nutri~fit GmbH &#x00026; Co KG, M&#x000fc;hlen, Germany</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>23</day><month>1</month><year>2008</year></pub-date><volume>8</volume><fpage>2</fpage><lpage>2</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1471-2482/8/2"/><history><date date-type="received"><day>14</day><month>4</month><year>2007</year></date><date date-type="accepted"><day>23</day><month>1</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Schemmer et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Schemmer et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
