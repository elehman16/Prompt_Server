<article xmlns:ali="http://www.niso.org/schemas/ali/1.0/" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="protocol"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27311914</article-id><article-id pub-id-type="pmc">4916634</article-id><article-id pub-id-type="publisher-id">bmjopen-2016-011505</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2016-011505</article-id><article-categories><subj-group subj-group-type="heading"><subject>Anaesthesia</subject><subj-group><subject>Protocol</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1682</subject><subject>1709</subject><subject>1698</subject><subject>1713</subject></subj-group></article-categories><title-group><article-title>Protocol for the Electroencephalography Guidance of Anesthesia to Alleviate Geriatric Syndromes (ENGAGES) study: a pragmatic, randomised clinical trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Wildes</surname><given-names>T S</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Winter</surname><given-names>A C</given-names></name><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Maybrier</surname><given-names>H R</given-names></name><xref ref-type="aff" rid="af1"