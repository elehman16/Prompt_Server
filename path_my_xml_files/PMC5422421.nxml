"><name><surname>Kim</surname><given-names>Chan Gyoo</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Kim</surname><given-names>Soo Jin</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Rho</surname><given-names>Ji Yoon</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Kim</surname><given-names>Seok Ki</given-names></name><xref ref-type="aff" rid="aff1">2</xref></contrib><contrib contrib-type="author"><name><surname>Kook</surname><given-names>Myeong-Cherl</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Kim</surname><given-names>Young-Woo</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Ryu</surname><given-names>Keun Won</given-names></name><xref ref-type="corresp" rid="cor1">*</xref><xref ref-type="aff" rid="aff1">1</xref><email>docryu@ncc.re.kr</email></contrib></contrib-group><aff id="aff1">
