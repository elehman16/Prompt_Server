<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Ment Health</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Ment Health</journal-id><journal-id journal-id-type="publisher-id">JMH</journal-id><journal-title-group><journal-title>JMIR Mental Health</journal-title></journal-title-group><issn pub-type="epub">2368-7959</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29326098</article-id><article-id pub-id-type="pmc">5785683</article-id><article-id pub-id-type="publisher-id">v5i1e4</article-id><article-id pub-id-type="doi">10.2196/mental.9092</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Paper</subject></subj-group><subj-group subj-group-type="article-type"><subject>Original Paper</subject></subj-group></article-categories><title-group><article-title>Cognitive and Behavioral Skills Exercises Completed by Patients with Major Depression During Smartphone Cognitive Behavioral Therapy: Secondary Analysis of a Randomized Controlled Trial</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Bragazzi</surname><given-names>Nicola</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Aggarwal</surname><given-names>Shweta</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Furukawa</surname><given-names>Toshi A</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff01">1</xref><address><institution>Department of Psychiatry and Cognitive-Behavioral Medicine</institution><institution>Nagoya City University Graduate School of Medical Sciences</institution><addr-line>Kawasumi 1, Mizuho-cho, Mizuho-ku,</addr-line><addr-line>Nagoya, 467-8601</addr-line><country>Japan</country><phone>81 52 753 9491</phone><email>furukawa@kuhp.kyoto-u.ac.jp</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2159-3776</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Horikoshi</surname><given-names>Masaru</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff02">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9336-8813</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Fujita</surname><given-names>Hirokazu</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff03">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9556-6570</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Tsujino</surname><given-names>Naohisa</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff04">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-5507-8946</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Jinnin</surname><given-names>Ran</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff05">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2520-1285</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Kako</surname><given-names>Yuki</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff06">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-7841-5498</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Ogawa</surname><given-names>Sei</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff01">1</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-9772-4651</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>Sato</surname><given-names>Hirotoshi</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff07">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3418-6504</contrib-id></contrib><contrib id="contrib9" contrib-type="author"><name><surname>Kitagawa</surname><given-names>Nobuki</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff08">8</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7337-6058</contrib-id></contrib><contrib id="contrib10" contrib-type="author"><name><surname>Shinagawa</surname><given-names>Yoshihiro</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff09">9</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-1734-9711</contrib-id></contrib><contrib id="contrib11" contrib-type="author"><name><surname>Ikeda</surname><given-names>Yoshio</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff10">10</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-8900-4216</contrib-id></contrib><contrib id="contrib12" contrib-type="author"><name><surname>Imai</surname><given-names>Hissei</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff11">11</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3656-5188</contrib-id></contrib><contrib id="contrib13" contrib-type="author"><name><surname>Tajika</surname><given-names>Aran</given-names></name><degrees>MD, PhD</degrees><xref ref-type="aff" rid="aff11">11</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-3926-8867</contrib-id></contrib><contrib id="contrib14" contrib-type="author"><name><surname>Ogawa</surname><given-names>Yusuke</given-names></name><degrees>MPH, MD, PhD</degrees><xref ref-type="aff" rid="aff11">11</xref><contrib-id contrib-id-type="orcid">http://orci