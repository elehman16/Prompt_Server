<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Diabetes Investig</journal-id><journal-id journal-id-type="iso-abbrev">J Diabetes Investig</journal-id><journal-id journal-id-type="doi">10.1111/(ISSN)2040-1124</journal-id><journal-id journal-id-type="publisher-id">JDI</journal-id><journal-title-group><journal-title>Journal of Diabetes Investigation</journal-title></journal-title-group><issn pub-type="ppub">2040-1116</issn><issn pub-type="epub">2040-1124</issn><publisher><publisher-name>Wiley-Blackwell</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">24843649</article-id><article-id pub-id-type="pmc">4019272</article-id><article-id pub-id-type="doi">10.1111/jdi.12001</article-id><article-id pub-id-type="publisher-id">JDI12001</article-id><article-categories><subj-group subj-group-type="overline"><subject>Original Article</subject></subj-group><subj-group subj-group-type="heading"><subject>Articles</subject><subj-group subj-group-type="heading"><subject>Clinical Science and Care</subject></subj-group></subj-group></article-categories><title-group><article-title>Addition of sitagliptin to ongoing metformin monotherapy improves glycemic control in <styled-content style="fixed-case">J</styled-content>apanese patients with type 2 diabetes over 52&#x000a0;weeks</article-title><alt-title alt-title-type="left-running-head">Kadowaki <italic>et&#x000a0;al</italic>.</alt-title></title-group><contrib-group><contrib id="jdi12001-cr-0001" contrib-type="author"><name><surname>Kadowaki</surname><given-names>Takashi</given-names></name><xref ref-type="aff" rid="jdi12001-aff-0001"><sup>1</sup></xref></contrib><contrib id="jdi12001-cr-0002" contrib-type="author"><name><surname>Tajima</surname><given-names>Naoko</given-names></name><xref ref-type="aff" rid="jdi12001-aff-0002"><sup>2</sup></xref></contrib><contrib id="jdi12001-cr-0003" contrib-type="author"><name><surname>Odawara</surname><given-names>Masato</given-names></name><xref ref-type="aff" rid="jdi12001-aff-0003"><sup>3</sup></xref></contrib><contrib id="jdi12001-cr-0004" contrib-type="author"><name><surname>Nishii</surname><given-names>Mikio</given-names></name><xref ref-type="correspondence-address" rid="correspondenceTo">*</xref><xref ref-type="aff" rid="jdi12001-aff-0004"><sup>5</sup></xref></contrib><contrib id="jdi12001-cr-0005" contrib-type="author"><name><surname>Taniguchi</surname><given-names>Tadaaki</given-names></name><xref ref-type="aff" rid="jdi12001-aff-0005"><sup>4</sup></xref></contrib><contrib id="jdi12001-cr-0006" contrib-type="author"><name><surname>Ferreira</surname><given-names>Juan Camilo Arjona</given-names></name><xref ref-type="aff" rid="jdi12001-aff-0006"><sup>6</sup></xref></contrib></contrib-group><aff id="jdi12001-aff-0001"><label><sup>1</sup></label><named-content content-type="organisation-division">Graduate School of Medicine</named-content><institution>The University of Tokyo</institution><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="jdi12001-aff-0002"><label><sup>2</sup></label><institution>Jikei University School of Medicine</institution><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="jdi12001-aff-0003"><label><sup>3</sup></label><named-content content-type="organisation-division">The Third Department of Internal Medicine</named-content><institution>Tokyo Medical University</institution><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="jdi12001-aff-0005"><label><sup>4</sup></label><institution>MSD K. K</institution><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="jdi12001-aff-0004"><label><sup>5</sup></label><named-content content-type="organisation-division">Development Planning</named-content><institution>Ono Pharmaceutical Co., Ltd</institution><named-content content-type="city">Osaka</named-content><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="jdi12001-aff-0006"><label><sup>6</sup></label><institution>Merck Sharp &#x00026; Dohme Corp. a subsidiary of Merck &#x00026; Co., Inc</institution><named-content content-type="city">Whitehouse Station</named-content><named-content content-type="country-part">NJ</named-content><country country="US">USA</country></aff><author-notes><corresp id="correspondenceTo"><label>*</label>
