<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Nutr J</journal-id><journal-title>Nutrition Journal</journal-title><issn pub-type="epub">1475-2891</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">17945020</article-id><article-id pub-id-type="pmc">2151062</article-id><article-id pub-id-type="publisher-id">1475-2891-6-29</article-id><article-id pub-id-type="doi">10.1186/1475-2891-6-29</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Improved weight management using genetic information to personalize a calorie controlled diet</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Arkadianos</surname><given-names>Ioannis</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>idc.diet@eexi.gr</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Valdes</surname><given-names>Ana M</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>ana.valdes@kcl.ac.uk</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Marinos</surname><given-names>Efstathios</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>smarin@biomed.ntua.gr</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Florou</surname><given-names>Anna</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>tsurekia@gmail.com</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Gill</surname><given-names>Rosalynn D</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>rgill-garrison@sciona.com</email></contrib><contrib id="A6" corresp="yes" contrib-type="author"><name><surname>Grimaldi</surname><given-names>Keith A</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>kgrimaldi@sciona.com</email></contrib></contrib-group><aff id="I1"><label>1</label>The Dr Arkadianos Clinic, Messogion Av, Athens, Greece</aff><aff id="I2"><label>2</label>Twin Research Unit, King's College London, UK</aff><aff id="I3"><label>3</label>Biomedical Engineering Laboratory, National Technical University of Athens, Greece</aff><aff id="I4"><label>4</label>Sciona Inc, Boulder, 80302, Colorado, USA</aff><pub-date pub-type="collection"><year>2007</year></pub-date><pub-date pub-type="epub"><day>18</day><month>10</month><year>2007</year></pub-date><volume>6</volume><fpage>29</fpage><lpage>29</lpage><ext-link ext-link-type="uri" xlink:href="http://www.nutritionj.com/content/6/1/29"/><history><date date-type="received"><day>23</day><month>5</month><year>2007</year></date><date date-type="accepted"><day>18</day><month>10</month><year>2007</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2007 Arkadianos et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2007</copyright-year><copyright-holder>Arkadianos et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
