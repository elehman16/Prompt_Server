addr-line>N 758 Montefiore Hospital</addr-line><addr-line>Pittsburgh, PA, 15213</addr-line><country>United States</country><phone>1 412 647 5734</phone><fax>1 412 864 5458</fax><email>molinarim@upmc.edu</email></address><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9763-6253</contrib-id></contrib><contrib id="contrib2" contrib-type="author"><name><surname>Puttarajappa</surname><given-names>Chethan</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4856-9242</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Wijkstrom</surname><given-names>Martin</given-names></name><degrees>MD, MSc</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6848-1478</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Ganoza</surname><given-names>Armando</given-names></name><degrees>MD, MSc</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2748-1962</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Lopez</surname><given-names>Roberto</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-5431-6594</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Tevar</surname><given-names>Amit</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff2">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-0030-6707</contrib-id></contrib></contrib-group><aff id="aff1">
