<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Res Protoc</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Res Protoc</journal-id><journal-id journal-id-type="publisher-id">ResProt</journal-id><journal-title-group><journal-title>JMIR Research Protocols</journal-title></journal-title-group><issn pub-type="epub">1929-0748</issn><publisher><publisher-name>JMIR Publications Inc.</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">23611773</article-id><article-id pub-id-type="pmc">3626143</article-id><article-id pub-id-type="publisher-id">v1i1e4</article-id><article-id pub-id-type="doi">10.2196/resprot.1970</article-id><article-categories><subj-group subj-group-type="heading"><subject>Protocol</subject></subj-group></article-categories><title-group><article-title>Harm Reduction Text Messages Delivered During Alcohol Drinking: Feasibility Study Protocol</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Johnson</surname><given-names>Natalie</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Whiteside</surname><given-names>Ursula</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes"><name><surname>Renner</surname><given-names>Karen Adell</given-names></name><degrees>BA, MA</degrees><xref ref-type="aff" rid="aff1">1</xref><address><institution>General Practice and Primary Health Care</institution><institution>School of Population Health</institution><institution>University of Auckland</institution><addr-line>Bldg 730</addr-line><addr-line>