<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Exerc Rehabil</journal-id><journal-id journal-id-type="iso-abbrev">J Exerc Rehabil</journal-id><journal-title-group><journal-title>Journal of Exercise Rehabilitation</journal-title></journal-title-group><issn pub-type="ppub">2288-176X</issn><issn pub-type="epub">2288-1778</issn><publisher><publisher-name>Korean Society of Exercise Rehabilitation</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29511660</article-id><article-id pub-id-type="pmc">5833954</article-id><article-id pub-id-type="doi">10.12965/jer.1835122.561</article-id><article-id pub-id-type="publisher-id">jer-14-1-106</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title>Sprint interval training on the vertical treadmill improves aerobic and anaerobic running performance</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Jordan</surname><given-names>Alastair Ross</given-names></name><xref ref-type="aff" rid="af1-jer-14-1-106">1</xref><xref rid="c1-jer-14-1-106" ref-type="corresp">*</xref></contrib><contrib contrib-type="author"><name><surname>Claxton</surname><given-names>David</given-names></name><xref ref-type="aff" rid="af2-jer-14-1-106">2</xref></contrib><contrib contrib-type="author"><name><surname>Purvis</surname><given-names>Alison</given-names></name><xref ref-type="aff" rid="af2-jer-14-1-106">2</xref></contrib><contrib contrib-type="author"><name><surname>Barnes</surname><given-names>Andrew</given-names></name><xref ref-type="aff" rid="af2-jer-14-1-106">2</xref></contrib><contrib contrib-type="author"><name><surname>Fysh</surname><given-names>Mary</given-names></name><xref ref-type="aff" rid="af2-jer-14-1-106">2</xref></contrib></contrib-group><aff id="af1-jer-14-1-106">
