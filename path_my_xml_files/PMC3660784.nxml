rid="au19">19</xref></contrib><contrib contrib-type="author"><name><surname>Borrosch</surname><given-names>F</given-names></name><xref ref-type="aff" rid="au20">20</xref></contrib><contrib contrib-type="author"><name><surname>Reich</surname><given-names>K</given-names></name><xref ref-type="aff" rid="au21">21</xref></contrib><contrib contrib-type="author"><name><surname>Berking</surname><given-names>C</given-names></name><xref ref-type="aff" rid="au22">22</xref></contrib><contrib contrib-type="author"><name><surname>Wolf</surname><given-names>P</given-names></name><xref ref-type="aff" rid="au23">23</xref></contrib><contrib contrib-type="author"><name><surname>Lehmann</surname><given-names>P</given-names></name><xref ref-type="aff" rid="au24">24</xref></contrib><contrib contrib-type="author"><name><surname>Moers-Carpi</surname><given-names>M</given-names></name><xref ref-type="aff" rid="au25">25</xref></contrib><contrib contrib-type="author"><name><surname>H&#x000f6;nigsmann</surname><given-names>H</given-names></name><xref ref-type="aff" rid="au26">26</xref></contrib><contrib contrib-type="author"><name><surname>Wernicke-Panten</surname><given-names>K</given-names></name><xref ref-type="aff" rid="au27">27</xref></contrib><contrib contrib-type="author"><name><surname>Hahn</surname><given-names>S</given-names></name><xref ref-type="aff" rid="au27">27</xref></contrib><contrib contrib-type="author"><name><surname>Pabst</surname><given-names>G</given-names></name><xref ref-type="aff" rid="au28">28</xref></contrib><contrib contrib-type="author"><name><surname>Voss</surname><given-names>D</given-names></name><xref ref-type="aff" rid="au28">28</xref></contrib><contrib contrib-type="author"><name><surname>Foguet</surname><given-names>M</given-names></name><xref ref-type="aff" rid="au29">29</xref></contrib><contrib contrib-type="author"><name><surname>Schmitz</surname><given-names>B</given-names></name><xref ref-type="aff" rid="au29">29</xref></contrib><contrib contrib-type="