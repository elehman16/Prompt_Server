/mml:mn><mml:mo>.</mml:mo><mml:mn>343</mml:mn><mml:msup><mml:mi>e</mml:mi><mml:mrow><mml:mrow><mml:mo>(</mml:mo><mml:mrow><mml:mfrac><mml:mi>d</mml:mi><mml:mrow><mml:mn>20</mml:mn></mml:mrow></mml:mfrac></mml:mrow><mml:mo>)</mml:mo></mml:mrow></mml:mrow></mml:msup><mml:mspace width="0.25em"/></mml:mrow></mml:math></inline-formula>], linear [<inline-formula id="inline-formula17-0269881117722998"><mml:math id="math17-0269881117722998"><mml:mrow><mml:mi>f</mml:mi><mml:mrow><mml:mo>(</mml:mo><mml:mi>d</mml:mi><mml:mo>)</mml:mo></mml:mrow><mml:mo>=</mml:mo><mml:mn>2</mml:mn><mml:mo>+</mml:mo><mml:mn>0</mml:mn><mml:mo>.</mml:mo><mml:mn>157</mml:mn><mml:mi>d</mml:mi></mml:mrow></mml:math></inline-formula>], logistic 1 [<inline-formula id="inline-formula18-0269881117722998"><mml:math id="math18-0269881117722998"><mml:mrow><mml:mi>f</mml:mi><mml:mrow><mml:mo>(</mml:mo><mml:mi>d</mml:mi><mml:mo>)</mml:mo></mml:mrow><mml:mo>=</mml:mo><mml:mspace width="0.25em"/><mml:mn>2</mml:mn><mml:mo>+</mml:mo><mml:mfrac><mml:mrow><mml:mn>11</mml:mn></mml:mrow><mml:mrow><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:msup><mml:mi>e</mml:mi><mml:mrow><mml:mrow><mml:mo>(</mml:mo><mml:mrow><mml:mn>20</mml:mn><mml:mo>&#x02212;</mml:mo><mml:mi>d</mml:mi></mml:mrow><mml:mo>)</mml:mo></mml:mrow></mml:mrow></mml:msup></mml:mrow></mml:mfrac></mml:mrow></mml:math></inline-formula>], logistic 2 [<inline-formula id="inline-formula19-0269881117722998"><mml:math id="math19-0269881117722998"><mml:mrow><mml:mi>f</mml:mi><mml:mrow><mml:mo>(</mml:mo><mml:mi>d</mml:mi><mml:mo>)</mml:mo></mml:mrow><mml:mo>=</mml:mo><mml:mspace width="0.25em"/><mml:mn>1</mml:mn><mml:mo>.</mml:mo><mml:mn>80</mml:mn><mml:mo>+</mml:mo><mml:mfrac><mml:mrow><mml:mn>11</mml:mn><mml:mo>.</mml:mo><mml:mn>20</mml:mn></mml:mrow><mml:mrow><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:msup><mml:mi>e</mml:mi><mml:mrow><mml:mrow><mml:mo>(</mml:mo><mml:mrow><mml:mfrac><mml:mrow><mml:mn>20</mml:mn><mml:mo>&#x02212;</mml:mo><mml:mi>d</mml:mi></mml:mrow><mm