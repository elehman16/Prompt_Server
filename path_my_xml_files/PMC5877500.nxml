trib-type="author"><name><surname>Clarke</surname><given-names>Graham W</given-names></name><xref ref-type="aff" rid="af4-copd-13-1009">4</xref><xref ref-type="aff" rid="af5-copd-13-1009">5</xref></contrib><contrib contrib-type="author"><name><surname>Rennard</surname><given-names>Stephen I</given-names></name><xref ref-type="aff" rid="af6-copd-13-1009">6</xref><xref ref-type="aff" rid="af7-copd-13-1009">7</xref></contrib><contrib contrib-type="author"><name><surname>Make</surname><given-names>Barry J</given-names></name><xref ref-type="aff" rid="af8-copd-13-1009">8</xref></contrib><contrib contrib-type="author"><name><surname>Wise</surname><given-names>Robert A</given-names></name><xref ref-type="aff" rid="af9-copd-13-1009">9</xref></contrib><contrib contrib-type="author"><name><surname>Jansson</surname><given-names>Paul</given-names></name><xref ref-type="aff" rid="af1-copd-13-1009">1</xref><xref ref-type="corresp" rid="c1-copd-13-1009"/></contrib></contrib-group><aff id="af1-copd-13-1009">
