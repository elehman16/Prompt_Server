<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Thorax</journal-id><journal-id journal-id-type="iso-abbrev">Thorax</journal-id><journal-id journal-id-type="hwp">thoraxjnl</journal-id><journal-id journal-id-type="publisher-id">thorax</journal-id><journal-title-group><journal-title>Thorax</journal-title></journal-title-group><issn pub-type="ppub">0040-6376</issn><issn pub-type="epub">1468-3296</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27406165</article-id><article-id pub-id-type="pmc">5036235</article-id><article-id pub-id-type="publisher-id">thoraxjnl-2016-208501</article-id><article-id pub-id-type="doi">10.1136/thoraxjnl-2016-208501</article-id><article-categories><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group><subj-group subj-group-type="heading"><subject>Non-Invasive Ventilation</subject></subj-group><series-title>Original article</series-title></article-categories><title-group><article-title>Non-invasive ventilation in obesity hypoventilation syndrome without severe obstructive sleep apnoea</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Masa</surname><given-names>Juan F</given-names></name><xref ref-type="aff" rid="af1">1</xref><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Corral</surname><given-names>Jaime</given-names></name><xref ref-type="aff" rid="af1">1</xref><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Caballero</surname><given-names>Candela</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Barrot</surname><given-names>Emilia</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Ter&#x000e1;n-Santos</surname><given-names>Joaquin</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Alonso-&#x000c1;lvarez</surname><given-names>Maria L</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Gomez-Garcia</surname><given-names>Teresa</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Gonz&#x000e1;lez</surname><given-names>M&#x000f3;nica</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>L&#x000f3;pez-Mart&#x000ed;n</surname><given-names>Soledad</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>De Lucas</surname><given-names>Pilar</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>Marin</surname><given-names>Jos&#x000e9; M</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af8">8</xref></contrib><contrib contrib-type="author"><name><surname>Marti</surname><given-names>Sergi</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af9">9</xref></contrib><contrib contrib-type="author"><name><surname>D&#x000ed;az-Cambriles</surname><given-names>Trinidad</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af10">10</xref></contrib><contrib contrib-type="author"><name><surname>Chiner</surname><given-names>Eusebi</given-names></name><xref ref-type="aff" rid="af11">11</xref></contrib><contrib contrib-type="author"><name><surname>Egea</surname><given-names>Carlos</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af12">12</xref></contrib><contrib contrib-type="author"><name><surname>Miranda</surname><given-names>Erika</given-names></name><xref ref-type="aff" rid="af13">13</xref></contrib><contrib contrib-type="author"><name><surname>Mokhlesi</surname><given-names>Babak</given-names></name><xref ref-type="aff" rid="af14">14</xref></contrib><contrib contrib-type="author"><collab>on behalf of the Spanish Sleep Network</collab></contrib><contrib contrib-type="author"><name><surname>Garc&#x000ed;a-Ledesma</surname><given-names>Estefan&#x000ed;a</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>S&#x000e1;nchez-Quiroga</surname><given-names>M-&#x000c1;ngeles</given-names></name><xref ref-type="aff" rid="af15">15</xref></contrib><contrib contrib-type="author"><name><surname>Ordax</surname><given-names>Estrella</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Gonz&#x000e1;lez-Mangado</surname><given-names>Nicol&#x000e1;s</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Troncoso</surname><given-names>Maria F</given-names></name><xref ref-type="aff" rid="af2">2</xref><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Martinez-Martinez</surname><given-names>Maria-&#x000c1;ngeles</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>Cantalejo</surname><given-names>Olga</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>Ojeda</surname><given-names>Elena</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surn