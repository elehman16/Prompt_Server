<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Gut Liver</journal-id><journal-id journal-id-type="iso-abbrev">Gut Liver</journal-id><journal-title-group><journal-title>Gut and Liver</journal-title></journal-title-group><issn pub-type="ppub">1976-2283</issn><issn pub-type="epub">2005-1212</issn><publisher><publisher-name>Editorial Office of Gut and Liver</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28798288</article-id><article-id pub-id-type="pmc">5753689</article-id><article-id pub-id-type="doi">10.5009/gnl17048</article-id><article-id pub-id-type="publisher-id">gnl-12-086</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group></article-categories><title-group><article-title>The Real-World Safety and Efficacy of Daclatasvir and Asunaprevir for Elderly Patients</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Taki</surname><given-names>Shinya</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Tamai</surname><given-names>Hideyuki</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref><xref ref-type="corresp" rid="c1-gnl-12-086"/></contrib><contrib contrib-type="author"><name><surname>Ida</surname><given-names>Yoshiyuki</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Shingaki</surname><given-names>Naoki</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Kawashima</surname><given-names>Akira</given-names></name><xref ref-type="aff" rid="af2-gnl-12-086">2</xref></contrib><contrib contrib-type="author"><name><surname>Shimizu</surname><given-names>Ryo</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Moribata</surname><given-names>Kosaku</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Maekita</surname><given-names>Takao</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Iguchi</surname><given-names>Mikitaka</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Kato</surname><given-names>Jun</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib><contrib contrib-type="author"><name><surname>Nakao</surname><given-names>Taisei</given-names></name><xref ref-type="aff" rid="af2-gnl-12-086">2</xref></contrib><contrib contrib-type="author"><name><surname>Kitano</surname><given-names>Masayuki</given-names></name><xref ref-type="aff" rid="af1-gnl-12-086">1</xref></contrib></contrib-group><aff id="af1-gnl-12-086">
