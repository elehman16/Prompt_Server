<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Inflamm (Lond)</journal-id><journal-title>Journal of Inflammation (London, England)</journal-title><issn pub-type="epub">1476-9255</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">16242032</article-id><article-id pub-id-type="pmc">1276811</article-id><article-id pub-id-type="publisher-id">1476-9255-2-11</article-id><article-id pub-id-type="doi">10.1186/1476-9255-2-11</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Early relief of osteoarthritis symptoms with a natural mineral supplement and a herbomineral combination: A randomized controlled trial [ISRCTN38432711]</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Miller</surname><given-names>Mark JS</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>millermj@mail.amc.edu</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Mehta</surname><given-names>Komal</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Kunte</surname><given-names>Sameer</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Raut</surname><given-names>Vidyanand</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Gala</surname><given-names>Jayesh</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Dhumale</surname><given-names>Ramesh</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Shukla</surname><given-names>Anil</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Tupalli</surname><given-names>Hemant</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Parikh</surname><given-names>Himanshu</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>vedic@ayuherbal.com</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Bobrowski</surname><given-names>Paul</given-names></name><xref ref-type="aff" rid="I6">6</xref><email>pbobrowski@santerra-pharma.com</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Chaudhary</surname><given-names>Jayesh</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>jayesh@ayuherbal.com</email></contrib></contrib-group><aff id="I1"><label>1</label>Center for Cardiovascular Sciences, Albany Medical College, Albany, New York, USA</aff><aff id="I2"><label>2</label>Vedic Lifesciences, Mumbai, India</aff><aff id="I3"><label>3</label>Ashok Raut Orthopedic Hospital, Virarwest, India</aff><aff id="I4"><label>4</label>Bhagwati Hospital, Mumbai, India</aff><aff id="I5"><label>5</label>K.J. Somaiya Medical College &#x00026; Hospital, Mumbai, India</aff><aff id="I6"><label>6</label>Santerra Pharmaceuticals, LLC, Raleigh, North Carolina, USA</aff><pub-date pub-type="collection"><year>2005</year></pub-date><pub-date pub-type="epub"><day>21</day><month>10</month><year>2005</year></pub-date><volume>2</volume><fpage>11</fpage><lpage>11</lpage><ext-link ext-link-type="uri" xlink:href="http://www.journal-inflammation.com/content/2/1/11"/><history><date date-type="received"><day>7</day><month>3</month><year>2005</year></date><date date-type="accepted"><day>21</day><month>10</month><year>2005</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2005 Miller et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2005</copyright-year><copyright-holder>Miller et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
