<article xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">NPJ Prim Care Respir Med</journal-id><journal-id journal-id-type="iso-abbrev">NPJ Prim Care Respir Med</journal-id><journal-title-group><journal-title>NPJ Primary Care Respiratory Medicine</journal-title></journal-title-group><issn pub-type="epub">2055-1010</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">26677770</article-id><article-id pub-id-type="pmc">4682572</article-id><article-id pub-id-type="pii">npjpcrm201571</article-id><article-id pub-id-type="doi">10.1038/npjpcrm.2015.71</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Exploring the variation in implementation of a COPD disease management programme and its impact on health outcomes: a <italic>post hoc</italic> analysis of the RECODE cluster randomised trial</article-title><alt-title alt-title-type="running">Variation in implementation of the RECODE trial</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Boland</surname><given-names>Melinde R S</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><contrib contrib-type="author"><name><surname>Kruis</surname><given-names>Annemarije L</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Huygens</surname><given-names>Simone A</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Tsiachristas</surname><given-names>Apostolos</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-t