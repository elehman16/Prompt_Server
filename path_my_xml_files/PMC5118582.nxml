trib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2866-0169</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>Amino</surname><given-names>Hiroshi</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff02">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-0219-8482</contrib-id></contrib><contrib id="contrib9" contrib-type="author"><name><surname>Suichi</surname><given-names>Tomoki</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="aff02">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9575-9444</contrib-id></contrib><contrib id="contrib10" contrib-type="author"><name><surname>Yokota</surname><given-names>Takanori</given-names></name><degrees>PhD, MD</degrees><xref ref-type="aff" rid="aff04">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6490-3263</contrib-id></contrib><contrib id="contrib11" contrib-type="author"><name><surname>Nishida</surname><given-names>Yoichiro</given-names></name><degrees>PhD, MD</degrees><xref ref-type="aff" rid="aff04">4</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6158-9355</contrib-id></contrib><contrib id="contrib12" contrib-type="author"><name><surname>Kohara</surname><given-names>Nobuo</given-names></name><degrees>PhD, MD</degrees><xref ref-type="aff" rid="aff05">5</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-7062-6306</contrib-id></contrib><contrib id="contrib13" contrib-type="author"><name><surname>Hirata</surname><given-names>Koichi</given-names></name><degrees>PhD, MD</degrees><xref ref-type="aff" rid="aff06">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6492-4734</contrib-id></contrib><contrib id="contrib14" contrib-type="author"><name><surname>Nishiyama</surname><given-names>Kazutoshi</given-names></name><degrees>PhD, MD</degrees><xref ref-type="aff" rid="aff07">7</xref><contrib-id contrib-id-type="orcid">http://orcid.org/