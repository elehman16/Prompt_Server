<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Biopsychosoc Med</journal-id><journal-title>Biopsychosocial Medicine</journal-title><issn pub-type="epub">1751-0759</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18267038</article-id><article-id pub-id-type="pmc">2275291</article-id><article-id pub-id-type="publisher-id">1751-0759-2-5</article-id><article-id pub-id-type="doi">10.1186/1751-0759-2-5</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Psychological and weight-related characteristics of patients with anorexia nervosa-restricting type who later develop bulimia nervosa</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Nishimura</surname><given-names>Hiroki</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>nisimura@ncnp.go.jp</email></contrib><contrib id="A2" corresp="yes" contrib-type="author"><name><surname>Komaki</surname><given-names>Gen</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>komaki@ncnp.go.jp</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Ando</surname><given-names>Tetsuya</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>ando-t@ncnp.go.jp</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Nakahara</surname><given-names>Toshihiro</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>ayana-n@m3.kufm.kagoshima-u.ac.jp</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Oka</surname><given-names>Takakazu</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>toka@med.uoeh-u.ac.jp</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Kawai</surna