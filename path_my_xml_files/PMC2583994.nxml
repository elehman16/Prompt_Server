<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Transl Med</journal-id><journal-title>Journal of Translational Medicine</journal-title><issn pub-type="epub">1479-5876</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18976450</article-id><article-id pub-id-type="pmc">2583994</article-id><article-id pub-id-type="publisher-id">1479-5876-6-65</article-id><article-id pub-id-type="doi">10.1186/1479-5876-6-65</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research</subject></subj-group></article-categories><title-group><article-title>Randomized phase II study with two gemcitabine- and docetaxel-based combinations as first-line chemotherapy for metastatic non-small cell lung cancer</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Passardi</surname><given-names>Alessandro</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>sandropass@libero.it</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Cecconetto</surname><given-names>Lorenzo</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>lorenzocecco@hotmail.com</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Dall'Agata</surname><given-names>Monia</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>monia.dallagata@ausl.fo.it</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Dazzi</surname><given-names>Claudio</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>c.dazzi@ausl.ra.it</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Pasquini</surname><given-names>Enzo</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>oncologiacattolica@libero.it</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Oliverio</surname><given-names>Giovanni</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>goliverio@ausl.ra.it</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Zumaglini</surname><given-names>Federica</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>f.zumaglini@ausl.ra.it</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Zoli</surname><given-names>Wainer</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>w.zoli@ausl.fo.it</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Nanni</surname><given-names>Oriana</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>o.nanni@irst.emr.it</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Milandri</surname><given-names>Carlo</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>cmilandri@libero.it</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Frassineti</surname><given-names>Giovanni Luca</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>lu.frax@libero.it</email></contrib><contrib id="A12" contrib-type="author"><name><surname>Amadori</surname><given-names>Dino</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>segronco@ausl.fo.it</email></contrib></contrib-group><aff id="I1"><label>1</label>Istituto Scientifico Romagnolo per lo Studio e la Cura dei Tumori, Meldola, Italy</aff><aff id="I2"><label>2</label>Istituto Oncologico Romagnolo, Forl&#x000ec;, Italy</aff><aff id="I3"><label>3</label>Department of Oncology, Santa Maria delle Croci Hospital, Ravenna, Italy</aff><aff id="I4"><label>4</label>Department of Oncology, Infermi Hospital, Rimini, Italy</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>31</day><month>10</month><year>2008</year></pub-date><volume>6</volume><fpage>65</fpage><lpage>65</lpage><ext-link ext-link-type="uri" xlink:href="http://www.translational-medicine.com/content/6/1/65"/><history><date date-type="received"><day>7</day><month>7</month><year>2008</year></date><date date-type="accepted"><day>31</day><month>10</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Passardi et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Passardi et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
