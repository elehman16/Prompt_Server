<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="review-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Drugs Context</journal-id><journal-id journal-id-type="iso-abbrev">Drugs Context</journal-id><journal-id journal-id-type="publisher-id">DIC</journal-id><journal-title-group><journal-title>Drugs in Context</journal-title></journal-title-group><issn pub-type="ppub">1745-1981</issn><issn pub-type="epub">1740-4398</issn><publisher><publisher-name>BioExcel Publishing Ltd</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29707029</article-id><article-id pub-id-type="pmc">5916097</article-id><article-id pub-id-type="doi">10.7573/dic.212518</article-id><article-id pub-id-type="publisher-id">dic-7-212518</article-id><article-categories><subj-group subj-group-type="heading"><subject>Review</subject></subj-group></article-categories><title-group><article-title>Cardiac myosin activators for heart failure therapy: focus on omecamtiv mecarbil</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Kaplinsky</surname><given-names>Edgardo</given-names></name><degrees>MD</degrees><xref ref-type="aff" rid="af1-dic-7-212518">1</xref></contrib><contrib contrib-type="author"><name><surname>Mallarkey</surname><given-names>Gordon</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="af2-dic-7-212518">2</xref></contrib></contrib-group><aff id="af1-dic-7-212518">
