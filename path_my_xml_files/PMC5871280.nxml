<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Evid Based Complementary Altern Med</journal-id><journal-id journal-id-type="iso-abbrev">J Evid Based Complementary Altern Med</journal-id><journal-id journal-id-type="publisher-id">CHP</journal-id><journal-id journal-id-type="hwp">spchp</journal-id><journal-title-group><journal-title>Journal of Evidence-based Complementary &#x00026; Alternative Medicine</journal-title></journal-title-group><issn pub-type="ppub">2156-5872</issn><issn pub-type="epub">2156-5899</issn><publisher><publisher-name>SAGE Publications</publisher-name><publisher-loc>Sage CA: Los Angeles, CA</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28497700</article-id><article-id pub-id-type="pmc">5871280</article-id><article-id pub-id-type="doi">10.1177/2156587217707117</article-id><article-id pub-id-type="publisher-id">10.1177_2156587217707117</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Articles</subject></subj-group></article-categories><title-group><article-title>Instrumental Measurements of Water and the Surrounding Space During a Randomized Blinded Controlled Trial of Focused Intention</article-title></title-group><contrib-group><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-2698-3151</contrib-id><name><surname>Matos</surname><given-names>Lu&#x000ed;s Carlos</given-names></name><degrees>MTCM</degrees><xref ref-type="aff" rid="aff1-2156587217707117">1</xref><xref ref-type="corresp" rid="corresp1-2156587217707117"/></contrib><contrib contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0002-7516-1955</contrib-id><name><surname>Santos</surname><given-names>Sara Cristina</given-names></name><degrees>MEng</degrees><xref ref-type="aff" rid="aff1-2156587217707117">1</xref></contrib><contrib contrib-type="author"><name><surname>Anderson</surname><given-names>Joel G.</given-names></name><degrees>PhD, CHTP</degrees><xref ref-type="aff" rid="aff2-2156587217707117">2</xref></contrib><contrib contrib-type="author"><name><surname>Machado</surname><given-names>Jorge</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff3-2156587217707117">3</xref></contrib><contrib contrib-type="author"><name><surname>Greten</surname><given-names>Henry Johannes</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff3-2156587217707117">3</xref><xref ref-type="aff" rid="aff4-2156587217707117">4</xref></contrib><contrib contrib-type="author"><name><surname>Monteiro</surname><given-names>Fernando Jorge</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff1-2156587217707117">1</xref></contrib></contrib-group><aff id="aff1-2156587217707117">
