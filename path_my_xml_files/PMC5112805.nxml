thor"><name><surname>Moreira</surname><given-names>Thereza Maria Magalhaes</given-names></name><xref ref-type="aff" rid="aff002"><sup>2</sup></xref><xref ref-type="author-notes" rid="currentaff002"><sup>&#x000a4;b</sup></xref></contrib><contrib contrib-type="author"><name><surname>de Oliveira</surname><given-names>Edmar Souza</given-names></name><xref ref-type="aff" rid="aff003"><sup>3</sup></xref></contrib><contrib contrib-type="author"><name><surname>de Menezes</surname><given-names>Ana&#x000ed;ze Viana Bezerra</given-names></name><xref ref-type="aff" rid="aff004"><sup>4</sup></xref></contrib><contrib contrib-type="author"><name><surname>Loureiro</surname><given-names>Aline Maria Oliveira</given-names></name><xref ref-type="aff" rid="aff005"><sup>5</sup></xref></contrib><contrib contrib-type="author"><name><surname>Silva</surname><given-names>Camila Brasileiro de Ara&#x000fa;jo</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib><contrib contrib-type="author"><name><surname>Linard</surname><given-names>Jair Gomes</given-names></name><xref ref-type="aff" rid="aff006"><sup>6</sup></xref></contrib><contrib contrib-type="author"><name><surname>de Almeida</surname><given-names>Italo Lennon Sales</given-names></name><xref ref-type="aff" rid="aff007"><sup>7</sup></xref></contrib><contrib contrib-type="author"><name><surname>Mattos</surname><given-names>Samuel Miranda</given-names></name><xref ref-type="aff" rid="aff007"><sup>7</sup></xref></contrib><contrib contrib-type="author"><name><surname>Borges</surname><given-names>Jos&#x000e9; Wicto Pereira</given-names></name><xref ref-type="aff" rid="aff008"><sup>8</sup></xref></contrib></contrib-group><aff id="aff001"><label>1</label><addr-line>Postgraduate Program Clinical Care in Nursing and Health (PPCCLIS), Universidade Estadual do Cear&#x000e1; (UECE), Unit of Clinical Pharmacology, Center for Research and Development of Medicines (NPDM), Universidade Federal do Cear&#x000e1;, Fortaleza, Bra