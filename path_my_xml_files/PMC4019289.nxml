<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Diabetes Investig</journal-id><journal-id journal-id-type="iso-abbrev">J Diabetes Investig</journal-id><journal-id journal-id-type="doi">10.1111/(ISSN)2040-1124</journal-id><journal-id journal-id-type="publisher-id">JDI</journal-id><journal-title-group><journal-title>Journal of Diabetes Investigation</journal-title></journal-title-group><issn pub-type="ppub">2040-1116</issn><issn pub-type="epub">2040-1124</issn><publisher><publisher-name>Wiley-Blackwell</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">24843632</article-id><article-id pub-id-type="pmc">4019289</article-id><article-id pub-id-type="doi">10.1111/j.2040-1124.2012.00240.x</article-id><article-id pub-id-type="publisher-id">JDI240</article-id><article-categories><subj-group subj-group-type="overline"><subject>Clinical Trial</subject></subj-group><subj-group subj-group-type="heading"><subject>Articles</subject><subj-group subj-group-type="heading"><subject>Clinical Science and Care</subject></subj-group></subj-group></article-categories><title-group><article-title>Insulin degludec in <styled-content style="fixed-case">J</styled-content>apanese patients with type 1 diabetes mellitus: A randomized controlled trial</article-title><alt-title alt-title-type="left-running-head">Iwamoto <italic>et&#x000a0;al</italic>.</alt-title></title-group><contrib-group><contrib id="jdi240-cr-0001" contrib-type="author"><name><surname>Iwamoto</surname><given-names>Yasuhiko</given-names></name><xref ref-type="correspondence-address" rid="correspondenceTo">*</xref><xref ref-type="aff" rid="jdi240-aff-0001"><sup>1</sup></xref></contrib><contrib id="jdi240-cr-0002" contrib-type="author"><name><surname>Clauson</surname><given-names>Per</given-names></name><xref ref-type="aff" rid="jdi240-aff-0002"><sup>2</sup></xref></contrib><contrib id="jdi240-cr-0003" contrib-type="author"><name><surname>Nishida</surname><given-names>Tomoyuki</given-names></name><xref ref-type="aff" rid="jdi240-aff-0002"><sup>2</sup></xref></contrib><contrib id="jdi240-cr-0004" contrib-type="author"><name><surname>Kaku</surname><given-names>Kohei</given-names></name><xref ref-type="aff" rid="jdi240-aff-0003"><sup>3</sup></xref></contrib></contrib-group><aff id="jdi240-aff-0001"><label><sup>1</sup></label><institution>Tokyo Women's Medical University</institution><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="jdi240-aff-0002"><label><sup>2</sup></label><institution>Novo Nordisk Pharma Limited</institution><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="jdi240-aff-0003"><label><sup>3</sup></label><institution>Kawasaki Medical School</institution><named-content content-type="city">Okayama</named-content><country country="JP">Japan</country></aff><author-notes><corresp id="correspondenceTo"><label>*</label>
