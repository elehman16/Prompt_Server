<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open Sport Exerc Med</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open Sport Exerc Med</journal-id><journal-id journal-id-type="hwp">bmjosem</journal-id><journal-id journal-id-type="publisher-id">bmjosem</journal-id><journal-title-group><journal-title>BMJ Open Sport &#x02014; Exercise Medicine</journal-title></journal-title-group><issn pub-type="epub">2055-7647</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28761713</article-id><article-id pub-id-type="pmc">5530127</article-id><article-id pub-id-type="publisher-id">bmjsem-2017-000229</article-id><article-id pub-id-type="doi">10.1136/bmjsem-2017-000229</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Article</subject></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group></article-categories><title-group><article-title>The effect of 1 week of repeated ischaemic leg preconditioning on simulated Keirin cycling performance: a randomised trial</article-title></title-group><contrib-group><contrib id="author-53602846" contrib-type="author"><name><surname>Lindsay</surname><given-names>Angus</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-53602574" contrib-type="author"><name><surname>Petersen</surname><given-names>Carl</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-53602948" contrib-type="author"><name><surname>Blackwell</surname><given-names>Gavin</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-53603107" contrib-type="author"><name><surname>Ferguson</surname><given-names>Hamish</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-53603121" contrib-type="author"><name><surname>Parker</surname><given-names>Greg</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-53603136" contrib-type="author"><name><surname>Steyn</surname><given-names>Nina</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-53603174" contrib-type="author"><name><surname>Gieseg</surname><given-names>Steven P</given-names></name><xref ref-type="aff" rid="aff4">4</xref><xref ref-type="aff" rid="aff5">5</xref></contrib></contrib-group><aff id="aff1">
