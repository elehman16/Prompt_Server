><contrib contrib-type="author"><name><surname>Nissinen</surname><given-names>Markku J</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>Stenman</surname><given-names>Ulf-H&#x000e5;kan</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>Gylling</surname><given-names>Helena</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Palom&#x000e4;ki</surname><given-names>Ari</given-names></name><xref ref-type="aff" rid="aff7">7</xref><xref ref-type="aff" rid="aff8">8</xref><xref ref-type="aff" rid="aff9">9</xref></contrib></contrib-group><aff id="aff1">
