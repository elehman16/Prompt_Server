<article xmlns:ali="http://www.niso.org/schemas/ali/1.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Physiol Rep</journal-id><journal-id journal-id-type="iso-abbrev">Physiol Rep</journal-id><journal-id journal-id-type="publisher-id">phy2</journal-id><journal-title-group><journal-title>Physiological Reports</journal-title></journal-title-group><issn pub-type="ppub">2051-817X</issn><issn pub-type="epub">2051-817X</issn><publisher><publisher-name>John Wiley &#x00026; Sons, Ltd</publisher-name><publisher-loc>Chichester, UK</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">26272733</article-id><article-id pub-id-type="pmc">4562558</article-id><article-id pub-id-type="doi">10.14814/phy2.12472</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>The effect of training volume and intensity on improvements in muscular strength and size in resistance-trained men</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Mangine</surname><given-names>Gerald T</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Hoffman</surname><given-names>Jay R</given-names></name><xref ref-type="aff" rid="au1">1</xref><xref ref-type="corresp" rid="cor1"/></contrib><contrib contrib-type="author"><name><surname>Gonzalez</surname><given-names>Adam M</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Townsend</surname><given-names>Jeremy R</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Wells</surname><given-names>Adam J</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Jajtner</surname><given-names>Adam R</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Beyer</surname><given-names>Kyle S</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Boone</surname><given-names>Carleigh H</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Miramonti</surname><given-names>Amelia A</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Ran</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>LaMonica</surname><given-names>Michael B</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Fukuda</surname><given-names>David H</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><contrib contrib-type="author"><name><surname>Ratamess</surname><given-names>Nicholas A</given-names></name><xref ref-type="aff" rid="au2">2</xref></contrib><contrib contrib-type="author"><name><surname>Stout</surname><given-names>Jeffrey R</given-names></name><xref ref-type="aff" rid="au1">1</xref></contrib><aff id="au1"><label>1</label><institution>Institute of Exercise Physiology and Wellness, University of Central Florida</institution><addr-line>Orlando, Florida</addr-line></aff><aff id="au2"><label>2</label><institution>Health &#x00026; Exercise Science, The College of New Jersey</institution><addr-line>Ewing, New Jersey</addr-line></aff></contrib-group><author-notes><corresp id="cor1"><bold>Correspondence</bold> Jay R. Hoffman, Institute of Exercise Physiology and Wellness, University of Central Florida, 4000 Central Florida Blvd, Orlando, FL 32816-1250., Tel: 407-823-1272, E-mail: <email>jay.hoffman@ucf.edu</email></corresp><fn><p>
