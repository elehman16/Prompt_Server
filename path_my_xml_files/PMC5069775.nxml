<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Implement Sci</journal-id><journal-id journal-id-type="iso-abbrev">Implement Sci</journal-id><journal-title-group><journal-title>Implementation Science : IS</journal-title></journal-title-group><issn pub-type="epub">1748-5908</issn><publisher><publisher-name>BioMed Central</publisher-name><publisher-loc>London</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27756434</article-id><article-id pub-id-type="pmc">5069775</article-id><article-id pub-id-type="publisher-id">503</article-id><article-id pub-id-type="doi">10.1186/s13012-016-0503-6</article-id><article-categories><subj-group subj-group-type="heading"><subject>Study Protocol</subject></subj-group></article-categories><title-group><article-title>Triage, treatment and transfer of patients with stroke in emergency department trial (the T<sup>3</sup> Trial): a cluster randomised trial protocol</article-title></title-group><contrib-group><contrib contrib-type="author" corresp="yes"><name><surname>Middleton</surname><given-names>Sandy</given-names></name><address><email>Sandy.middleton@acu.edu.au</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Levi</surname><given-names>Chris</given-names></name><address><email>christopher.levi@hnehealth.nsw.gov.au</email></address><xref ref-type="aff" rid="Aff2">2</xref><xref ref-type="aff" rid="Aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Dale</surname><given-names>Simeon</given-names></name><address><email>Simeon.dale@acu.edu.au</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Cheung</surname><given-names>N. Wah</given-names></name><address><email>wah.cheung@sydney.edu.au</email></address><xref ref-type="aff" rid="Aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>McInnes</surname><given-names>Elizabeth</given-names></name><address><email>Elizabeth.mcinnes@acu.edu.au</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Considine</surname><given-names>Julie</given-names></name><address><email>julie.considine@deakin.edu.au</email></address><xref ref-type="aff" rid="Aff5">5</xref></contrib><contrib contrib-type="author"><name><surname>D&#x02019;Este</surname><given-names>Catherine</given-names></name><address><email>catherine.deste@anu.edu.au</email></address><xref ref-type="aff" rid="Aff6">6</xref></contrib><contrib contrib-type="author"><name><surname>Cadilhac</surname><given-names>Dominique A.</given-names></name><address><email>dominique.cadilhac@florey.edu.au</email></address><xref ref-type="aff" rid="Aff7">7</xref><xref ref-type="aff" rid="Aff8">8</xref></contrib><contrib contrib-type="author"><name><surname>Grimshaw</surname><given-names>Jeremy</given-names></name><address><email>jgrimshaw@ohri.ca</email></address><xref ref-type="aff" rid="Aff9">9</xref><xref ref-type="aff" rid="Aff10">10</xref></contrib><contrib contrib-type="author"><name><surname>Gerraty</surname><given-names>Richard</given-names></name><address><email>richard.gerraty@monash.edu</email></address><xref ref-type="aff" rid="Aff11">11</xref><xref ref-type="aff" rid="Aff12">12</xref></contrib><contrib contrib-type="author"><name><surname>Craig</surname><given-names>Louise</given-names></name><address><email>Louise.craig@acu.edu.au</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Schadewaldt</surname><given-names>Verena</given-names></name><address><email>Verena.schadewaldt@acu.edu.au</email></address><xref ref-type="aff" rid="Aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>McElduff</surname><given-names>Patrick</