<article article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Br J Cancer</journal-id><journal-id journal-id-type="iso-abbrev">Br. J. Cancer</journal-id><journal-title-group><journal-title>British Journal of Cancer</journal-title></journal-title-group><issn pub-type="ppub">0007-0920</issn><issn pub-type="epub">1532-1827</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">22767144</article-id><article-id pub-id-type="pmc">3405223</article-id><article-id pub-id-type="pii">bjc2012286</article-id><article-id pub-id-type="doi">10.1038/bjc.2012.286</article-id><article-categories><subj-group subj-group-type="heading"><subject>Clinical Study</subject></subj-group></article-categories><title-group><article-title>Docetaxel combined with irinotecan or 5-fluorouracil in patients with advanced oesophago-gastric cancer: a randomised phase II study</article-title><alt-title alt-title-type="running">Docetaxel plus Irinotecan/5FU in oesophago-gastric cancer</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Roy</surname><given-names>A</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Cunningham</surname><given-names>D</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><contrib contrib-type="author"><name><surname>Hawkins</surname><given-names>R</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib contrib-type="author"><name><surname>S&#x000f6;rbye</surname><given-names>H</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib contrib-type="author"><name><surname>Adenis</surname><given-names>A</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib contrib-type="author"><name><surname>Barcelo</surn