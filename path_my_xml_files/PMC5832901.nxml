<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Ment Health</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Ment Health</journal-id><journal-id journal-id-type="publisher-id">JMH</journal-id><journal-title-group><journal-title>JMIR Mental Health</journal-title></journal-title-group><issn pub-type="epub">2368-7959</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29449202</article-id><article-id pub-id-type="pmc">5832901</article-id><article-id pub-id-type="publisher-id">v5i1e13</article-id><article-id pub-id-type="doi">10.2196/mental.8894</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Paper</subject></subj-group><subj-group subj-group-type="article-type"><subject>Original Paper</subject></subj-group></article-categories><title-group><article-title>A Web-Based Psychoeducational Intervention for Adolescent Depression: Design and Development of MoodHwb</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Lennon</surname><given-names>Marilyn</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Buntrock</surname><given-names>Claudia</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Cox</surname><given-names>Georgina</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author" corresp="yes" equal-contrib="yes"><name><surname>Bevan Jones</surname><given-names>Rhys</given-names></name><degrees>PhD, MRCPsych</degrees><xref ref-type="aff" rid="af