<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="protocol"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29151054</article-id><article-id pub-id-type="pmc">5701991</article-id><article-id pub-id-type="publisher-id">bmjopen-2017-018560</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2017-018560</article-id><article-categories><subj-group subj-group-type="heading"><subject>Anaesthesia</subject><subj-group><subject>Protocol</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1682</subject></subj-group></article-categories><title-group><article-title>Effect of ephedrine and phenylephrine on brain oxygenation and microcirculation in anaesthetised patients with cerebral tumours: study protocol for a randomised controlled trial</article-title></title-group><contrib-group><contrib id="author-56508105" contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0001-8784-094X</contrib-id><name><surname>Koch</surname><given-names>Klaus Ulrik</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-56610094" contrib-type="author"><name><surname>Tietze</surname><given-names>Anna</given-names></name><xref ref-type="aff" rid="aff2">2</xref><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-56609972" contrib-type="author"><name><surname>Aanerud</surname><given-names>Joel</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-56610134" contrib-type="author"><name><surname>von &#x000d6;ettingen</surname><given-names>Gorm</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-56610171" contrib-type="author"><name><surname>Juul</surname><given-names>Niels</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-56610285" contrib-type="author"><name><surname>S&#x000f8;rensen</surname><given-names>Jens Christian Hedemann</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-56610327" contrib-type="author"><name><surname>Nikolajsen</surname><given-names>Lone</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-56610363" contrib-type="author"><name><surname>&#x000d8;stergaard</surname><given-names>Leif</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-56610399" contrib-type="author"><name><surname>Rasmussen</surname><given-names>Mads</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib></contrib-group><aff id="aff1">
