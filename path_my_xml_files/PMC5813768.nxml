-type="author"><name><surname>Tsuruga</surname><given-names>Koji</given-names></name><xref ref-type="aff" rid="af5-ndt-14-505">5</xref></contrib><contrib contrib-type="author"><name><surname>Sato</surname><given-names>Yasushi</given-names></name><xref ref-type="aff" rid="af1-ndt-14-505">1</xref></contrib><contrib contrib-type="author"><name><surname>Ishioka</surname><given-names>Masamichi</given-names></name><xref ref-type="aff" rid="af6-ndt-14-505">6</xref></contrib><contrib contrib-type="author"><name><surname>Nakamura</surname><given-names>Kazuhiko</given-names></name><xref ref-type="aff" rid="af1-ndt-14-505">1</xref></contrib><contrib contrib-type="author"><name><surname>Yasui-Furukori</surname><given-names>Norio</given-names></name><xref ref-type="aff" rid="af1-ndt-14-505">1</xref></contrib></contrib-group><aff id="af1-ndt-14-505">
