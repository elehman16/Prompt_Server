<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Pain Res</journal-id><journal-id journal-id-type="iso-abbrev">J Pain Res</journal-id><journal-id journal-id-type="publisher-id">Journal of Pain Research</journal-id><journal-title-group><journal-title>Journal of Pain Research</journal-title></journal-title-group><issn pub-type="epub">1178-7090</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29483783</article-id><article-id pub-id-type="pmc">5815470</article-id><article-id pub-id-type="doi">10.2147/JPR.S136299</article-id><article-id pub-id-type="publisher-id">jpr-11-335</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Acupuncture versus titrated morphine in acute renal colic: a randomized controlled trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Beltaief</surname><given-names>Kaouthar</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Grissa</surname><given-names>Mohamed Habib</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Msolli</surname><given-names>Mohamed Amine</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Bzeouich</surname><given-names>Nasri</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Fredj</surname><given-names>Nizar</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Sakma</surname><given-names>Adel</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Boubaker</surname><given-names>Hamdi</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Bouida</surname><given-names>Wahid</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref></contrib><contrib contrib-type="author"><name><surname>Boukef</surname><given-names>Riadh</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af3-jpr-11-335">3</xref></contrib><contrib contrib-type="author" corresp="yes"><name><surname>Nouira</surname><given-names>Semir</given-names></name><xref ref-type="aff" rid="af1-jpr-11-335">1</xref><xref ref-type="aff" rid="af2-jpr-11-335">2</xref><xref ref-type="corresp" rid="c1-jpr-11-335"/></contrib></contrib-group><aff id="af1-jpr-11-335">
