<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="protocol"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28550023</article-id><article-id pub-id-type="pmc">5729997</article-id><article-id pub-id-type="publisher-id">bmjopen-2016-015680</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2016-015680</article-id><article-categories><subj-group subj-group-type="heading"><subject>Reproductive Medicine</subject><subj-group><subject>Protocol</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1846</subject></subj-group></article-categories><title-group><article-title>The SUPER study: protocol for a randomised controlled trial comparing follicle-stimulating hormone and clomiphene citrate for ovarian stimulation in intrauterine insemination</article-title></title-group><contrib-group><contrib id="author-53130595" contrib-type="author"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0003-2448-5383</contrib-id><name><surname>Danhof</surname><given-names>NA</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-20409543" contrib-type="author"><name><surname>van Wely</surname><given-names>M</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-53130883" contrib-type="author"><name><surname>Koks</surname><given-names>CAM</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-53130905" contrib-type="author"><name><surname>Gianotten</surname><given-names>J</given-names></name><xref ref-type="aff" rid="aff3">3</xref></contrib><contrib id="author-53130927" contrib-type="author"><name><surname>de Bruin</surname><given-names>JP</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-53130939" contrib-type="author"><name><surname>Cohlen</surname><given-names>BJ</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-53130953" contrib-type="author"><name><surname>van der Ham</surname><given-names>DP</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib id="author-53130970" contrib-type="author"><name><surname>Klijn</surname><given-names>NF</given-names></name><xref ref-type="aff" rid="aff7">7</xref></contrib><contrib id="author-53130988" contrib-type="author"><name><surname>van Hooff</surname><given-names>MHA</given-names></name><xref ref-type="aff" rid="aff8">8</xref></contrib><contrib id="author-34486280" contrib-type="author"><name><surname>Broekmans</surname><given-names>FJM</given-names></name><xref ref-type="aff" rid="aff9">9</xref></contrib><contrib id="author-53131006" contrib-type="author"><name><surname>Fleischer</surname><given-names>K</given-names></name><xref ref-type="aff" rid="aff10">10</xref></contrib><contrib id="author-53131018" contrib-type="author"><name><surname>Janssen</surname><given-names>CAH</given-names></name><xref ref-type="aff" rid="aff11">11</xref></contrib><contrib id="author-53131026" contrib-type="author"><name><surname>Rijn van Weert</surname><given-names>JM</given-names></name><xref ref-type="aff" rid="aff12">12</xref></contrib><contrib id="author-53131038" contrib-type="author"><name><surname>van Disseldorp</surname><given-names>J</given-names></name><xref ref-type="aff" rid="aff13">13</xref></cont