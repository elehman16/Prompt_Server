<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Pain Res</journal-id><journal-id journal-id-type="iso-abbrev">J Pain Res</journal-id><journal-id journal-id-type="publisher-id">Journal of Pain Research</journal-id><journal-title-group><journal-title>Journal of Pain Research</journal-title></journal-title-group><issn pub-type="epub">1178-7090</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29750052</article-id><article-id pub-id-type="pmc">5933365</article-id><article-id pub-id-type="doi">10.2147/JPR.S155979</article-id><article-id pub-id-type="publisher-id">jpr-11-901</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Placebo response in neuropathic pain after spinal cord injury: a meta-analysis of individual participant data</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Jutzeler</surname><given-names>Catherine R</given-names></name><xref ref-type="aff" rid="af1-jpr-11-901">1</xref><xref ref-type="aff" rid="af2-jpr-11-901">2</xref><xref ref-type="aff" rid="af3-jpr-11-901">3</xref><xref ref-type="corresp" rid="c1-jpr-11-901"/></contrib><contrib contrib-type="author"><name><surname>Warner</surname><given-names>Freda M</given-names></name><xref ref-type="aff" rid="af1-jpr-11-901">1</xref><xref ref-type="aff" rid="af2-jpr-11-901">2</xref></contrib><contrib contrib-type="author"><name><surname>Cragg</surname><given-names>Jacquelyn J</given-names></name><xref ref-type="aff" rid="af1-jpr-11-901">1</xref><xref ref-type="aff" rid="af3-jpr-11-901">3</xref></contrib><contrib contrib-type="author"><name><surname>Haefeli</surname><given-names>Jenny</given-names></name><xref ref-type="aff" rid="af4-jpr-11-901">4</xref></contrib><contrib contrib-type="author"><name><surname>Richards</surname><given-names>J Scott</given-names></name><xref ref-type="aff" rid="af5-jpr-11-901">5</xref></contrib><contrib contrib-type="author"><name><surname>Andresen</surname><given-names>Sven R</given-names></name><xref ref-type="aff" rid="af6-jpr-11-901">6</xref></contrib><contrib contrib-type="author"><name><surname>Finnerup</surname><given-names>Nanna B</given-names></name><xref ref-type="aff" rid="af7-jpr-11-901">7</xref><xref ref-type="aff" rid="af8-jpr-11-901">8</xref></contrib><contrib contrib-type="author"><name><surname>Mercier</surname><given-names>Catherine</given-names></name><xref ref-type="aff" rid="af9-jpr-11-901">9</xref></contrib><contrib contrib-type="author"><name><surname>Kramer</surname><given-names>John LK</given-names></name><xref ref-type="aff" rid="af1-jpr-11-901">1</xref><xref ref-type="aff" rid="af2-jpr-11-901">2</xref></contrib></contrib-group><aff id="af1-jpr-11-901">
