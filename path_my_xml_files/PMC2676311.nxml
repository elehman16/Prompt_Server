<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Med</journal-id><journal-title>BMC Medicine</journal-title><issn pub-type="epub">1741-7015</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">19341446</article-id><article-id pub-id-type="pmc">2676311</article-id><article-id pub-id-type="publisher-id">1741-7015-7-13</article-id><article-id pub-id-type="doi">10.1186/1741-7015-7-13</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title>Intradermal influenza vaccination of healthy adults using a new microinjection system: a 3-year randomised controlled safety and immunogenicity trial</article-title></title-group><contrib-group><contrib id="A1" contrib-type="author"><name><surname>Beran</surname><given-names>Jiri</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>jiri.beran@vakcinace.cz</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Ambrozaitis</surname><given-names>Arvydas</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>infek@auste.elnet.lt</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Laiskonis</surname><given-names>Alvydas</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>laiskonis@med.kmu.lt</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Mickuviene</surname><given-names>Narseta</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>narseta@ktl.mii.lt</email></contrib><contrib id="A5" contrib-type="author"><name><surname>Bacart</surname><given-names>Patrick</given-names></name><xref ref-type="aff" rid="I5">5</xref><xref ref-type="aff" rid="I6">6</xref><email>bacart.doc@skynet.be</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Calozet</surname><given-names>Yvan</given-names></name><xref ref-type="aff" rid="I5">5</xref><xref ref-type="aff" rid="I7">7</xref><email>calozet.yvan@busmail.net</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Demanet</surname><given-names>Etienne</given-names></name><xref ref-type="aff" rid="I5">5</xref><xref ref-type="aff" rid="I8">8</xref><email>etienne.demanet@aresa.be</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Heijmans</surname><given-names>Stephane</given-names></name><xref ref-type="aff" rid="I5">5</xref><xref ref-type="aff" rid="I9">9</xref><email>stephane.Heijmans@researchlink.be</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Van Belle</surname><given-names>Paul</given-names></name><xref ref-type="aff" rid="I5">5</xref><xref ref-type="aff" rid="I10">10</xref><email>vanbellepaul@skynet.be</email></contrib><contrib id="A10" corresp="yes" contrib-type="author"><name><surname>Weber</surname><given-names>Fran&#x000e7;oise</given-names></name><xref ref-type="aff" rid="I11">11</xref><email>francoise.weber@sanofipasteur.com</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Salamand</surname><given-names>Camille</given-names></name><xref ref-type="aff" rid="I11">11</xref><email>camille.salamand@sanofipasteur.com</email></contrib></contrib-group><aff id="I1"><label>1</label>Vaccination and Travel Medicine Centre, Hradec Kralove, Czech Republic</aff><aff id="I2"><label>2</label>Department of Infectious Diseases, Dermatovenerology and Microbiology, Vilnius University, Vilnius, Lithuania</aff><aff id="I3"><label>3</label>Kaunas University of Medicine, Kaunas 2 Clinical Hospital, Kaunas, Lithuania</aff><aff id="I4"><label>4</label>Institute of Psychophysiology and Rehabilitation, Kaunas University of Medicine, Lithuania</aff><aff id="I5"><label>5</label>ResearchLink, a Clinical Trial Network of GPs, Thuin, Belgium</aff><aff id="I6"><label>6</label>General Practitioner, Private Practice, Molenbeek, Belgium</aff><aff id="I7"><label>7</label>General Practitioner, Private Practice, Gribomont, Belgium</aff><aff id="I8"><label>8</label>General Practitioner, Private Practice, Thuin, Belgium</aff><aff id="I9"><label>9</label>General Practitioner, Private Practice, Linkebeek, Belgium</aff><aff id="I10"><label>10</label>General Practitioner, Private Practice, Kraainem, Belgium</aff><aff id="I11"><label>11</label>Sanofi Pasteur, Lyon, France</aff><pub-date pub-type="collection"><year>2009</year></pub-date><pub-date pub-type="epub"><day>2</day><month>4</month><year>2009</year></pub-date><volume>7</volume><fpage>13</fpage><lpage>13</lpage><ext-link ext-link-type="uri" xlink:href="http://www.biomedcentral.com/1741-7015/7/13"/><history><date date-type="received"><day>12</day><month>3</month><year>2009</year></date><date date-type="accepted"><day>2</day><month>4</month><year>2009</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2009 Beran et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2009</copyright-year><copyright-holder>Beran et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
