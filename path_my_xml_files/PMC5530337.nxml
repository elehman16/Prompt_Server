rib><contrib contrib-type="author"><name><surname>Ilivanova</surname><given-names>Elena</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Xu</surname><given-names>Xie Lillian</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Miao</surname><given-names>Ye</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Zhuang</surname><given-names>Yanli</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Nnane</surname><given-names>Ivo</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Radziszewski</surname><given-names>Waldemar</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Greenspan</surname><given-names>Andrew</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Beutler</surname><given-names>Anna</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Baker</surname><given-names>Daniel</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib></contrib-group><aff id="af1">
