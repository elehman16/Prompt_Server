<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Clin Interv Aging</journal-id><journal-id journal-id-type="iso-abbrev">Clin Interv Aging</journal-id><journal-id journal-id-type="publisher-id">Clinical Interventions in Aging</journal-id><journal-title-group><journal-title>Clinical Interventions in Aging</journal-title></journal-title-group><issn pub-type="ppub">1176-9092</issn><issn pub-type="epub">1178-1998</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29674845</article-id><article-id pub-id-type="pmc">5898885</article-id><article-id pub-id-type="doi">10.2147/CIA.S157479</article-id><article-id pub-id-type="publisher-id">cia-13-541</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Blood pressure response to resistance training in hypertensive and normotensive older women</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Nascimento</surname><given-names>Dahan da Cunha</given-names></name><xref ref-type="aff" rid="af1-cia-13-541">1</xref><xref ref-type="aff" rid="af2-cia-13-541">2</xref><xref ref-type="corresp" rid="c1-cia-13-541"/></contrib><contrib contrib-type="author"><name><surname>da Silva</surname><given-names>Cristiane Rocha</given-names></name><xref ref-type="aff" rid="af1-cia-13-541">1</xref></contrib><contrib contrib-type="author"><name><surname>Valduga</surname><given-names>Renato</given-names></name><xref ref-type="aff" rid="af1-cia-13-541">1</xref><xref ref-type="aff" rid="af3-cia-13-541">3</xref></contrib><contrib contrib-type="author"><name><surname>Saraiva</surname><given-names>Bruno</given-names></name><xref ref-type="aff" rid="af1-cia-13-541">1</xref></contrib><contrib contrib-type="author"><name><surname>de Sousa Neto</surname><given-names>Ivo Vieira</given-names></name><xref ref-type="aff" rid="af4-cia-13-541">4</xref></contrib><contrib contrib-type="author"><name><surname>Vieira</surname><given-names>Amilton</given-names></name><xref ref-type="aff" rid="af4-cia-13-541">4</xref></contrib><contrib contrib-type="author"><name><surname>Funghetto</surname><given-names>Silvana Schwerz</given-names></name><xref ref-type="aff" rid="af4-cia-13-541">4</xref></contrib><contrib contrib-type="author"><name><surname>Silva</surname><given-names>Alessandro Oliveira</given-names></name><xref ref-type="aff" rid="af5-cia-13-541">5</xref></contrib><contrib contrib-type="author"><name><surname>Oliveira</surname><given-names>Samuel da Cunha</given-names></name><xref ref-type="aff" rid="af1-cia-13-541">1</xref></contrib><contrib contrib-type="author"><name><surname>Pereira</surname><given-names>Guilherme Borges</given-names></name><xref ref-type="aff" rid="af1-cia-13-541">1</xref></contrib><contrib contrib-type="author"><name><surname>Willardson</surname><given-names>Jeffrey M</given-names></name><xref ref-type="aff" rid="af6-cia-13-541">6</xref></contrib><contrib contrib-type="author"><name><surname>Prestes</surname><given-names>Jonato</given-names></name><xref ref-type="aff" rid="af1-cia-13-541">1</xref></contrib></contrib-group><aff id="af1-cia-13-541">
