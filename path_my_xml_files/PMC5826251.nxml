<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Local Reg Anesth</journal-id><journal-id journal-id-type="iso-abbrev">Local Reg Anesth</journal-id><journal-id journal-id-type="publisher-id">Local and Regional Anesthesia</journal-id><journal-title-group><journal-title>Local and Regional Anesthesia</journal-title></journal-title-group><issn pub-type="epub">1178-7112</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29503582</article-id><article-id pub-id-type="pmc">5826251</article-id><article-id pub-id-type="doi">10.2147/LRA.S147288</article-id><article-id pub-id-type="publisher-id">lra-11-009</article-id><article-categories><subj-group subj-group-type="heading"><subject>Clinical Trial Report</subject></subj-group></article-categories><title-group><article-title>Effect of warming anesthetic on pain perception during dental injection: a split-mouth randomized clinical trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Aravena</surname><given-names>Pedro Christian</given-names></name><xref ref-type="aff" rid="af1-lra-11-009">1</xref><xref ref-type="aff" rid="af2-lra-11-009">2</xref><xref ref-type="corresp" rid="c1-lra-11-009"/></contrib><contrib contrib-type="author"><name><surname>Barrientos</surname><given-names>Camila</given-names></name><xref ref-type="aff" rid="af1-lra-11-009">1</xref></contrib><contrib contrib-type="author"><name><surname>Troncoso</surname><given-names>Catalina</given-names></name><xref ref-type="aff" rid="af1-lra-11-009">1</xref></contrib><contrib contrib-type="author"><name><surname>Coronado</surname><given-names>Cesar</given-names></name><xref ref-type="aff" rid="af3-lra-11-009">3</xref></contrib><contrib contrib-type="author"><name><surname>Sotelo-Hitschfeld</surname><given-names>Pamela</given-names></name><xref ref-type="aff" rid="af4-lra-11-009">4</xref></contrib></contrib-group><aff id="af1-lra-11-009">
