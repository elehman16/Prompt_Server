contrib><contrib contrib-type="author"><name><surname>Li</surname><given-names>Peijun</given-names></name><xref ref-type="aff" rid="af1-copd-13-1713">1</xref></contrib><contrib contrib-type="author"><name><surname>Wang</surname><given-names>Zhenwei</given-names></name><xref ref-type="aff" rid="af3-copd-13-1713">3</xref></contrib></contrib-group><aff id="af1-copd-13-1713">
