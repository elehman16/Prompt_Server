f></contrib><contrib contrib-type="author"><name><surname>Liporoni</surname><given-names>Priscila Christiane Suzy</given-names></name><xref ref-type="aff" rid="af3-ccide-9-039">3</xref></contrib><contrib contrib-type="author"><name><surname>Catelan</surname><given-names>Anderson</given-names></name><xref ref-type="aff" rid="af3-ccide-9-039">3</xref></contrib></contrib-group><aff id="af1-ccide-9-039">
