<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMC Gastroenterol</journal-id><journal-title-group><journal-title>BMC Gastroenterology</journal-title></journal-title-group><issn pub-type="epub">1471-230X</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">20170514</article-id><article-id pub-id-type="pmc">2837223</article-id><article-id pub-id-type="publisher-id">1471-230X-10-21</article-id><article-id pub-id-type="doi">10.1186/1471-230X-10-21</article-id><article-categories><subj-group subj-group-type="heading"><subject>Research Article</subject></subj-group></article-categories><title-group><article-title>Short versus standard treatment with pegylated interferon alfa-2A plus ribavirin in patients with hepatitis C virus genotype 2 or 3: the cleo trial</article-title></title-group><contrib-group><contrib contrib-type="author" equal-contrib="yes" id="A1"><name><surname>Mecenate</surname><given-names>Fabrizio</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>famecenate@yahoo.it</email></contrib><contrib contrib-type="author" corresp="yes" equal-contrib="yes" id="A2"><name><surname>Pellicelli</surname><given-names>Adriano M</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>adriano.pellicelli@tiscali.it</email></contrib><contrib contrib-type="author" equal-contrib="yes" id="A3"><name><surname>Barbaro</surname><given-names>Giuseppe</given-names></name><xref ref-type="aff" rid="I4">4</xref><email>g.barbaro@tin.it</email></contrib><contrib contrib-type="author" equal-contrib="yes" id="A4"><name><surname>Romano</surname><given-names>Mario</given-names></name><xref ref-type="aff" rid="I3">3</xref><email>mario.romano@aslromab.it</email></contrib><contrib contrib-type="author" equal-contrib="yes" id="