-type="author"><name><surname>Strippoli</surname><given-names>Paolo</given-names></name><xref ref-type="aff" rid="aff012"><sup>12</sup></xref></contrib><contrib contrib-type="author"><name><surname>Salomone</surname><given-names>Mario</given-names></name><xref ref-type="aff" rid="aff013"><sup>13</sup></xref></contrib><contrib contrib-type="author"><name><surname>Santoro</surname><given-names>Antonio</given-names></name><xref ref-type="aff" rid="aff014"><sup>14</sup></xref></contrib><contrib contrib-type="author"><name><surname>Maffei</surname><given-names>Stefano</given-names></name><xref ref-type="aff" rid="aff013"><sup>13</sup></xref></contrib><contrib contrib-type="author"><name><surname>Hegbrant</surname><given-names>J&#x000f6;rgen</given-names></name><xref ref-type="aff" rid="aff002"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>Tognoni</surname><given-names>Gianni</given-names></name><xref ref-type="aff" rid="aff015"><sup>15</sup></xref></contrib><contrib contrib-type="author"><contrib-id authenticated="true" contrib-id-type="orcid">http://orcid.org/0000-0002-6936-0616</contrib-id><name><surname>Strippoli</surname><given-names>Giovanni F. M.</given-names></name><xref ref-type="aff" rid="aff001"><sup>1</sup></xref><xref ref-type="aff" rid="aff002"><sup>2</sup></xref><xref ref-type="aff" rid="aff016"><sup>16</sup></xref><xref ref-type="corresp" rid="cor001">*</xref></contrib><contrib contrib-type="author"><collab>for the CE-DOSE Study Investigators</collab><xref ref-type="author-notes" rid="fn001"><sup>&#x000b6;</sup></xref></contrib></contrib-group><aff id="aff001"><label>1</label><addr-line>Sydney School of Public Health, Edward Ford Building, The University of Sydney, New South Wales, Australia</addr-line></aff><aff id="aff002"><label>2</label><addr-line>Diaverum Renal Services Group, Lund, Sweden</addr-line></aff><aff id="aff003"><label>3</label><addr-line>Department of Medicine, University of Otago Christchurch, Christchurch, Ne