 id="cas13232-aff-0007"><label><sup>7</sup></label><institution>Pfizer Japan</institution><named-content content-type="city">Tokyo</named-content><country country="JP">Japan</country></aff><aff id="cas13232-aff-0008"><label><sup>8</sup></label><institution>Pfizer Oncology</institution><named-content content-type="city">San Diego</named-content><named-content content-type="country-part">California</named-content><country country="US">USA</country></aff><aff id="cas13232-aff-0009"><label><sup>9</sup></label><named-content content-type="organisation-division">Department of Urology</named-content><institution>Kindai University Faculty of Medicine</institution><named-content content-type="city">Osaka</named-content><country country="JP">Japan</country></aff><aff id="cas13232-curr-0001"><label><sup>10</sup></label>Present address: <institution>Niigata University Graduate School of Medical and Dental Sciences</institution><named-content content-type="city">Niigata</named-content><country country="JP">Japan</country></aff><author-notes><corresp id="correspondenceTo"><label>*</label><bold>Correspondence</bold><break/>
