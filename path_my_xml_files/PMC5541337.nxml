<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">BMJ Open</journal-id><journal-id journal-id-type="iso-abbrev">BMJ Open</journal-id><journal-id journal-id-type="hwp">bmjopen</journal-id><journal-id journal-id-type="publisher-id">bmjopen</journal-id><journal-title-group><journal-title>BMJ Open</journal-title></journal-title-group><issn pub-type="epub">2044-6055</issn><publisher><publisher-name>BMJ Open</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">28515193</article-id><article-id pub-id-type="pmc">5541337</article-id><article-id pub-id-type="publisher-id">bmjopen-2016-014616</article-id><article-id pub-id-type="doi">10.1136/bmjopen-2016-014616</article-id><article-categories><subj-group subj-group-type="heading"><subject>Health Economics</subject><subj-group><subject>Research</subject></subj-group></subj-group><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject><subject>1701</subject></subj-group></article-categories><title-group><article-title>Cost-effectiveness of telehealthcare to patients with chronic obstructive pulmonary disease: results from the Danish &#x02018;TeleCare North&#x02019; cluster-randomised trial</article-title></title-group><contrib-group><contrib id="author-49290838" contrib-type="author"><name><surname>Witt Udsen</surname><given-names>Flemming</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-48780177" contrib-type="author"><name><surname>Lilholt</surname><given-names>Pernille Heyckendorff</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-34225778" contrib-type="author"><name><surname>Hejlesen</surname><given-names>Ole</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-20411869" contrib-type="author"><name><surname>Ehlers</surname><given-names>Lars</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib></contrib-group><aff id="aff1">
