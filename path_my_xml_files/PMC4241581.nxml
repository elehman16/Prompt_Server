<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Muscle Nerve</journal-id><journal-id journal-id-type="iso-abbrev">Muscle Nerve</journal-id><journal-id journal-id-type="publisher-id">mus</journal-id><journal-title-group><journal-title>Muscle &#x00026; Nerve</journal-title></journal-title-group><issn pub-type="ppub">0148-639X</issn><issn pub-type="epub">1097-4598</issn><publisher><publisher-name>BlackWell Publishing Ltd</publisher-name><publisher-loc>Oxford, UK</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">25042182</article-id><article-id pub-id-type="pmc">4241581</article-id><article-id pub-id-type="doi">10.1002/mus.24332</article-id><article-categories><subj-group subj-group-type="heading"><subject>Main Articles</subject></subj-group></article-categories><title-group><article-title>Ataluren treatment of patients with nonsense mutation dystrophinopathy</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Bushby</surname><given-names>Katharine</given-names><suffix>MD</suffix></name><xref ref-type="aff" rid="au1">1</xref><xref ref-type="author-notes" rid="fn1">*</xref></contrib><contrib contrib-type="author"><name><surname>Finkel</surname><given-names>Richard</given-names><suffix>MD</suffix></name><xref ref-type="aff" rid="au2">2</xref><xref ref-type="author-notes" rid="fn1">*</xref></contrib><contrib contrib-type="author"><name><surname>Wong</surname><given-names>Brenda</given-names><suffix>MD</suffix></name><xref ref-type="aff" rid="au3">3</xref></contrib><contrib contrib-type="author"><name><surname>Barohn</surname><given-names>Richard</given-names><suffix>MD</suffix></name><xref ref-type="aff" rid="au4">4</xref></contrib><contrib contrib-type="author"><name><surname>Campbell</surname><given-names>Craig</given-names><suffix>M