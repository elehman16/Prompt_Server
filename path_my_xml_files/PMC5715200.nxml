<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">JMIR Res Protoc</journal-id><journal-id journal-id-type="iso-abbrev">JMIR Res Protoc</journal-id><journal-id journal-id-type="publisher-id">ResProt</journal-id><journal-title-group><journal-title>JMIR Research Protocols</journal-title></journal-title-group><issn pub-type="epub">1929-0748</issn><publisher><publisher-name>JMIR Publications</publisher-name><publisher-loc>Toronto, Canada</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29158206</article-id><article-id pub-id-type="pmc">5715200</article-id><article-id pub-id-type="publisher-id">v6i11e228</article-id><article-id pub-id-type="doi">10.2196/resprot.8178</article-id><article-categories><subj-group subj-group-type="heading"><subject>Protocol</subject></subj-group><subj-group subj-group-type="article-type"><subject>Protocol</subject></subj-group></article-categories><title-group><article-title>Pulmonary Rehabilitation With Balance Training for Fall Reduction in Chronic Obstructive Pulmonary Disease: Protocol for a Randomized Controlled Trial</article-title></title-group><contrib-group><contrib contrib-type="editor"><name><surname>Eysenbach</surname><given-names>Gunther</given-names></name></contrib></contrib-group><contrib-group><contrib contrib-type="reviewer"><name><surname>Boyd</surname><given-names>Andrew</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Smeets</surname><given-names>Christophe</given-names></name></contrib><contrib contrib-type="reviewer"><name><surname>Alwashmi</surname><given-names>Meshari</given-names></name></contrib></contrib-group><contrib-group><contrib id="contrib1" contrib-type="author"><name><surname>Beauchamp</surname><given-names>Marla K</given-names></name><degrees>MScPT, PhD</degrees><xref ref-type="aff" rid="aff01">1</xref><xref ref-type="aff" rid="aff02">2</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2843-388X</contrib-id></contrib><contrib id="contrib2" contrib-type="author" corresp="yes"><name><surname>Brooks</surname><given-names>Dina</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff02">2</xref><xref ref-type="aff" rid="aff03">3</xref><address><institution>Department of Physical Therapy</institution><institution>University of Toronto</institution><addr-line>#160-500 University Avenue</addr-line><addr-line>Toronto, ON, M5G 1V7</addr-line><country>Canada</country><phone>1 4169781739</phone><email>dina.brooks@utoronto.ca</email></address><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0001-6080-5052</contrib-id></contrib><contrib id="contrib3" contrib-type="author"><name><surname>Ellerton</surname><given-names>Cindy</given-names></name><degrees>PT, MSc</degrees><xref ref-type="aff" rid="aff02">2</xref><xref ref-type="aff" rid="aff03">3</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-4176-1151</contrib-id></contrib><contrib id="contrib4" contrib-type="author"><name><surname>Lee</surname><given-names>Annemarie</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff04">4</xref><xref ref-type="aff" rid="aff05">5</xref><xref ref-type="aff" rid="aff06">6</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8631-0135</contrib-id></contrib><contrib id="contrib5" contrib-type="author"><name><surname>Alison</surname><given-names>Jennifer</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff07">7</xref><xref ref-type="aff" rid="aff08">8</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2011-4756</contrib-id></contrib><contrib id="contrib6" contrib-type="author"><name><surname>Camp</surname><given-names>Pat G</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff09">9</xref><xref ref-type="aff" rid="aff10">10</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-9152-8251</contrib-id></contrib><contrib id="contrib7" contrib-type="author"><name><surname>Dechman</surname><given-names>Gail</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff11">11</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-6275-687X</contrib-id></contrib><contrib id="contrib8" contrib-type="author"><name><surname>Haines</surname><given-names>Kimberley</given-names></name><degrees>PT</degrees><xref ref-type="aff" rid="aff12">12</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-2882-1594</contrib-id></contrib><contrib id="contrib9" contrib-type="author"><name><surname>Harrison</surname><given-names>Samantha L</given-names></name><degrees>MScPT, PhD</degrees><xref ref-type="aff" rid="aff13">13</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-8871-781X</contrib-id></contrib><contrib id="contrib10" contrib-type="author"><name><surname>Holland</surname><given-names>Anne E</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff04">4</xref><xref ref-type="aff" rid="aff05">5</xref><xref ref-type="aff" rid="aff14">14</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-2061-845X</contrib-id></contrib><contrib id="contrib11" contrib-type="author"><name><surname>Marques</surname><given-names>Alda</given-names></name><degrees>PT, PhD</degrees><xref ref-type="aff" rid="aff15">15</xref><xref ref-type="aff" rid="aff16">16</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0003-4980-6200</contrib-id></contrib><contrib id="contrib12" contrib-type="author"><name><surname>Moineddin</surname><given-names>Rahim</given-names></name><degrees>PhD</degrees><xref ref-type="aff" rid="aff17">17</xref><contrib-id contrib-id-type="orcid">http://orcid.org/0000-0002-5506-084X</contrib-id></contrib><contrib id="contrib13" contrib-type="author"><name><surname>Skinner</surname><g